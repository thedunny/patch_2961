--| Procedimento recupera os dados da folha de salários - Bloco M350
---------------------------------------------------------------------
PROCEDURE PKB_DADOS_FOL_PIS_M350( EN_INFPISFOLHA_ID IN INF_PIS_FOLHA.ID%TYPE ) IS
   --
   vn_fase         number := 0;
   vv_nome_empresa varchar2(1000) := null;
   ve_invalido     exception;
   --
   cursor c_fol_pis is
      select *
        from inf_pis_folha ip
       where ip.id = en_infpisfolha_id;
   --
BEGIN
   --
   vn_fase := 1;
   gt_row_inf_pis_folha := null;
   --
   vn_fase := 2;
   open c_fol_pis;
   fetch c_fol_pis into gt_row_inf_pis_folha;
   close c_fol_pis;
   --
   vn_fase := 3;
   --| Cabeçalho para as mensagens de log de erro e inconsistências
   gv_mensagem_log := 'Folha de Salários';
   --
   vn_fase := 4;
   --
   if nvl(gt_row_inf_pis_folha.id,0) > 0 then
      --
      vn_fase := 5;
      --
      vv_nome_empresa := pk_csf.fkg_nome_empresa( en_empresa_id => gt_row_inf_pis_folha.empresa_id );
      --
      vn_fase := 6;
      --| Cabeçalho para as mensagens de log de erro e inconsistências
      gv_mensagem_log := gv_mensagem_log||', no período de '||to_char(gt_row_inf_pis_folha.dt_ini,'dd/mm/rrrr')||' até '||
                         to_char(gt_row_inf_pis_folha.dt_fin,'dd/mm/rrrr')||' - Empresa: '||vv_nome_empresa;
      --
   else
      --
      vn_fase := 7;
      --
      raise ve_invalido;
   end if;
   --
EXCEPTION
   when ve_invalido then
      --
      gv_resumo_log := 'Folha de salário não encontrada. Verifique o identificador informado (id = '||en_infpisfolha_id||').';
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
