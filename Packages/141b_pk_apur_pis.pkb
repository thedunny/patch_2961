--| Procedimento para gravar os valores nas tabelas de banco - Bloco M400
--------------------------------------------------------------------------
PROCEDURE PKB_GRAVA_DADOS_M400 IS
   --
   vn_fase    number := 0;
   vn_ind     number := 0;
   vn_ind_bi  number := 0;
   vn_ind_tri number := 0;
   --
BEGIN
   --
   vn_fase := 1;
   --| Gravar os dados de receita isenta - rec_isenta_pis
   vn_ind := nvl(vt_tab_reg_m400.first,0);
   --
   vn_fase := 2;
   --
   loop
      --
      vn_fase := 3;
      --
      if nvl(vn_ind,0) = 0 then
         exit;
      end if;
      --
      vn_fase := 4;
      --
      begin
         insert into rec_isenta_pis( id
                                   , dm_situacao
                                   , codst_id
                                   , vl_tot_rec
                                   , planoconta_id
                                   , desc_compl
                                   , perrecisentapis_id )
                             values( vt_tab_reg_m400(vn_ind).recisentapis_id
                                   , vt_tab_reg_m400(vn_ind).dm_situacao
                                   , vt_tab_reg_m400(vn_ind).codst_id
                                   , vt_tab_reg_m400(vn_ind).vl_tot_rec
                                   , vt_tab_reg_m400(vn_ind).planoconta_id
                                   , vt_tab_reg_m400(vn_ind).desc_compl
                                   , gt_row_per_rec_isenta_pis.id );
      exception
         when dup_val_on_index then
            --
            vn_fase := 5;
            --
            begin
               update rec_isenta_pis ri
                  set ri.dm_situacao        = vt_tab_reg_m400(vn_ind).dm_situacao
                    , ri.codst_id           = vt_tab_reg_m400(vn_ind).codst_id
                    , ri.vl_tot_rec         = vt_tab_reg_m400(vn_ind).vl_tot_rec
                    , ri.planoconta_id      = vt_tab_reg_m400(vn_ind).planoconta_id
                    , ri.desc_compl         = vt_tab_reg_m400(vn_ind).desc_compl
                    , ri.perrecisentapis_id = gt_row_per_rec_isenta_pis.id
                where ri.id = vt_tab_reg_m400(vn_ind).recisentapis_id;
            exception
               when others then
                  --
                  gv_resumo_log := 'Problemas ao alterar consolidação (id='||vt_tab_reg_m400(vn_ind).recisentapis_id||' fase = '||vn_fase||'). Erro = '||sqlerrm;
                  --
                  declare
                     vn_loggenerico_id  log_generico.id%type;
                  begin
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                      , ev_mensagem        => gv_mensagem_log
                                                      , ev_resumo          => gv_resumo_log
                                                      , en_tipo_log        => erro_de_sistema
                                                      , en_referencia_id   => gn_referencia_id
                                                      , ev_obj_referencia  => gv_obj_referencia );
                  exception
                     when others then
                        null;
                  end;
                  --
                  raise_application_error (-20101, gv_resumo_log);
                  --
            end;
         when others then
            --
            gv_resumo_log := 'Problemas ao incluir receita isenta (id='||vt_tab_reg_m400(vn_ind).recisentapis_id||' fase = '||vn_fase||'). Erro = '||sqlerrm;
            --
            declare
               vn_loggenerico_id  log_generico.id%type;
            begin
               pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                , ev_mensagem        => gv_mensagem_log
                                                , ev_resumo          => gv_resumo_log
                                                , en_tipo_log        => erro_de_sistema
                                                , en_referencia_id   => gn_referencia_id
                                                , ev_obj_referencia  => gv_obj_referencia );
            exception
               when others then
                  null;
            end;
            --
            raise_application_error (-20101, gv_resumo_log);
            --
      end;
      --
      vn_fase := 6;
      --
      if vn_ind = vt_tab_reg_m400.last then
         exit;
      else
         vn_ind := vt_tab_reg_m400.next(vn_ind);
      end if;
      --
   end loop;
   --
   vn_fase := 7;
   --| Gravar os dados de detalhe da receita isenta - det_rec_isenta_pis
   vn_ind := nvl(vt_tri_tab_reg_m410.first,0);
   --
   vn_fase := 8;
   --
   loop
      --
      vn_fase := 9;
      --
      if nvl(vn_ind,0) = 0 then
         exit;
      end if;
      --
      vn_fase := 10;
      --
      vn_ind_bi := nvl(vt_tri_tab_reg_m410(vn_ind).first,0);
      --
      vn_fase := 11;
      --
      loop
         --
         vn_fase := 12;
         --
         if nvl(vn_ind_bi,0) = 0 then
            exit;
         end if;
         --
         vn_fase := 13;
         --
         vn_ind_tri := nvl(vt_tri_tab_reg_m410(vn_ind)(vn_ind_bi).first,0);
         --
         vn_fase := 14;
         --
         loop
            --
            vn_fase := 15;
            --
            if nvl(vn_ind_tri,0) = 0 then
               exit;
            end if;
            --
            vn_fase := 16;
            --
            begin
               insert into det_rec_isenta_pis( id
                                             , recisentapis_id
                                             , natrecpc_id
                                             , vl_rec
                                             , planoconta_id
                                             , desc_compl )
                                       values( vt_tri_tab_reg_m410(vn_ind)(vn_ind_bi)(vn_ind_tri).detrecisentapis_id
                                             , vt_tri_tab_reg_m410(vn_ind)(vn_ind_bi)(vn_ind_tri).recisentapis_id
                                             , vt_tri_tab_reg_m410(vn_ind)(vn_ind_bi)(vn_ind_tri).natrecpc_id
                                             , vt_tri_tab_reg_m410(vn_ind)(vn_ind_bi)(vn_ind_tri).vl_rec
                                             , vt_tri_tab_reg_m410(vn_ind)(vn_ind_bi)(vn_ind_tri).planoconta_id
                                             , vt_tri_tab_reg_m410(vn_ind)(vn_ind_bi)(vn_ind_tri).desc_compl );
            exception
               when others then
                  --
                  gv_resumo_log := 'Problemas ao incluir detalhes da receita isenta (id='||vt_tri_tab_reg_m410(vn_ind)(vn_ind_bi)(vn_ind_tri).detrecisentapis_id||
                                   ' fase = '||vn_fase||'). Erro = '||sqlerrm;
                  --
                  declare
                     vn_loggenerico_id  log_generico.id%type;
                  begin
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                      , ev_mensagem        => gv_mensagem_log
                                                      , ev_resumo          => gv_resumo_log
                                                      , en_tipo_log        => erro_de_sistema
                                                      , en_referencia_id   => gn_referencia_id
                                                      , ev_obj_referencia  => gv_obj_referencia );
                  exception
                     when others then
                        null;
                  end;
                  --
                  raise_application_error (-20101, gv_resumo_log);
                  --
            end;
            --
            vn_fase := 17;
            --
            if vn_ind_tri = vt_tri_tab_reg_m410(vn_ind)(vn_ind_bi).last then
               exit;
            else
               vn_ind_tri := vt_tri_tab_reg_m410(vn_ind)(vn_ind_bi).next(vn_ind_tri);
            end if;
            --
         end loop;
         --
         vn_fase := 18;
         --
         if vn_ind_bi = vt_tri_tab_reg_m410(vn_ind).last then
            exit;
         else
            vn_ind_bi := vt_tri_tab_reg_m410(vn_ind).next(vn_ind_bi);
         end if;
         --
      end loop;
      --
      vn_fase := 19;
      --
      if vn_ind = vt_tri_tab_reg_m410.last then
         exit;
      else
         vn_ind := vt_tri_tab_reg_m410.next(vn_ind);
      end if;
      --
   end loop;
   --
   vn_fase := 20;
   --
   commit;
   --
EXCEPTION
   when others then
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.pkb_grava_dados_m400 (fase = '||vn_fase||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => gn_referencia_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
END PKB_GRAVA_DADOS_M400;
