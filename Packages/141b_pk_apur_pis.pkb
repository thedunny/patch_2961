   gv_obj_referencia := 'INF_PIS_FOLHA';
   gv_mensagem_log   := 'Desprocesso da folha de salários - PIS/PASEP';
   gv_resumo_log     := 'Início do processo (identificador da folha = '||en_infpisfolha_id||').';
