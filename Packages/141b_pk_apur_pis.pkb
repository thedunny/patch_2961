                                    , ev_obj_referencia => gv_obj_referencia );
   --
   gv_resumo_log := null;
   --
   vn_fase := 3;
   --
   if nvl(en_perrecisentapis_id,0) = 0 then
      --
      vn_fase := 4;
      gv_resumo_log := 'O identificador do período de receita isenta deve ser informado para que a validação seja efetuada.';
      pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                       , ev_mensagem        => gv_mensagem_log
                                       , ev_resumo          => gv_resumo_log
                                       , en_tipo_log        => erro_de_validacao
                                       , en_referencia_id   => null
                                       , ev_obj_referencia  => gv_obj_referencia );
      --
   else
      --
      vn_fase := 5;
      --
      if pk_csf_efd_pc.fkg_qtde_per_rec_pis( en_perrecisentapis_id => en_perrecisentapis_id ) = 0 then -- devem existir receitas geradas
         --
         vn_fase := 6;
         gv_resumo_log := 'O período de receita informado não possui receitas geradas, portanto, a validação não será realizada.';
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_validacao
                                          , en_referencia_id   => en_perrecisentapis_id
                                          , ev_obj_referencia  => gv_obj_referencia );
         --
      else
         --
         vn_fase := 7;
         --
         gt_row_per_rec_isenta_pis := null;
         open c_per_rec_isenta_pis( en_perrecisentapis_id => en_perrecisentapis_id );
         fetch c_per_rec_isenta_pis into gt_row_per_rec_isenta_pis;
         close c_per_rec_isenta_pis;
