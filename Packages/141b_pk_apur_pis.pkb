                       pkb_monta_vetor_m100( en_ch_cc_tpcr_or_tp_aliq => vn_tipocredpc_id||r_reg.dm_ind_cred_ori||r_reg.tipo||
                                                                         replace(replace(nvl(r_reg.aliq_pis,0),',',''),'.','')||
                                                                         0/*replace(replace(nvl(r_reg.vl_aliq_pis_quant,0),',',''),'.','')*/
                                           , en_tipocredpc_id         => vn_tipocredpc_id
                                           , en_dm_ind_cred_ori       => r_reg.dm_ind_cred_ori
                                           , en_tipo                  => r_reg.tipo
                                           , en_aliq_pis              => nvl(r_reg.aliq_pis,0)
                                           , en_vl_aliq_pis_quant     => 0 --nvl(r_reg.vl_aliq_pis_quant,0)
                                           , en_vl_bc_pis             => 0 --nvl(vn_vl_bc_pis,0)
                                           , en_quant_bc_pis          => 0 --nvl(r_reg.quant_bc_pis,0)
                                           , en_basecalccredpc_id     => r_reg.basecalccredpc_id
                                           , en_codst_id              => r_reg.codst_id
                                           , en_vl_bc_pis_tot         => nvl(r_reg.vl_bc_pis_tot,0)
                                           , en_vl_bc_pis_cum         => 0 --nvl(r_reg.vl_bc_pis_cum,0)
                                           , en_vl_bc_pis_nc          => nvl(r_reg.vl_bc_pis_nc,0)
                                           , en_quant_bc_pis_tot      => 0 ); --nvl(r_reg.quant_bc_pis_tot,0) );
                       --                                         
                     else
                       --
                       vn_fase := 13.18;                       
