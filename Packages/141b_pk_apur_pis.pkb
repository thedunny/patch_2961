      --
      if vv_proc_ded = 'S' then -- executou o processo de deduções dos impostos retidos
         vn_vl_ret_nc      := nvl(vn_tot_vl_ret_nc,0);
         vn_vl_out_ded_nc  := nvl(vn_tot_vl_out_ded_nc,0);
         vn_vl_cont_nc_rec := nvl(vn_vl_tot_cont_nc_dev_rat,0);
      else -- não executou o processo de deduções dos impostos retidos
         vn_vl_ret_nc      := 0;
         vn_vl_out_ded_nc  := nvl(vn_vl_out_ded_nc,0); -- valor recuperado do Bloco F700
         vn_vl_cont_nc_rec := nvl(vn_vl_tot_cont_nc_dev_rat,0) - nvl(vn_vl_ret_nc,0) - nvl(vn_vl_out_ded_nc,0);
      end if;
      --
   end if;
   --
   vn_fase := 24;
   --
   if gn_dm_cod_inc_trib = 1 then -- 1-Exclusivamente no regime não-cumulativo
      vn_vl_tot_cont_cum_per := 0;
   else
      open c_vl_tot_cont_cum_per; -- soma detalhe da consolidação - bloco m200 -> det_cons_contr_pis.vl_cont_per
      fetch c_vl_tot_cont_cum_per into vn_vl_tot_cont_cum_per;
      close c_vl_tot_cont_cum_per;
   end if;
   --
   vn_fase := 25;
   --
   ------------------------------------------------------------
   -- Compôr os valores Cumulativos para Geração do Bloco 1300
   ------------------------------------------------------------
   --
   vn_vl_ret_cum             := 0;
   vn_tot_vl_ret_cum         := 0;
   vn_vl_out_ded_cum         := 0;
   vn_tot_vl_out_ded_cum     := 0;
   vn_vl_cont_cum_rec        := 0;
   vn_vl_ret_apu_bl_1300_cum := 0;
   vn_vl_ret_ded_bl_1300_cum := 0;
   vv_proc_ded               := 'N';
   --
   if gn_dm_cod_inc_trib = 1 or -- 1-Exclusivamente no regime não-cumulativo
      nvl(vn_vl_tot_cont_cum_per,0) = 0 then
      --
      vn_fase := 26;
      --
   else
      --
      vn_fase := 27;
      --
      vn_contrvlrretfontepis_id := 0;
      open c_contr;
      fetch c_contr into vn_contrvlrretfontepis_id; -- verifica se já existe lançto de controle de valor de natureza cumulativa
      close c_contr;
      --
      vn_fase := 28;
      -- soma deduções diversas - bloco f700 -> deducao_diversa_pc.vl_ded_pis
      open c_vl_ded_pis( en_ind_nat_ded => 1 ); -- 0-dedução de natureza não cumulativa, 1-dedução de natureza cumulativa
      fetch c_vl_ded_pis into vn_vl_out_ded_cum, vn_vl_bc_oper;
      close c_vl_ded_pis;
      --
      vn_fase := 29;
      vn_vl_tot_cont_cum_per_rat := nvl(vn_vl_tot_cont_cum_per,0);
      --
      vn_fase := 30;
      --
      for r_vl_ret_apu in c_vl_ret_apu( en_dm_ind_nat_rec => 1 ) -- 0-Receita de Natureza Não-Cumulativa, 1-Receita de Natureza Cumulativa
      loop
         --
         exit when c_vl_ret_apu%notfound or (c_vl_ret_apu%notfound) is null;
         --
         vn_fase := 31;
         --
         vv_proc_ded               := 'S';
         vn_vl_ret_cum             := nvl(r_vl_ret_apu.vl_ret_apu,0);
         vn_vl_ret_apu_bl_1300_cum := nvl(r_vl_ret_apu.vl_ret_apu,0);
         --
         vn_fase := 32;
         --
         if nvl(vn_vl_tot_cont_cum_per_rat,0) = 0 then -- Valor Total da Contribuição Cumulativa no período
            --
            vn_fase := 32.1;
            --
            vn_vl_ret_cum     := 0;
            vn_vl_out_ded_cum := 0;
            --
         else
            --
            vn_fase := 32.2;
            --
            if nvl(vn_vl_ret_cum,0) > nvl(vn_vl_tot_cont_cum_per_rat,0) then -- valor de retenção maior que o total
               --
               vn_fase := 32.3;
               --
               vn_vl_ret_cum              := nvl(vn_vl_tot_cont_cum_per_rat,0); -- valor de retenção fica igual ao total para zerar
               vn_vl_out_ded_cum          := 0; -- não irá precisar desse valor para zerar o total
               vn_vl_tot_cont_cum_per_rat := 0;
               vn_tot_vl_ret_cum          := nvl(vn_tot_vl_ret_cum,0) + nvl(vn_vl_ret_cum,0);
               --
            elsif nvl(vn_vl_ret_cum,0) = nvl(vn_vl_tot_cont_cum_per_rat,0) then -- valor de retenção igual total
                  --
                  vn_fase := 32.4;
                  -- vn_vl_ret_cum: valor de retenção está igual ao total para zerar
                  vn_vl_out_ded_cum          := 0; -- não irá precisar desse valor para zerar o total
                  vn_vl_tot_cont_cum_per_rat := 0;
                  vn_tot_vl_ret_cum          := nvl(vn_tot_vl_ret_cum,0) + nvl(vn_vl_ret_cum,0);
                  --
            else -- nvl(vn_vl_ret_cum,0) < nvl(vn_vl_tot_cont_cum_per,0) -- valor de retenção menor que total
               --
               vn_fase := 32.5;
               -- vn_vl_ret_cum: valor de retenção será usado e ficará saldo no total
               if (nvl(vn_vl_ret_cum,0) + nvl(vn_vl_out_ded_cum,0)) > nvl(vn_vl_tot_cont_cum_per_rat,0) then -- valor de (retenção + outras) está maior que o total
                  --
                  vn_fase := 32.6;
                  --
                  vn_vl_out_ded_cum          := nvl(vn_vl_tot_cont_cum_per_rat,0) - nvl(vn_vl_ret_cum,0); -- valor de outras deve ser a diferença para zerar o total
                  vn_tot_vl_ret_cum          := nvl(vn_tot_vl_ret_cum,0) + nvl(vn_vl_ret_cum,0);
                  vn_tot_vl_out_ded_cum      := nvl(vn_tot_vl_out_ded_cum,0) + nvl(vn_vl_out_ded_cum,0);
                  vn_vl_tot_cont_cum_per_rat := 0;
                  --
               elsif (nvl(vn_vl_ret_cum,0) + nvl(vn_vl_out_ded_cum,0)) = nvl(vn_vl_tot_cont_cum_per_rat,0) then -- valor de (retenção + outras) igual ao total
                     --
                     vn_fase := 32.7;
                     -- vn_vl_ret_cum: valor de retenção está igual ao total para zerar
                     -- vn_vl_out_ded_cum: valor de outras está igual ao total para zerar
                     vn_tot_vl_ret_cum          := nvl(vn_tot_vl_ret_cum,0) + nvl(vn_vl_ret_cum,0);
                     vn_tot_vl_out_ded_cum      := nvl(vn_tot_vl_out_ded_cum,0) + nvl(vn_vl_out_ded_cum,0);
                     vn_vl_out_ded_cum          := 0;
                     vn_vl_tot_cont_cum_per_rat := 0;
                     --
               else -- (nvl(vn_vl_ret_cum,0) + nvl(vn_vl_out_ded_cum,0)) < nvl(vn_vl_tot_cont_cum_per,0) then -- valor de (retenção + outras) menor que total
                  --
                  vn_fase := 32.8;
                  -- vn_vl_ret_cum: valor de retenção será usado e ficará saldo no total
                  -- vn_vl_out_ded_cum: valor de outras será usado e ficará saldo no total
                  vn_vl_tot_cont_cum_per_rat := (nvl(vn_vl_tot_cont_cum_per_rat,0) - (nvl(vn_vl_ret_cum,0) + nvl(vn_vl_out_ded_cum,0)));
                  vn_tot_vl_ret_cum          := nvl(vn_tot_vl_ret_cum,0) + nvl(vn_vl_ret_cum,0);
                  vn_tot_vl_out_ded_cum      := nvl(vn_tot_vl_out_ded_cum,0) + nvl(vn_vl_out_ded_cum,0);
                  vn_vl_out_ded_cum          := 0;
                  --
               end if;
               --
            end if;
            --
         end if;
         --
         vn_fase := 33;
         vn_vl_ret_ded_bl_1300_cum := 0;
         --
         vn_fase := 34;
         -- Atualizar o valor do bloco 1300 - campo vl_ret_ded
         for r_vl_imp_pis in c_vl_imp_pis( en_dm_ind_nat_rec => 1 -- 0-dedução de natureza não-cumulativa, 1-dedução de natureza cumulativa
                                         , ev_dm_ind_nat_ret => r_vl_ret_apu.dm_ind_nat_ret)
         loop
            --
            exit when c_vl_imp_pis%notfound or (c_vl_imp_pis%notfound) is null;
            --
            vn_fase := 35;
            --
            if nvl(r_vl_imp_pis.vl_ret_pis,0) > nvl(vn_vl_ret_cum,0) then
               --
               vn_fase := 35.1;
               -- valor do imposto atingiu o valor acumulado de retenção para dedução não-cumulativa
               vn_vl_ret_ded_bl_1300_cum := nvl(vn_vl_ret_ded_bl_1300_cum,0) + nvl(vn_vl_ret_cum,0);
               exit;
               --
            elsif nvl(r_vl_imp_pis.vl_ret_pis,0) = nvl(vn_vl_ret_cum,0) then
                  --
                  vn_fase := 35.2;
                  -- valor do imposto atingiu o valor acumulado de retenção para dedução não-cumulativa
                  vn_vl_ret_ded_bl_1300_cum := nvl(vn_vl_ret_ded_bl_1300_cum,0) + nvl(r_vl_imp_pis.vl_ret_pis,0);
                  exit;
                  --
            else -- nvl(r_vl_imp_pis.vl_ret_pis,0) < nvl(vn_vl_ret_cum,0)
               --
               vn_fase := 35.3;
               -- valor do imposto atingiu o valor acumulado de retenção para dedução não-cumulativa
               vn_vl_ret_cum             := nvl(vn_vl_ret_cum,0) - nvl(r_vl_imp_pis.vl_ret_pis,0);
               vn_vl_ret_ded_bl_1300_cum := nvl(vn_vl_ret_ded_bl_1300_cum,0) + nvl(r_vl_imp_pis.vl_ret_pis,0);
               --
            end if;
            --
         end loop;
         --
         vn_fase := 36;
         --
         if nvl(vn_contrvlrretfontepis_id,0) = 0 then
            --
            vn_fase := 37;
            --
            begin
               select contrvlrretfontepis_seq.nextval
                 into vn_contrvlrretfontepis_id
                 from dual;
            exception
               when others then
                  gv_resumo_log := 'Problemas ao recuperar sequence do controle de valor de retenção apurada e deduzida - valores cumulativos - Bloco '||
                                   '1300 (pk_apur_pis.pkb_calcula_bloco_1300). Erro = '||sqlerrm;
                  pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                                   , ev_mensagem       => gv_mensagem_log
                                                   , ev_resumo         => gv_resumo_log
                                                   , en_tipo_log       => erro_de_sistema
                                                   , en_referencia_id  => gn_referencia_id
                                                   , ev_obj_referencia => gv_obj_referencia );
            end;
            --
            vn_fase := 38;
            --
            begin
               insert into contr_vlr_ret_fonte_pis( id
                                                  , empresa_id
                                                  , ano
                                                  , mes
                                                  , dm_situacao
                                                  , dm_ind_nat_ret
                                                  , per_rec_ret
                                                  , vl_ret_apu
                                                  , vl_ret_ded
                                                  , vl_ret_per
                                                  , vl_ret_dcomp
                                                  , vl_sld_ret
                                                  , dm_origem
                                                  )
                                            values( vn_contrvlrretfontepis_id
                                                  , gt_row_per_cons_contr_pis.empresa_id
                                                  , to_char(gt_row_per_cons_contr_pis.dt_ini,'rrrr')
                                                  , to_char(gt_row_per_cons_contr_pis.dt_ini,'mm')
                                                  , 3 -- dm_situacao: 3-Validado
                                                  , r_vl_ret_apu.dm_ind_nat_ret -- dm_ind_nat_ret
                                                  , to_char(gt_row_per_cons_contr_pis.dt_ini,'mmrrrr') -- per_rec_ret
                                                  , nvl(vn_vl_ret_apu_bl_1300_cum,0) -- vl_ret_apu
                                                  , nvl(vn_vl_ret_ded_bl_1300_cum,0) -- vl_ret_ded
                                                  , 0 -- vl_ret_per
                                                  , 0 -- vl_ret_dcomp
                                                  , (nvl(vn_vl_ret_apu_bl_1300_cum,0) - nvl(vn_vl_ret_ded_bl_1300_cum,0)) -- vl_sld_ret
                                                  , 1 -- 0-digitado, 1-gerado no bloco m200
                                                  );
            exception
               when others then
                  gv_resumo_log := 'Problemas ao incluir controle de valor de retenção apurada e deduzida - valores cumulativos - Bloco 1300 (pk_apur_pis.'||
                                   'pkb_calcula_bloco_1300). Erro = '||sqlerrm;
                  pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                   , ev_mensagem        => gv_mensagem_log
                                                   , ev_resumo          => gv_resumo_log
                                                   , en_tipo_log        => erro_de_sistema
                                                   , en_referencia_id   => gn_referencia_id
                                                   , ev_obj_referencia  => gv_obj_referencia );
            end;
            --
         else
            --
            vn_fase := 39;
            --
            begin
               update contr_vlr_ret_fonte_pis cv
                  set cv.vl_ret_apu = (nvl(cv.vl_ret_apu,0) + nvl(vn_vl_ret_apu_bl_1300_cum,0))
                    , cv.vl_ret_ded = (nvl(cv.vl_ret_ded,0) + nvl(vn_vl_ret_ded_bl_1300_cum,0))
                    , cv.vl_sld_ret = (nvl(cv.vl_sld_ret,0) + (nvl(vn_vl_ret_apu_bl_1300_cum,0) - nvl(vn_vl_ret_ded_bl_1300_cum,0)))
                where cv.id = vn_contrvlrretfontepis_id;
            exception
               when others then
                  gv_resumo_log := 'Problemas ao alterar controle de valor de retenção apurada e deduzida - valores cumulativos - Bloco 1300 '||
                                   '(pk_apur_pis.pkb_calcula_bloco_1300). Erro = '||sqlerrm;
                  pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                   , ev_mensagem        => gv_mensagem_log
                                                   , ev_resumo          => gv_resumo_log
                                                   , en_tipo_log        => erro_de_sistema
                                                   , en_referencia_id   => gn_referencia_id
                                                   , ev_obj_referencia  => gv_obj_referencia );
            end;
            --
         end if;
         --
      end loop;
      --
      vn_fase := 40;
      --
      if vv_proc_ded = 'S' then -- executou o processo de deduções dos impostos retidos
         vn_vl_ret_cum      := nvl(vn_tot_vl_ret_cum,0);
         vn_vl_out_ded_cum  := nvl(vn_tot_vl_out_ded_cum,0);
         vn_vl_cont_cum_rec := nvl(vn_vl_tot_cont_cum_per_rat,0);
      else -- não executou o processo de deduções dos impostos retidos
         vn_vl_ret_cum      := 0;
         vn_vl_out_ded_cum  := nvl(vn_vl_out_ded_cum,0);
         vn_vl_cont_cum_rec := nvl(vn_vl_tot_cont_cum_per_rat,0) - nvl(vn_vl_ret_cum,0) - nvl(vn_vl_out_ded_cum,0);
      end if;
      --
   end if;
   --
   vn_fase := 41;
   -- Variáveis de retorno:
   sn_vl_ret_nc           := nvl(vn_vl_ret_nc,0);
   sn_vl_out_ded_nc       := nvl(vn_vl_out_ded_nc,0);
   sn_vl_cont_nc_rec      := nvl(vn_vl_cont_nc_rec,0);
   sn_vl_tot_cont_cum_per := nvl(vn_vl_tot_cont_cum_per,0);
   sn_vl_ret_cum          := nvl(vn_vl_ret_cum,0);
   sn_vl_out_ded_cum      := nvl(vn_vl_out_ded_cum,0);
   sn_vl_cont_cum_rec     := nvl(vn_vl_cont_cum_rec,0);
   --
EXCEPTION
   when others then
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.pkb_calcula_bloco_1300 (fase = '||vn_fase||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => gn_referencia_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
END PKB_CALCULA_BLOCO_1300;
------------------------------------------------------------------
--| Procedimento para calcular a consolidação do PIS - Bloco M200
--| Serão calculados alguns campos dos blocos M200, M210 e M220
------------------------------------------------------------------
PROCEDURE PKB_CALCULAR_CONS_PIS_M200( EN_CONSCONTRPIS_ID IN CONS_CONTR_PIS.ID%TYPE ) IS
   --
   vn_fase                     number := 0;
   vn_vl_ajus_acres            det_cons_contr_pis.vl_ajust_acrec%type;
   vn_vl_ajus_reduc            det_cons_contr_pis.vl_ajust_reduc%type;
   vn_vl_difer                 det_cons_contr_pis.vl_cont_difer%type;
   vn_vl_difer_ant             det_cons_contr_pis.vl_cont_difer_ant%type;
   vn_vl_cont_per              det_cons_contr_pis.vl_cont_per%type;
   vn_vl_cont_apur             det_cons_contr_pis.vl_cont_apur%type;
   vn_vl_ajus_acres_bc_pis     det_cons_contr_pis.vl_ajus_acres_bc_pis%type;
   vn_vl_ajus_reduc_bc_pis     det_cons_contr_pis.vl_ajus_reduc_bc_pis%type;
   vn_vl_bc_cont_ajus          det_cons_contr_pis.vl_bc_cont_ajus%type;
   vn_vl_tot_cont_nc_per       cons_contr_pis.vl_tot_cont_nc_per%type;
   vn_vl_tot_cred_desc         cons_contr_pis.vl_tot_cred_desc%type;
   vn_vl_tot_cred_desc_ant     cons_contr_pis.vl_tot_cred_desc_ant%type;
   vn_vl_totcreddescant_1100   cons_contr_pis.vl_tot_cred_desc_ant%type;   
   vn_vl_tot_cont_nc_dev       cons_contr_pis.vl_tot_cont_nc_dev%type;
   vn_vl_ret_nc                cons_contr_pis.vl_ret_nc%type;
   vn_vl_out_ded_nc            cons_contr_pis.vl_out_ded_nc%type;
   vn_vl_cont_nc_rec           cons_contr_pis.vl_cont_nc_rec%type;
   vn_vl_tot_cont_cum_per      cons_contr_pis.vl_tot_cont_cum_per%type;
   vn_vl_ret_cum               cons_contr_pis.vl_ret_cum%type;
   vn_vl_out_ded_cum           cons_contr_pis.vl_out_ded_cum%type;
   vn_vl_cont_cum_rec          cons_contr_pis.vl_cont_cum_rec%type;
   vn_vl_tot_cont_rec          cons_contr_pis.vl_tot_cont_rec%type;
   vn_vl_cont_nc_rec_m205      cons_contr_pis.vl_cont_nc_rec%type;
   vn_vl_cont_cum_rec_m205     cons_contr_pis.vl_cont_cum_rec%type;
   vn_loggenerico_id           log_generico.id%type;
   vn_valor                    cons_contr_pis.vl_tot_cred_desc%type;
   vn_dm_ind_desc_cred_gr      apur_cred_pis.dm_ind_desc_cred%type := 0;
   vn_vl_cred_desc_gr          number := 0;
   vn_vl_sld_cred_gr           number := 0;
   vv_zerar                    varchar2(1) := 'N';
   vv_calculou_1100            varchar2(1) := 'N';   
   vn_dia_pagto                param_efd_contr.dia_pagto%type := null;
   vn_qtde_mes_subseq          param_efd_contr.qtde_mes_subseq%type := null;
   vn_tiporetimp_id_pis_cum    param_efd_contr.tiporetimp_id_pis_cum%type := null;
   vn_tiporetimprec_id_pis_cum param_efd_contr.tiporetimpreceita_id_pis_cum%type := null;
   vn_tiporetimp_id_pis_nc     param_efd_contr.tiporetimp_id_pis_nc%type := null;
   vn_tiporetimprec_id_pis_nc  param_efd_contr.tiporetimpreceita_id_pis_nc%type := null;
   vn_dia                      number;
   vn_mes                      number;
   vn_ano                      number;
   vd_dt_vencto                cons_contr_pis_or.dt_vencto%type := null;
   --
   cursor c_per_cons_contr_pis( en_perconscontrpis_id in per_cons_contr_pis.id%type ) is
      select *
        from per_cons_contr_pis pc
       where pc.id  = en_perconscontrpis_id;
   --
   cursor c_det_cons is -- detalhe da consolidação - bloco m200
      select dc.id detconscontrpis_id
           , dc.contrsocapurpc_id
           , dc.vl_cont_apur
           , dc.vl_bc_cont
           , dc.vl_bc_cont_ajus
           , dc.aliq_pis
           , dc.quant_bc_pis
           , dc.vl_aliq_pis_quant
        from det_cons_contr_pis dc
       where dc.conscontrpis_id = gt_row_cons_contr_pis.id;
   --
   cursor c_vl_aj( en_detconscontrpis_id in number
                 , en_ind_aj             in number ) is -- ajuste da consolidação - bloco m200
      select nvl(sum(nvl(ac.vl_aj,0)),0) vl_aj
        from ajust_contr_pis_apur ac
       where ac.detconscontrpis_id = en_detconscontrpis_id
         and ac.dm_ind_aj          = en_ind_aj; -- 0-ajuste de redução, 1-ajuste de acréscimo - bloco m200
   --
   cursor c_vl_difer( en_detconscontrpis_id in number ) is -- informações adicionais da consolidação - bloco m200
      select nvl(sum(nvl(ia.vl_cont_dif,0)),0)
        from inf_adic_dif_pis ia
       where ia.detconscontrpis_id = en_detconscontrpis_id;
   --
   cursor c_vl_difer_ant( en_contrsocapurpc_id in number ) is -- contribuição diferida em períodos anteriores - bloco m300
      select nvl(sum(nvl(cp.vl_cont_difer_ant,0)),0)
        from contr_pis_dif_per_ant cp
       where cp.empresa_id        = gt_row_per_cons_contr_pis.empresa_id
         and cp.dt_ini      between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
         and cp.dt_fin      between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
         and cp.contrsocapurpc_id = en_contrsocapurpc_id
         and cp.dm_situacao       = 3; -- Processada
   --
   cursor c_vl_ajus_acres_bc_pis (en_detconscontrpis_id in number) is 
   select sum(nvl(decode(abc.dm_ind_aj_bc, 0, 0, abc.vl_aj_bc), 0)) vl_ajus_acres_bc_pis 
        , sum(nvl(decode(abc.dm_ind_aj_bc, 0, abc.vl_aj_bc, 0), 0)) vl_ajus_reduc_bc_pis
      from AJUST_BC_CONT_PIS abc
   where abc.detconscontrpis_id = en_detconscontrpis_id;   
   --
   cursor c_vl_tot_cont_per is -- detalhe da consolidação - bloco m200
      select nvl(sum(nvl(dc.vl_cont_per,0)),0)
        from det_cons_contr_pis dc
       where dc.conscontrpis_id = gt_row_cons_contr_pis.id
         and ((gn_dm_cod_inc_trib in (1, 3) and -- 1-regime não-cumulativo, 3-regime cumulativo e não-cumulativo
               pk_csf_efd_pc.fkg_cd_contr_soc_apur_pc ( dc.contrsocapurpc_id ) in ('01', '02', '03', '04', '32'))
               or
              (gn_dm_cod_inc_trib = 1 and -- 1-regime não-cumulativo
               pk_csf_efd_pc.fkg_cd_contr_soc_apur_pc ( dc.contrsocapurpc_id ) in ('01', '02', '03', '04', '32', '71')));
   --
   cursor c_vl_tot_cred_ajuste is -- apuração do pis - bloco m100
      select pa.id
           --, substr(tc.cd,1,1) cd_tipocredpc
           , tc.cd cd_tipocredpc
           , tc.id tipocredpc_id
           , ac.aliq_pis
           , nvl(ac.vl_cred_disp,0) vl_cred_disp
           , nvl(ac.vl_cred_desc,0) vl_cred_desc
        from tipo_cred_pc      tc
           , apur_cred_pis     ac
           , per_apur_cred_pis pa
       where pa.empresa_id        = gt_row_per_cons_contr_pis.empresa_id
         and pa.dt_ini      between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
         and pa.dt_fin      between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
         and ac.perapurcredpis_id = pa.id
         and ac.dm_situacao       = 3 -- 3-processada
         and ac.dm_ind_cred_ori   = 0 -- Indicador de Credito Oriundo de: 0-Operações próprias, 1-Evento de incorporação, cisão ou fusão
         and tc.id                = ac.tipocredpc_id
       order by 1,2;
   --
   cursor c_vl_tot_cred_total is -- apuração do pis - bloco m100
      select nvl(sum(nvl(ac.vl_cred_desc,0)),0)
        from apur_cred_pis     ac
           , per_apur_cred_pis pa
       where pa.empresa_id        = gt_row_per_cons_contr_pis.empresa_id
         and pa.dt_ini      between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
         and pa.dt_fin      between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
         and ac.perapurcredpis_id = pa.id
         and ac.dm_situacao       = 3; -- 3-processada
   --
   cursor c_vl_cred_desc_efd is -- controle de créditos fiscais - bloco 1100
      select nvl(sum(nvl(cc.vl_cred_desc_efd,0)),0)
        from contr_cred_fiscal_pis cc
           , relac_apur_contr_pis  ra
       where cc.empresa_id      = gt_row_per_cons_contr_pis.empresa_id
         and cc.dm_situacao     = 3 -- 0-Aberto, 1-Calculada, 2-Erro no cálculo, 3-Validado, 4-Erro de validação
         and cc.dm_orig_cred    = '01' -- 01-crédito decorrente de operações próprias; 02-credito transferido por pessoa jurídica sucedida
         and ((cc.ano = to_char(gt_row_per_cons_contr_pis.dt_ini,'rrrr') -- ano
               and
               cc.mes < to_char(gt_row_per_cons_contr_pis.dt_ini,'mm')) -- mes
              or
              (cc.ano < to_char(gt_row_per_cons_contr_pis.dt_ini,'rrrr'))) -- ano
         and ra.contrcredfiscalpis_id = cc.id
         and ra.ano                   = to_char(gt_row_per_cons_contr_pis.dt_ini,'rrrr') -- ano
         and ra.mes                   = to_char(gt_row_per_cons_contr_pis.dt_ini,'mm'); -- mes
   --
   cursor c_param is
      select pe.dia_pagto
           , pe.qtde_mes_subseq
           , pe.tiporetimp_id_pis_cum
           , pe.tiporetimpreceita_id_pis_cum
           , pe.tiporetimp_id_pis_nc
           , pe.tiporetimpreceita_id_pis_nc
        from param_efd_contr pe
       where pe.empresa_id = gt_row_per_cons_contr_pis.empresa_id;
   --
BEGIN
   --
   vn_fase := 1;
   --
   pkb_excluir_log( en_referencia_id  => en_conscontrpis_id
                  , ev_obj_referencia => 'CONS_CONTR_PIS' );
   --
   vn_fase := 2;
   --
   gv_obj_referencia := 'CONS_CONTR_PIS';
   gv_mensagem_log   := 'Cálculo da consolidação do imposto PIS/PASEP';
   gv_resumo_log     := 'Início do processo (identificador da consolidação = '||en_conscontrpis_id||').';
   gn_referencia_id  := en_conscontrpis_id;
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => gn_referencia_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
   gv_resumo_log := null;
   --
   vn_fase := 3;
   --
   if nvl(en_conscontrpis_id,0) = 0 then
      --
      vn_fase := 4;
      gv_resumo_log := 'O identificador da consolidação deve ser informado para que o cálculo seja efetuado.';
      pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                       , ev_mensagem        => gv_mensagem_log
                                       , ev_resumo          => gv_resumo_log
                                       , en_tipo_log        => erro_de_validacao
                                       , en_referencia_id   => null
                                       , ev_obj_referencia  => gv_obj_referencia );
      --
   else
      --
      vn_fase := 5;
      -- Recupera os dados da consolidação do imposto pis utilizado como referência
      pkb_dados_cons_pis_m200( en_conscontrpis_id => en_conscontrpis_id );
      gv_mensagem_log := 'Cálculo da '||gv_mensagem_log;
      --
      vn_fase := 6;
      --
      if gt_row_cons_contr_pis.dm_situacao <> 0 then -- não estiver em aberto
         --
         vn_fase := 7;
         gv_resumo_log := 'A consolidação informada não está com situação em aberto, portanto, o cálculo não será realizado.';
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_validacao
                                          , en_referencia_id   => gn_referencia_id
                                          , ev_obj_referencia  => gv_obj_referencia );
         --
      else
         --
         vn_fase := 8;
         --
         if nvl(gt_row_per_cons_contr_pis.id,0) = 0 then
            --
            vn_fase := 9;
            gt_row_per_cons_contr_pis := null;
            open c_per_cons_contr_pis( en_perconscontrpis_id => gt_row_cons_contr_pis.perconscontrpis_id );
            fetch c_per_cons_contr_pis into gt_row_per_cons_contr_pis;
            close c_per_cons_contr_pis;
            --
         end if;
         --
         vn_fase := 10;
         -- Recupera os dados da empresa
         pkb_dados_abert_empr( en_empresa_id => gt_row_per_cons_contr_pis.empresa_id
                             , ed_dt_inicial => gt_row_per_cons_contr_pis.dt_ini
                             , ed_dt_final   => gt_row_per_cons_contr_pis.dt_fin );
         --
         vn_fase := 11;
         --
         if nvl(gn_aberturaefdpc_id,0) <> 0 then
            --
            vn_fase := 12;
            --
            for r_reg in c_det_cons
            loop
               --
               vn_fase := 13;
               --
               open c_vl_aj( en_detconscontrpis_id => r_reg.detconscontrpis_id
                           , en_ind_aj             => 1); -- 0-ajuste de redução, 1-ajuste de acréscimo
               fetch c_vl_aj into vn_vl_ajus_acres;
               close c_vl_aj;
               --
               vn_fase := 14;
               --
               open c_vl_aj( en_detconscontrpis_id => r_reg.detconscontrpis_id
                           , en_ind_aj             => 0); -- 0-ajuste de redução, 1-ajuste de acréscimo
               fetch c_vl_aj into vn_vl_ajus_reduc;
               close c_vl_aj;
               --
               vn_fase := 15;
               --
               open c_vl_difer( en_detconscontrpis_id => r_reg.detconscontrpis_id );
               fetch c_vl_difer into vn_vl_difer;
               close c_vl_difer;
               --
               vn_fase := 16;
               --
               open c_vl_difer_ant( en_contrsocapurpc_id => r_reg.contrsocapurpc_id );
               fetch c_vl_difer_ant into vn_vl_difer_ant;
               close c_vl_difer_ant;
               --
               vn_fase := 17;
               --
               open c_vl_ajus_acres_bc_pis( en_detconscontrpis_id => r_reg.detconscontrpis_id );
               fetch c_vl_ajus_acres_bc_pis into vn_vl_ajus_acres_bc_pis, vn_vl_ajus_reduc_bc_pis;
               close c_vl_ajus_acres_bc_pis;
               --
               vn_fase := 18;
               --
               vn_vl_bc_cont_ajus := (nvl(r_reg.vl_bc_cont,0) + nvl(vn_vl_ajus_acres_bc_pis,0) - nvl(vn_vl_ajus_reduc_bc_pis,0));
               --
               vn_fase := 18.1;
               --
               vn_vl_cont_apur := (nvl(vn_vl_bc_cont_ajus,0) * r_reg.aliq_pis) /100;
               --
               -- Caso não conseguiu montar o valor de vl_cont_apur, tenta por Quantidade na Base de Calculo e Aliquota em Reais --
               if nvl(vn_vl_cont_apur,0) = 0 then
                  --
                  vn_fase := 18.11;
                  --
                  vn_vl_cont_apur := nvl(r_reg.quant_bc_pis,0) * nvl(r_reg.vl_aliq_pis_quant,0);
                  --
               end if;
               --
               vn_fase := 18.2;
               --
               vn_vl_cont_per := (nvl(vn_vl_cont_apur,0) + nvl(vn_vl_ajus_acres,0) - nvl(vn_vl_ajus_reduc,0) -
                                  nvl(vn_vl_difer,0) + nvl(vn_vl_difer_ant,0));
               
               --               
               vn_fase := 19;
               --
               begin
                  update det_cons_contr_pis dc
                     set dc.vl_ajust_acrec       = vn_vl_ajus_acres
                       , dc.vl_ajust_reduc       = vn_vl_ajus_reduc
                       , dc.vl_cont_difer        = vn_vl_difer
                       , dc.vl_cont_difer_ant    = vn_vl_difer_ant
                       , dc.vl_cont_per          = nvl(vn_vl_cont_per,0)
                       , dc.vl_ajus_acres_bc_pis = nvl(vn_vl_ajus_acres_bc_pis,0)
                       , dc.vl_ajus_reduc_bc_pis = nvl(vn_vl_ajus_reduc_bc_pis,0)
                       , dc.vl_cont_apur         = nvl(vn_vl_cont_apur,0) 
                       , dc.vl_bc_cont_ajus      = nvl(vn_vl_bc_cont_ajus,0)
                   where dc.id = r_reg.detconscontrpis_id;
               exception
                  when others then
                     gv_resumo_log := 'Problemas ao atualizar os valores do detalhe da consolidação. Erro = '||sqlerrm;
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                      , ev_mensagem        => gv_mensagem_log
                                                      , ev_resumo          => gv_resumo_log
                                                      , en_tipo_log        => erro_de_sistema
                                                      , en_referencia_id   => gn_referencia_id
                                                      , ev_obj_referencia  => gv_obj_referencia );
               end;
               --
               vn_fase := 20;
               --| Atualizar número de documento e descrição de ajustes
               begin
                  update ajust_contr_pis_apur ac
                     set ac.num_doc  = ltrim(rtrim(pk_csf.fkg_converte(ac.num_doc)))
                       , ac.descr_aj = ltrim(rtrim(pk_csf.fkg_converte(ac.descr_aj)))
                   where ac.detconscontrpis_id = r_reg.detconscontrpis_id;
               exception
                  when others then
                     gv_resumo_log := 'Problemas ao atualizar número de documento e descrição de ajustes (contribuição). Erro = '||sqlerrm;
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                      , ev_mensagem        => gv_mensagem_log
                                                      , ev_resumo          => gv_resumo_log
                                                      , en_tipo_log        => erro_de_sistema
                                                      , en_referencia_id   => gn_referencia_id
                                                      , ev_obj_referencia  => gv_obj_referencia );
               end;
               --
            end loop;
            --
            vn_fase := 21;
            --
            if gn_dm_cod_inc_trib = 2 then -- 2-Exclusivamente no regime cumulativo
               vn_vl_tot_cont_nc_per := 0;
            else
               open c_vl_tot_cont_per; -- soma detalhe da consolidação - bloco m200 -> det_cons_contr_pis.vl_cont_per
               fetch c_vl_tot_cont_per into vn_vl_tot_cont_nc_per;
               close c_vl_tot_cont_per;
            end if;
            --
            vn_fase := 22;
            --
            if gn_dm_cod_inc_trib = 2 then -- 2-Exclusivamente no regime cumulativo
               --
               vn_fase := 22.1;
               --
               vn_vl_tot_cred_desc := 0;
               --
               begin
                  update apur_cred_pis ac
                     set ac.vl_cred_desc     = 0 -- zerado
                       , ac.dm_ind_desc_cred = 0 -- 0-Utilização do valor total, 1-Utilização de valor parcial => p/descto da contrib. no período, Registro M200
                       , ac.vl_sld_cred      = ac.vl_cred_disp -- (vl_cred_disp - vl_cred_desc)
                   where ac.dm_ind_cred_ori    = 0 -- Indicador de Credito Oriundo de: 0-Operações próprias, 1-Evento de incorporação, cisão ou fusão
                     and ac.perapurcredpis_id in (select pa.id
                                                    from per_apur_cred_pis pa
                                                   where pa.empresa_id        = gt_row_per_cons_contr_pis.empresa_id
                                                     and pa.dt_ini      between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
                                                     and pa.dt_fin      between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
                                                     and ac.perapurcredpis_id = pa.id);
               exception
                  when others then
                     gv_resumo_log := 'Problemas ao atualizar Bloco M100 - total de crédito descontado (zero - apuração de pis). Erro = '||sqlerrm;
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                                      , ev_mensagem       => gv_mensagem_log
                                                      , ev_resumo         => gv_resumo_log
                                                      , en_tipo_log       => erro_de_sistema
                                                      , en_referencia_id  => gn_referencia_id
                                                      , ev_obj_referencia => gv_obj_referencia );
               end;
               --
            elsif nvl(vn_vl_tot_cont_nc_per,0) = 0 then
                  --
                  vn_fase := 23;
                  vn_vl_tot_cred_desc := 0;
                  --
                  begin
                     update apur_cred_pis ac
                        set ac.vl_cred_desc     = 0 -- zerado
                          , ac.dm_ind_desc_cred = 1 -- 0-Utilização do valor total, 1-Utilização de valor parcial => p/descto da contrib. no período, Registro M200
                          , ac.vl_sld_cred      = ac.vl_cred_disp -- (vl_cred_disp - vl_cred_desc)
                      where ac.dm_ind_cred_ori    = 0 -- Indicador de Credito Oriundo de: 0-Operações próprias, 1-Evento de incorporação, cisão ou fusão
                        and ac.perapurcredpis_id in (select pa.id
                                                       from per_apur_cred_pis pa
                                                      where pa.empresa_id        = gt_row_per_cons_contr_pis.empresa_id
                                                        and pa.dt_ini      between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
                                                        and pa.dt_fin      between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
                                                        and ac.perapurcredpis_id = pa.id);
                  exception
                     when others then
                        gv_resumo_log := 'Problemas ao atualizar Bloco M100 - total da contribuição não-cumulativa (zero - apuração de pis). Erro = '||sqlerrm;
                        pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                                         , ev_mensagem       => gv_mensagem_log
                                                         , ev_resumo         => gv_resumo_log
                                                         , en_tipo_log       => erro_de_sistema
                                                         , en_referencia_id  => gn_referencia_id
                                                         , ev_obj_referencia => gv_obj_referencia );
                  end;
                  --
            else
               --
               vn_fase := 24;
               --
               vv_zerar               := 'N';
               vn_dm_ind_desc_cred_gr := 1; -- 1-Utilização de valor parcial para descto da contrib. no período, Registro M200
               vn_vl_cred_desc_gr     := 0;
               vn_vl_sld_cred_gr      := 0;
               vn_valor               := nvl(vn_vl_tot_cont_nc_per,0);
               --
               vn_fase := 25;
               --
               for r_reg in c_vl_tot_cred_ajuste
               loop
                  --
                  vn_fase := 26;
                  --
                  vn_valor := (nvl(vn_valor,0) - nvl(r_reg.vl_cred_disp,0));
                  --
                  vn_fase := 27;
                  --
                  if vv_zerar = 'S' then
                     --
                     vn_fase := 27.1;
                     if nvl(r_reg.vl_cred_disp,0) = 0 then
                        vn_dm_ind_desc_cred_gr := 0; -- 0-Utilização do valor total para descto da contrib. no período, Registro M200
                     else
                        vn_dm_ind_desc_cred_gr := 1; -- 1-Utilização de valor parcial para descto da contrib. no período, Registro M200
                     end if;
                     vn_vl_cred_desc_gr     := 0;
                     vn_vl_sld_cred_gr      := nvl(r_reg.vl_cred_disp,0);
                     --
                  elsif nvl(vn_valor,0) > 0 then
                        --
                        vn_fase := 27.2;
                        vn_dm_ind_desc_cred_gr := 0; -- 0-Utilização do valor total para descto da contrib. no período, Registro M200
                        vn_vl_cred_desc_gr     := r_reg.vl_cred_disp;
                        vn_vl_sld_cred_gr      := 0;
                        --
                  elsif nvl(vn_valor,0) < 0 then
                        --
                        vn_fase := 27.3;
                        vn_dm_ind_desc_cred_gr := 1; -- 1-Utilização de valor parcial para descto da contrib. no período, Registro M200
                        vn_vl_cred_desc_gr     := (nvl(vn_valor,0) + nvl(r_reg.vl_cred_disp,0));
                        vn_vl_sld_cred_gr      := (nvl(r_reg.vl_cred_disp,0) - (nvl(vn_valor,0) + nvl(r_reg.vl_cred_disp,0)));
                        vv_zerar               := 'S';
                        --
                  elsif nvl(vn_valor,0) = 0 then
                        --
                        vn_fase := 27.4;
                        vn_dm_ind_desc_cred_gr := 0; -- 0-Utilização do valor total para descto da contrib. no período, Registro M200
                        vn_vl_cred_desc_gr     := r_reg.vl_cred_disp;
                        vn_vl_sld_cred_gr      := 0;
                        vv_zerar               := 'S';
                        --
                  end if;
                  --
                  vn_fase := 28;
                  --
                  begin
                     update apur_cred_pis ac
                        set ac.vl_cred_desc     = vn_vl_cred_desc_gr
                          , ac.dm_ind_desc_cred = vn_dm_ind_desc_cred_gr
                          , ac.vl_sld_cred      = vn_vl_sld_cred_gr
                      where ac.perapurcredpis_id = r_reg.id
                        and ac.tipocredpc_id     = r_reg.tipocredpc_id
                        and ac.aliq_pis          = r_reg.aliq_pis
                        and ac.dm_ind_cred_ori   = 0; -- Indicador de Credito Oriundo de: 0-Operações próprias, 1-Evento de incorporação, cisão ou fusão
                  exception
                     when others then
                        gv_resumo_log := 'Problemas ao atualizar Bloco M100 - valor de crédito descontado -  (apuração de pis). Erro = '||sqlerrm;
                        pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                                         , ev_mensagem       => gv_mensagem_log
                                                         , ev_resumo         => gv_resumo_log
                                                         , en_tipo_log       => erro_de_sistema
                                                         , en_referencia_id  => gn_referencia_id
                                                         , ev_obj_referencia => gv_obj_referencia );
                  end;
                  --
               end loop;
               --
               vn_fase := 29;
               --
               open c_vl_tot_cred_total; -- soma apuração do pis - bloco m100 -> apur_cred_pis.vl_cred_desc
               fetch c_vl_tot_cred_total into vn_vl_tot_cred_desc;
               close c_vl_tot_cred_total;
               --
            end if;
            --
            vn_fase := 30;
            -- Excluir os registros do Bloco 1100 gerados pelos Blocos M100/M200
            pkb_exclui_bloco_1100_m100;
            --
            vn_fase := 31;
            -- Gerar os registros no Bloco 1100 dos saldos gerados nas apurações do Bloco M100
            pkb_gera_bloco_1100_m100;
            --
            vn_fase := 32;
            --
            if gn_dm_cod_inc_trib = 2 then -- 2-Exclusivamente no regime cumulativo
               vn_vl_tot_cred_desc_ant := 0;
            else
               --
               vn_fase := 32.1;
               --
               if (nvl(vn_vl_tot_cont_nc_per,0) - nvl(vn_vl_tot_cred_desc,0)) < 0 then
                  -- se o valor ficar negativo houve algum erro entre o bloco m100 e m200, e o valor de desconto anterior não será utilizado - bloco 1100
                  vn_fase := 32.2;
                  vn_vl_tot_cred_desc_ant := 0;
                  --
               elsif (nvl(vn_vl_tot_cont_nc_per,0) - nvl(vn_vl_tot_cred_desc,0)) = 0 then
                     -- se o valor zerar entre o bloco m100 e m200, o valor de desconto anterior não será utilizado - bloco 1100
                     vn_fase := 32.3;
                     vn_vl_tot_cred_desc_ant := 0;
                     --
               else -- (nvl(vn_vl_tot_cont_nc_per,0) - nvl(vn_vl_tot_cred_desc,0)) > 0 -- temos um saldo que poderá ser "abatido" pelos valores do Bloco 1100
                  --
                  vn_fase := 32.4;
                  --
                  vn_vl_totcreddescant_1100 := 0; -- Valor utilizado no registro 1100
                  --                  
                  pkb_calcula_bloco_1100( en_saldo_m200     => (nvl(vn_vl_tot_cont_nc_per,0) - nvl(vn_vl_tot_cred_desc,0) - nvl(vn_vl_tot_cred_desc_ant,0)),
                                          en_ver_vlr_zerado => 1,
                                          sn_sld_efd_1100   => vn_vl_totcreddescant_1100 );
                  --
                  vv_calculou_1100 := 'S';
                  --				  
                  vn_fase := 32.5;
                  --
                  vn_vl_tot_cred_desc_ant := 0;
                  --open c_vl_cred_desc_efd; -- soma crédito do pis - bloco 1100 -> contr_cred_fiscal_pis.vl_cred_desc_efd
                  --fetch c_vl_cred_desc_efd into vn_vl_tot_cred_desc_ant;
                  --close c_vl_cred_desc_efd;
                  --
                  vn_fase := 32.6;
                  --
                  --if nvl(vn_vl_tot_cred_desc_ant,0) = nvl(vn_vl_totcreddescant_1100,0)then
                     --				  
                     --vn_vl_tot_cred_desc_ant := nvl(vn_vl_tot_cred_desc_ant,0);	
                     --					 
                  --else
                     -- 				  
                     vn_vl_tot_cred_desc_ant := nvl(vn_vl_tot_cred_desc_ant,0) + nvl(vn_vl_totcreddescant_1100,0);
                     -- 					 
                  --end if;
                  --				  
               end if;
               --
            end if;
            --
            vn_fase := 33;
            -- Soma bl.m200 det_cons_contr_pis.vl_cont_per - soma bl.m100 apur_cred_pis.vl_cred_desc - soma bl.1100 contr_cred_fiscal_pis.vl_cred_desc_efd
            vn_vl_tot_cont_nc_dev := (nvl(vn_vl_tot_cont_nc_per,0) - nvl(vn_vl_tot_cred_desc,0) - nvl(vn_vl_tot_cred_desc_ant,0));
            --
            vn_fase := 34;
            -- Compôr os valores Não-Cumulativos e Cumulativos devido a Geração do Bloco 1300
            pkb_calcula_bloco_1300( en_conscontrpis_id     => gt_row_cons_contr_pis.id
                                  , en_totcontncdev_m200   => nvl(vn_vl_tot_cont_nc_dev,0)
                                  , sn_vl_ret_nc           => vn_vl_ret_nc
                                  , sn_vl_out_ded_nc       => vn_vl_out_ded_nc
                                  , sn_vl_cont_nc_rec      => vn_vl_cont_nc_rec
                                  , sn_vl_tot_cont_cum_per => vn_vl_tot_cont_cum_per
                                  , sn_vl_ret_cum          => vn_vl_ret_cum
                                  , sn_vl_out_ded_cum      => vn_vl_out_ded_cum
                                  , sn_vl_cont_cum_rec     => vn_vl_cont_cum_rec );
            --
            vn_fase := 35;
            -- Somar os valores Digitados do bloco M205 - Contribuição para o PIS/Pasep a Recolher - Detalhamento por Código de Receita (Visão Débito DCTF)
            begin
               select nvl(sum(decode(cc.dm_num_campo,'08',nvl(cc.vl_rec,0))),0)
                    , nvl(sum(decode(cc.dm_num_campo,'12',nvl(cc.vl_rec,0))),0)
                 into vn_vl_cont_nc_rec_m205
                    , vn_vl_cont_cum_rec_m205
                 from cons_contr_pis_or cc
                where cc.conscontrpis_id = gt_row_cons_contr_pis.id
                  and cc.dm_num_campo   in ('08','12') -- '08'-Valor não-cumulativo, '12'-Valor cumulativo
                  and cc.dm_origem       = 0; -- 0-Digitado, 1-Gerado no Bloco M200
            exception
               when others then
                  gv_resumo_log := 'Problemas ao somar os valores de Pagamentos - Bloco M205 (pk_apur_pis.pkb_calcular_cons_pis_m200). Erro = '||sqlerrm;
                  pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                   , ev_mensagem        => gv_mensagem_log
                                                   , ev_resumo          => gv_resumo_log
                                                   , en_tipo_log        => erro_de_sistema
                                                   , en_referencia_id   => gn_referencia_id
                                                   , ev_obj_referencia  => gv_obj_referencia );
            end;
            --
            vn_fase := 36;
            -- vn_vl_cont_nc_rec => ((soma bl.m200 det_cons_contr_pis.vl_cont_per - soma bl.m100 apur_cred_pis.vl_cred_desc -
            -- soma bl.1100 contr_cred_fiscal_pis.vl_cred_desc_efd) - (soma bl.1300 contr_vlr_ret_fonte_pis.vl_ret_apu + bl.f600 contr_ret_fonte_pc.vl_ret_pis) -
            -- soma bl.f700 deducao_diversa_pc.vl_ded_pis)
            -- vn_vl_cont_cum_rec => (soma bl.m200 det_cons_contr_pis.vl_cont_per - (soma bl.1300 contr_vlr_ret_fonte_pis.vl_ret_apu + bl.f600 contr_ret_fonte_pc.vl_ret_pis) -
            -- soma bl.f700 deducao_diversa_pc.vl_ded_pis)
            vn_vl_tot_cont_rec := (nvl(vn_vl_cont_nc_rec,0) + nvl(vn_vl_cont_cum_rec,0) + nvl(vn_vl_cont_nc_rec_m205,0) + nvl(vn_vl_cont_cum_rec_m205,0));
            --
            vn_fase := 37;
            --
            begin
               update cons_contr_pis cc
                  set cc.dm_situacao          = 1 -- Calculado
                    , cc.vl_tot_cont_nc_per   = vn_vl_tot_cont_nc_per
                    , cc.vl_tot_cred_desc     = vn_vl_tot_cred_desc
                    , cc.vl_tot_cred_desc_ant = vn_vl_tot_cred_desc_ant
                    , cc.vl_tot_cont_nc_dev   = vn_vl_tot_cont_nc_dev
                    , cc.vl_ret_nc            = vn_vl_ret_nc
                    , cc.vl_out_ded_nc        = vn_vl_out_ded_nc
                    , cc.vl_cont_nc_rec       = (nvl(vn_vl_cont_nc_rec,0) + nvl(vn_vl_cont_nc_rec_m205,0))
                    , cc.vl_tot_cont_cum_per  = vn_vl_tot_cont_cum_per
                    , cc.vl_ret_cum           = vn_vl_ret_cum
                    , cc.vl_out_ded_cum       = vn_vl_out_ded_cum
                    , cc.vl_cont_cum_rec      = (nvl(vn_vl_cont_cum_rec,0) + nvl(vn_vl_cont_cum_rec_m205,0))
                    , cc.vl_tot_cont_rec      = vn_vl_tot_cont_rec
                where cc.id = gt_row_cons_contr_pis.id;
            exception
               when others then
                  gv_resumo_log := 'Problemas ao atualizar valores e situação da consolidação para 1(Calculado). Erro = '||sqlerrm;
                  pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                   , ev_mensagem        => gv_mensagem_log
                                                   , ev_resumo          => gv_resumo_log
                                                   , en_tipo_log        => erro_de_sistema
                                                   , en_referencia_id   => gn_referencia_id
                                                   , ev_obj_referencia  => gv_obj_referencia );
            end;
            --
            vn_fase := 38;
            --
            -- Gerar o bloco M205 - Contribuição para o PIS/Pasep a Recolher - Detalhamento por Código de Receita (Visão Débito DCTF)
            begin
               delete from cons_contr_pis_or cc
                where cc.conscontrpis_id = gt_row_cons_contr_pis.id
                  and cc.dm_origem       = 1; -- 0-Digitado, 1-Gerado no Bloco M200
            exception
               when others then
                  gv_resumo_log := 'Problemas ao excluir Pagamentos - Bloco M205 (pk_apur_pis.pkb_calcular_cons_pis_m200). Erro = '||sqlerrm;
                  pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                   , ev_mensagem        => gv_mensagem_log
                                                   , ev_resumo          => gv_resumo_log
                                                   , en_tipo_log        => erro_de_sistema
                                                   , en_referencia_id   => gn_referencia_id
                                                   , ev_obj_referencia  => gv_obj_referencia );
            end;
            --
            vn_fase := 39;
            --
            if (nvl(vn_vl_cont_nc_rec,0) > 0) or
               (nvl(vn_vl_cont_cum_rec,0) > 0) then
               --
               vn_fase := 40;
               --
               vn_dia_pagto                := null;
               vn_qtde_mes_subseq          := null;
               vn_tiporetimp_id_pis_cum    := null;
               vn_tiporetimprec_id_pis_cum := null;
               vn_tiporetimp_id_pis_nc     := null;
               vn_tiporetimprec_id_pis_nc  := null;
               vd_dt_vencto                := null;
               --
               vn_fase := 41;
               --
               open c_param;
               fetch c_param into vn_dia_pagto
                                , vn_qtde_mes_subseq
                                , vn_tiporetimp_id_pis_cum
                                , vn_tiporetimprec_id_pis_cum
                                , vn_tiporetimp_id_pis_nc
                                , vn_tiporetimprec_id_pis_nc;
               close c_param;
               --
               vn_fase := 42;
               vn_dia := nvl(to_number(to_char(gt_row_per_cons_contr_pis.dt_ini,'dd')),0);
               vn_mes := nvl(to_number(to_char(gt_row_per_cons_contr_pis.dt_ini,'mm')),0);
               vn_ano := nvl(to_number(to_char(gt_row_per_cons_contr_pis.dt_ini,'rrrr')),0);
               --
               loop
                  --
                  vn_fase := 43;
                  vn_mes  := vn_mes + nvl(vn_qtde_mes_subseq,0);
                  vn_qtde_mes_subseq := 0; -- usar o valor somente na primeira vez que entrar no loop
                  --
                  if vn_mes > 12 then
                     vn_mes := vn_mes - 12;
                     vn_ano := vn_ano + 1;
                  else
                     exit;
                  end if;
                  --

               end loop;
               --
               vn_fase := 44;
               --
               begin
                  select to_date((lpad(nvl(vn_dia_pagto,vn_dia),2,'0')||lpad(vn_mes,2,'0')||vn_ano),'ddmmrrrr')
                    into vd_dt_vencto
                    from dual;
               exception
                  when others then
                     gv_resumo_log := 'Verifique a data de vencimento ('||lpad(nvl(vn_dia_pagto,vn_dia),2,'0')||'/'||lpad(vn_mes,2,'0')||'/'||vn_ano||
                                      ') para pagamentos - Bloco M205 - de acordo com os parâmetros indicados. Confirme dia/mês/ano. Erro = '||sqlerrm;
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                      , ev_mensagem        => gv_mensagem_log
                                                      , ev_resumo          => gv_resumo_log
                                                      , en_tipo_log        => erro_de_sistema
                                                      , en_referencia_id   => gn_referencia_id
                                                      , ev_obj_referencia  => gv_obj_referencia );
               end;
               --
               vn_fase := 45;
               --
               if vd_dt_vencto is not null then
                  --
                  vn_fase := 46;
                  --
                  if nvl(vn_vl_cont_nc_rec,0) > 0 and
                     nvl(vn_tiporetimp_id_pis_nc,0) > 0 and
                     nvl(vn_tiporetimprec_id_pis_nc,0) > 0 then
                     --
                     vn_fase := 47;
                     --
                     begin
                        insert into cons_contr_pis_or( id
                                                     , conscontrpis_id
                                                     , dt_vencto
                                                     , vl_rec
                                                     , tiporetimp_id
                                                     , tiporetimpreceita_id
                                                     , dm_num_campo
                                                     , dm_origem
                                                     )
                                               values( conscontrpisor_seq.nextval -- id
                                                     , gt_row_cons_contr_pis.id -- conscontrpis_id
                                                     , vd_dt_vencto -- dt_vencto
                                                     , nvl(vn_vl_cont_nc_rec,0) -- vl_rec
                                                     , vn_tiporetimp_id_pis_nc -- tiporetimp_id
                                                     , vn_tiporetimprec_id_pis_nc -- tiporetimpreceita_id
                                                     , '08' -- dm_num_campo
                                                     , 1 -- gerado no bloco m200
                                                     );
                     exception
                        when others then
                           gv_resumo_log := 'Problemas ao incluir pagamentos - valor não-cumulativo - Bloco M205 (pk_apur_pis.pkb_calcular_cons_pis_m200). '||
                                            'Erro = '||sqlerrm;
                           pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                            , ev_mensagem        => gv_mensagem_log
                                                            , ev_resumo          => gv_resumo_log
                                                            , en_tipo_log        => erro_de_sistema
                                                            , en_referencia_id   => gn_referencia_id
                                                            , ev_obj_referencia  => gv_obj_referencia );
                     end;
                     --
                  end if;
                  --
                  vn_fase := 48;
                  --
                  if nvl(vn_vl_cont_cum_rec,0) > 0 and
                     nvl(vn_tiporetimp_id_pis_cum,0) > 0 and
                     nvl(vn_tiporetimprec_id_pis_cum,0) > 0 then
                     --
                     vn_fase := 49;
                     --
                     begin
                        insert into cons_contr_pis_or( id
                                                     , conscontrpis_id
                                                     , dt_vencto
                                                     , vl_rec
                                                     , tiporetimp_id
                                                     , tiporetimpreceita_id
                                                     , dm_num_campo
                                                     , dm_origem
                                                     )
                                               values( conscontrpisor_seq.nextval -- id
                                                     , gt_row_cons_contr_pis.id -- conscontrpis_id
                                                     , vd_dt_vencto -- dt_vencto
                                                     , nvl(vn_vl_cont_cum_rec,0) -- vl_rec
                                                     , vn_tiporetimp_id_pis_cum -- tiporetimp_id
                                                     , vn_tiporetimprec_id_pis_cum -- tiporetimpreceita_id
                                                     , '12' -- dm_num_campo
                                                     , 1 -- gerado no bloco m200
                                                     );
                     exception
                        when others then
                           gv_resumo_log := 'Problemas ao incluir pagamentos - valor cumulativo - Bloco M205 (pk_apur_pis.pkb_calcular_cons_pis_m200). '||
                                            'Erro = '||sqlerrm;
                           pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                            , ev_mensagem        => gv_mensagem_log
                                                            , ev_resumo          => gv_resumo_log
                                                            , en_tipo_log        => erro_de_sistema
                                                            , en_referencia_id   => gn_referencia_id
                                                            , ev_obj_referencia  => gv_obj_referencia );
                     end;
                     --
                  end if;
                  --
               end if; -- não possui data de vencimento e identificadores de tipo de retenção
               --
            end if; -- não possui valor cumulativo ou não-cumulativo
            --
            vn_fase := 50;
            --| Atualizar descrição de detalhe de ajustes e informações complementares
            begin
               update det_ajust_contr_pis_apur da
                  set da.descr_aj   = ltrim(rtrim(pk_csf.fkg_converte(da.descr_aj)))
                    , da.info_compl = ltrim(rtrim(pk_csf.fkg_converte(da.info_compl)))
                where da.ajustcontrpisapur_id in (select ac.id
                                                    from ajust_contr_pis_apur ac
                                                       , det_cons_contr_pis   dc
                                                   where dc.conscontrpis_id    = gt_row_cons_contr_pis.id
                                                     and ac.detconscontrpis_id = dc.id);
            exception
               when others then
                  gv_resumo_log := 'Problemas ao atualizar descrição de detalhe de ajuste e informações complementares. Erro = '||sqlerrm;
                  pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                                   , ev_mensagem       => gv_mensagem_log
                                                   , ev_resumo         => gv_resumo_log
                                                   , en_tipo_log       => erro_de_sistema
                                                   , en_referencia_id  => gn_referencia_id
                                                   , ev_obj_referencia => gv_obj_referencia );
            end;
            --
         else
            --
            vn_fase := 51;
            gv_resumo_log := 'Para o período de consolidação informado não foi encontrado o mesmo período de Geração de EFD-PIS/COFINS, ou o período de '||
                             'geração está com situação: "Erro de validação", "Gerado arquivo", "Erro na geração do arquivo", "Erro de cálculo", '||
                             '"Em geração", portanto, a consolidação não será realizada. Favor atualizar a Geração de EFD-PIS/COFINS.';
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                             , ev_mensagem        => gv_mensagem_log
                                             , ev_resumo          => gv_resumo_log
                                             , en_tipo_log        => erro_de_validacao
                                             , en_referencia_id   => gn_referencia_id
                                             , ev_obj_referencia  => gv_obj_referencia );
            --
         end if;
         --
      end if;
      --
   end if;
   --
   vn_fase := 52;
   --
   if nvl(vv_calculou_1100,'N') = 'N' then 
      --   
      vn_vl_totcreddescant_1100 := 0; 
      -- 
      -- Gera registro de relacionamento para 1100 com saldo e sem movimentação no mes para o M200   
      pkb_calcula_bloco_1100( en_saldo_m200     => 0,
                              en_ver_vlr_zerado => 0,
                              sn_sld_efd_1100   => vn_vl_totcreddescant_1100 );
      --
   end if;
   --    
   if gv_resumo_log is null then
      --
      vn_fase := 53;
      --
      gv_resumo_log := 'Término do processo (identificador da consolidação = '||en_conscontrpis_id||').';
      pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                       , ev_mensagem       => gv_mensagem_log
                                       , ev_resumo         => gv_resumo_log
                                       , en_tipo_log       => erro_inform_geral
                                       , en_referencia_id  => gn_referencia_id
                                       , ev_obj_referencia => gv_obj_referencia );
      --
   else
      --
      vn_fase := 54;
      --
      begin
         update cons_contr_pis cc
            set cc.dm_situacao = 2 -- Erro no Cálculo
          where cc.id = en_conscontrpis_id;
      exception
         when others then
            gv_resumo_log := 'Problemas ao atualizar situação da consolidação para 2(Erro no cálculo). Erro = '||sqlerrm;
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                             , ev_mensagem        => gv_mensagem_log
                                             , ev_resumo          => gv_resumo_log
                                             , en_tipo_log        => erro_de_sistema
                                             , en_referencia_id   => en_conscontrpis_id
                                             , ev_obj_referencia  => gv_obj_referencia );
      end;
      --
   end if;
   --
   vn_fase := 55;
   --
   commit;
   --
EXCEPTION
   when others then
      --
      begin
         update cons_contr_pis cc
            set cc.dm_situacao = 2 -- Erro no Cálculo
          where cc.id = en_conscontrpis_id;
      exception
         when others then
            gv_resumo_log := 'Problemas ao atualizar situação da consolidação para 2(Erro no cálculo). Erro = '||sqlerrm;
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                             , ev_mensagem        => gv_mensagem_log
                                             , ev_resumo          => gv_resumo_log
                                             , en_tipo_log        => erro_de_sistema
                                             , en_referencia_id   => en_conscontrpis_id
                                             , ev_obj_referencia  => gv_obj_referencia );
      end;
      --
      commit;
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.pkb_calcular_cons_pis_m200 (fase = '||vn_fase||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => en_conscontrpis_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
END PKB_CALCULAR_CONS_PIS_M200;
----------------------------------------------------------------------------------------------
--| Procedimento para calcular por período a consolidação da contribuição do PIS - Bloco M200
--| Serão calculados alguns campos dos blocos M200, M210 e M220
----------------------------------------------------------------------------------------------
PROCEDURE PKB_CALC_PER_CONS_PIS_M200( EN_PERCONSCONTRPIS_ID IN PER_CONS_CONTR_PIS.ID%TYPE ) IS
   --
   vn_fase           number := 0;
   vn_loggenerico_id log_generico.id%type;
   --
   cursor c_per_cons_contr_pis( en_perconscontrpis_id in per_cons_contr_pis.id%type ) is
      select *
        from per_cons_contr_pis pc
       where pc.id  = en_perconscontrpis_id;
   --
   cursor c_cons is
      select ac.id conscontrpis_id
        from cons_contr_pis ac
       where ac.perconscontrpis_id = en_perconscontrpis_id;
BEGIN
   --
   vn_fase := 1;
   --
   pkb_excluir_log( en_referencia_id  => en_perconscontrpis_id
                  , ev_obj_referencia => 'PER_CONS_CONTR_PIS' );
   --
   vn_fase := 2;
   --
   gv_mensagem_log   := 'Cálculo por período das consolidações de contribuição do imposto PIS';
   gv_obj_referencia := 'PER_CONS_CONTR_PIS';
   gv_resumo_log     := 'Início do processo (identificador do período = '||en_perconscontrpis_id||').';
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => en_perconscontrpis_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
   gv_resumo_log := null;
   --
   vn_fase := 3;
   --
   if nvl(en_perconscontrpis_id,0) = 0 then
      vn_fase := 4;
      gv_resumo_log := 'O identificador do período de consolidação deve ser informado para que o cálculo seja efetuado.';
      pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                       , ev_mensagem        => gv_mensagem_log
                                       , ev_resumo          => gv_resumo_log
                                       , en_tipo_log        => erro_de_validacao
                                       , en_referencia_id   => null
                                       , ev_obj_referencia  => gv_obj_referencia );
   else
      --
      vn_fase := 5;
      --
      if pk_csf_efd_pc.fkg_qtde_cons_pis( en_perconscontrpis_id => en_perconscontrpis_id ) = 0 then -- devem existir consolidações geradas
         vn_fase := 6;
         gv_resumo_log := 'O período de consolidação informado não possui consolidações geradas, portanto, o cálculo não será realizado.';
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_validacao
                                          , en_referencia_id   => en_perconscontrpis_id
                                          , ev_obj_referencia  => gv_obj_referencia );
         --
      else
         --
         vn_fase := 7;
         --
         gt_row_per_cons_contr_pis := null;
         open c_per_cons_contr_pis( en_perconscontrpis_id => en_perconscontrpis_id );
         fetch c_per_cons_contr_pis into gt_row_per_cons_contr_pis;
         close c_per_cons_contr_pis;
         --
         vn_fase := 8;
         --
         if pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => gt_row_per_cons_contr_pis.empresa_id
                                                  , ed_data       => gt_row_per_cons_contr_pis.dt_ini ) or  -- true
            pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => gt_row_per_cons_contr_pis.empresa_id
                                                  , ed_data       => gt_row_per_cons_contr_pis.dt_fin ) then -- true
            --
            vn_fase := 9;
            --
            gv_resumo_log := 'Existe(m) período(s) de abertura efd pis/cofins com situação: Validado ou Arquivo Gerado. Desprocesse a geração do arquivo.';
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                             , ev_mensagem        => gv_mensagem_log
                                             , ev_resumo          => gv_resumo_log
                                             , en_tipo_log        => erro_de_validacao
                                             , en_referencia_id   => gt_row_per_cons_contr_pis.id
                                             , ev_obj_referencia  => gv_obj_referencia );
            --
         else
            --
            vn_fase := 10;
            --
            for r_reg in c_cons
            loop
               --
               vn_fase := 11;
               --
               pkb_calcular_cons_pis_m200( en_conscontrpis_id => r_reg.conscontrpis_id );
               --
            end loop;
            --
         end if;
         --
      end if;
      --
   end if;
   --
   vn_fase := 12;
   --
   commit;
   --
   vn_fase := 13;
   --
   gv_resumo_log := 'Término do processo (identificador do período = '||en_perconscontrpis_id||').';
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => en_perconscontrpis_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
EXCEPTION
   when others then
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.pkb_calc_per_cons_pis_m200 (fase = '||vn_fase||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => 'Calcular por período a consolidação da contribuição do PIS'
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => en_perconscontrpis_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
END PKB_CALC_PER_CONS_PIS_M200;
---------------------------------------------------------------------------
--| Procedimento para validar a consolidação do PIS - Bloco M200
--| Serão validados alguns campos dos blocos M200, M210, M211, M220 e M230
---------------------------------------------------------------------------
PROCEDURE PKB_VALIDAR_CONS_PIS_M200( EN_CONSCONTRPIS_ID IN CONS_CONTR_PIS.ID%TYPE ) IS
   --
   vn_fase              number := 0;
   vn_loggenerico_id    log_generico.id%type := null;
   vn_valor             number := 0;
   vn_vl_tot_cred_desc  number := 0;
   vn_vl_rec            cons_contr_pis_or.vl_rec%type;
   vn_conscontrpisor_id cons_contr_pis_or.id%type;
   vn_det_valor_aj      number := 0;
   --
   cursor c_per_cons_contr_pis( en_perconscontrpis_id in per_cons_contr_pis.id%type ) is
      select *
        from per_cons_contr_pis pc
       where pc.id  = en_perconscontrpis_id;
   --
   cursor c_det_cons is
      select dc.id detconscontrpis_id
           , dc.contrsocapurpc_id
           , dc.vl_rec_brt
           , dc.vl_bc_cont
           , dc.vl_bc_cont_ajus
           , dc.aliq_pis
           , dc.quant_bc_pis
           , dc.vl_aliq_pis_quant
           , dc.vl_cont_apur
           , dc.vl_ajust_acrec
           , dc.vl_ajust_reduc
           , dc.vl_cont_difer
           , dc.vl_cont_difer_ant
           , dc.vl_cont_per
        from det_cons_contr_pis dc
       where dc.conscontrpis_id = gt_row_cons_contr_pis.id;
   --
   cursor c_vl_aj( en_detconscontrpis_id in number
                 , en_ind_aj             in number ) is
      select nvl(sum(nvl(ac.vl_aj,0)),0) vl_aj
        from ajust_contr_pis_apur ac
       where ac.detconscontrpis_id = en_detconscontrpis_id
         and ac.dm_ind_aj          = en_ind_aj; -- 0-ajuste de redução, 1-ajuste de acréscimo
   --
   cursor c_vl_difer( en_detconscontrpis_id in number ) is
      select nvl(sum(nvl(ia.vl_cont_dif,0)),0)
        from inf_adic_dif_pis ia
       where ia.detconscontrpis_id = en_detconscontrpis_id;
   --
   cursor c_vl_difer_ant( en_contrsocapurpc_id in number ) is
      select nvl(sum(nvl(cp.vl_cont_difer_ant,0)),0)
        from contr_pis_dif_per_ant cp
       where cp.empresa_id        = gt_row_per_cons_contr_pis.empresa_id
         and cp.dt_ini      between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
         and cp.dt_fin      between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
         and cp.contrsocapurpc_id = en_contrsocapurpc_id
         and cp.dm_situacao       = 3; -- Processada
   --
   cursor c_vl_tot_cont_per is
      select nvl(sum(nvl(dc.vl_cont_per,0)),0)
        from det_cons_contr_pis dc
       where dc.conscontrpis_id = gt_row_cons_contr_pis.id
         and ((gn_dm_cod_inc_trib in (1, 3) and -- 1-regime não-cumulativo, 3-regime cumulativo e não-cumulativo
               pk_csf_efd_pc.fkg_cd_contr_soc_apur_pc ( dc.contrsocapurpc_id ) in ('01', '02', '03', '04', '32'))
               or
              (gn_dm_cod_inc_trib = 1 and -- 1-regime não-cumulativo
               pk_csf_efd_pc.fkg_cd_contr_soc_apur_pc ( dc.contrsocapurpc_id ) in ('01', '02', '03', '04', '32', '71')));
   --
   cursor c_cons_tot_cred is
      select nvl(sum(nvl(cc.vl_tot_cred_desc,0)),0) vl_tot_cred_desc
        from cons_contr_pis cc
       where cc.perconscontrpis_id = gt_row_cons_contr_pis.perconscontrpis_id;
   --
   cursor c_vl_tot_cred is
      select nvl(sum(nvl(ac.vl_cred_desc,0)),0)
        from apur_cred_pis     ac
           , per_apur_cred_pis pa
       where pa.empresa_id        = gt_row_per_cons_contr_pis.empresa_id
         and pa.dt_ini      between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
         and pa.dt_fin      between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
         and ac.perapurcredpis_id = pa.id
         and ac.dm_situacao       = 3; -- 3-processada
   --
   cursor c_vl_cred_desc is
      select nvl(sum(nvl(ra.para_vl_cred_desc_efd,0)),0)  --nvl(sum(nvl(cc.vl_cred_desc_efd,0)),0)
        from contr_cred_fiscal_pis cc
           , relac_apur_contr_pis  ra		
       where cc.empresa_id            = gt_row_per_cons_contr_pis.empresa_id
         and to_date(('01'||lpad(cc.mes,2,'0')||cc.ano),'ddmmrrrr') < gt_row_per_cons_contr_pis.dt_ini
         and cc.dm_situacao           = 3 -- Validado
         and cc.dm_orig_cred          = '01'
         and ra.contrcredfiscalpis_id = cc.id
         and ra.ano                   = to_char(gt_row_per_cons_contr_pis.dt_ini,'rrrr')
         and ra.mes                   = to_char(gt_row_per_cons_contr_pis.dt_ini,'mm');		 
   --
   -- Controle de valores retidos da fonte - Bloco F600, mais, Controle dos valores retidos na fonte de pis - 1300, Processo não-cumulativo
   cursor c_vl_ret_apu_nc is
   select nvl(sum(nvl(sm.vl_ret_apu,0)),0) vl_ret_apu
     from (
      select nvl(sum(nvl(cr.vl_ret_pis,0)),0) vl_ret_apu
        from contr_ret_fonte_pc cr
       where cr.empresa_id in (select em.id empresa_id -- recuperar a matriz e as filiais
                                 from empresa em
                                start with em.id = gt_row_per_cons_contr_pis.empresa_id
                              connect by prior em.id = em.ar_empresa_id)
         and cr.dm_st_proc     = 1 -- validada
         and cr.dm_ind_nat_rec = 0 -- 0-Receita de Natureza Não-Cumulativa, 1-Receita de Natureza Cumulativa
         and cr.dt_ret between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
      union
      select nvl(sum(nvl(cv.vl_ret_ded,0)),0) vl_ret_ded
        from contr_vlr_ret_fonte_pis cv -- dados do Bloco 1300
       where cv.empresa_id in (select em.id empresa_id -- recuperar a matriz e as filiais
                                 from empresa em
                                start with em.id = gt_row_per_cons_contr_pis.empresa_id
                              connect by prior em.id = em.ar_empresa_id)
         and cv.dm_situacao = 3 -- validada
         and cv.dm_origem   = 0 -- digitado
         and cv.ano         = to_char(gt_row_per_cons_contr_pis.dt_ini,'rrrr')
         and cv.mes         = to_char(gt_row_per_cons_contr_pis.dt_ini,'mm')
         and cv.dm_ind_nat_ret in ('01','02','03','04','05','99') -- Receita de Natureza Não-Cumulativa
          ) sm;
   --
   -- Controle de valores retidos da fonte - Bloco F600, mais, Controle dos valores retidos na fonte de pis - 1300, Processo cumulativo
   cursor c_vl_ret_apu_cum is
   select nvl(sum(nvl(sm.vl_ret_apu,0)),0) vl_ret_apu
     from (
      select nvl(sum(nvl(cr.vl_ret_pis,0)),0) vl_ret_apu
        from contr_ret_fonte_pc cr
       where cr.empresa_id in (select em.id empresa_id -- recuperar a matriz e as filiais
                                 from empresa em
                                start with em.id = gt_row_per_cons_contr_pis.empresa_id
                              connect by prior em.id = em.ar_empresa_id)
         and cr.dm_st_proc     = 1 -- validada
         and cr.dm_ind_nat_rec = 1 -- 0-Receita de Natureza Não-Cumulativa, 1-Receita de Natureza Cumulativa
         and cr.dt_ret between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
      union
      select nvl(sum(nvl(cv.vl_ret_ded,0)),0) vl_ret_ded
        from contr_vlr_ret_fonte_pis cv -- dados do Bloco 1300
       where cv.empresa_id in (select em.id empresa_id -- recuperar a matriz e as filiais
                                 from empresa em
                                start with em.id = gt_row_per_cons_contr_pis.empresa_id
                              connect by prior em.id = em.ar_empresa_id)
         and cv.dm_situacao = 3 -- validada
         and cv.dm_origem   = 0 -- digitado
         and cv.ano         = to_char(gt_row_per_cons_contr_pis.dt_ini,'rrrr')
         and cv.mes         = to_char(gt_row_per_cons_contr_pis.dt_ini,'mm')
         and cv.dm_ind_nat_ret in ('51','52','53','54','55','59') -- Receita de Natureza Cumulativa
          ) sm;
   --
   cursor c_vl_ded_pis( en_ind_nat_ded in number ) is
      select nvl(sum(nvl(dd.vl_ded_pis,0)),0)
        from deducao_diversa_pc dd
       where dd.empresa_id in (select em.id empresa_id -- recuperar a matriz e as filiais
                                 from empresa em
                                start with em.id = gt_row_per_cons_contr_pis.empresa_id
                              connect by prior em.id = em.ar_empresa_id)
         and to_date(('01'||lpad(dd.mes_ref,2,'0')||dd.ano_ref),'ddmmrrrr') between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
         and dd.dm_ind_nat_ded = en_ind_nat_ded -- 0-dedução de natureza não cumulativa, 1-dedução de natureza cumulativa
         and dd.dm_st_proc     = 1; -- Validada
   --
   cursor c_vl_tot_cont_cum_per is
      select nvl(sum(nvl(dc.vl_cont_per,0)),0)
        from det_cons_contr_pis dc
       where dc.conscontrpis_id = gt_row_cons_contr_pis.id
         and ((gn_dm_cod_inc_trib = 2 and -- 2-regime cumulativo
               pk_csf_efd_pc.fkg_cd_contr_soc_apur_pc ( dc.contrsocapurpc_id ) in ('31', '32'))
               or
              (gn_dm_cod_inc_trib in (2,3) and -- 2-regime cumulativo e 3-regime cumulativo e não-cumulativo
               pk_csf_efd_pc.fkg_cd_contr_soc_apur_pc ( dc.contrsocapurpc_id ) in ('51', '52', '53', '54', '72')));
   --
   cursor c_soc_coop( en_detconscontrpis_id in number) is
      select sc.id soccoopcompbccalcpis_id
           , sc.dm_ind_tip_coop
           , sc.vl_bc_cont_ant_exc_coop
           , sc.vl_exc_coop_ger
           , sc.vl_exc_esp_coop
           , sc.vl_bc_cont
        from soc_coop_comp_bc_calc_pis sc
       where sc.detconscontrpis_id = en_detconscontrpis_id;
   --
   cursor c_ajuste( en_detconscontrpis_id in number) is
      select ac.id
           , ac.dm_ind_aj
           , ac.vl_aj
           , ac.dt_ref
        from ajust_contr_pis_apur ac
       where ac.detconscontrpis_id = en_detconscontrpis_id;
   --
   cursor c_det_aju( en_ajustcontrpisapur_id in number) is
      select nvl(sum(nvl(da.det_valor_aj,0)),0) det_valor_aj
        from det_ajust_contr_pis_apur da
       where da.ajustcontrpisapur_id = en_ajustcontrpisapur_id;
   --
   cursor c_inf_adic( en_detconscontrpis_id in number) is
      select ia.id
           , ia.vl_vend
           , ia.vl_nao_receb
           , ia.vl_cont_dif
           , ia.vl_cred_dif
        from inf_adic_dif_pis ia
       where ia.detconscontrpis_id = en_detconscontrpis_id;
   --
   cursor c_pagtos_cum is
      select nvl(sum(nvl(cc.vl_rec,0)),0) vl_rec
        from cons_contr_pis_or cc
       where cc.conscontrpis_id = gt_row_cons_contr_pis.id
         and cc.dm_num_campo    = '12';
   --
   cursor c_pagtos_ncum is
      select nvl(sum(nvl(cc.vl_rec,0)),0) vl_rec
        from cons_contr_pis_or cc
       where cc.conscontrpis_id = gt_row_cons_contr_pis.id
         and cc.dm_num_campo    = '08';
   --
   cursor c_pagtos_numcampo is
      select cc.id conscontrpisor_id
        from cons_contr_pis_or cc
       where cc.conscontrpis_id = gt_row_cons_contr_pis.id
         and cc.dm_num_campo not in ('08','12');
   --
   cursor c_pagtos_origem is
      select cc.id conscontrpisor_id
        from cons_contr_pis_or cc
       where cc.conscontrpis_id = gt_row_cons_contr_pis.id
         and cc.dm_origem not in (0,1);
   --
   cursor c_pagtos_tpret is
      select cc.id conscontrpisor_id
        from cons_contr_pis_or cc
           , tipo_ret_imp      tr
           , tipo_imposto      ti
       where cc.conscontrpis_id = gt_row_cons_contr_pis.id
         and tr.id              = cc.tiporetimp_id
         and ti.id              = tr.tipoimp_id
         and ti.cd             <> 4; -- imposto PIS
   --
   cursor c_pagtos_tpretrec is
      select cc.id conscontrpisor_id
        from cons_contr_pis_or    cc
           , tipo_ret_imp_receita tr
       where cc.conscontrpis_id = gt_row_cons_contr_pis.id
         and tr.id              = cc.tiporetimpreceita_id
         and tr.tiporetimp_id  <> cc.tiporetimp_id;
   --
BEGIN
   --
   vn_fase := 1;
   --
   pkb_excluir_log( en_referencia_id  => en_conscontrpis_id
                  , ev_obj_referencia => 'CONS_CONTR_PIS' );
   --
   vn_fase := 2;
   --
   gv_obj_referencia := 'CONS_CONTR_PIS';
   gv_mensagem_log   := 'Validação da consolidação do imposto PIS/PASEP';
   gv_resumo_log     := 'Início do processo (identificador da consolidação = '||en_conscontrpis_id||').';
   gn_referencia_id  := en_conscontrpis_id;
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => gn_referencia_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
   gv_resumo_log := null;
   --
   vn_fase := 3;
   --
   if nvl(en_conscontrpis_id,0) = 0 then
      --
      vn_fase := 4;
      gv_resumo_log := 'O identificador da consolidação deve ser informado para que a validação seja efetuada.';
      pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                       , ev_mensagem        => gv_mensagem_log
                                       , ev_resumo          => gv_resumo_log
                                       , en_tipo_log        => erro_de_validacao
                                       , en_referencia_id   => null
                                       , ev_obj_referencia  => gv_obj_referencia );
      --
   else
      --
      vn_fase := 5;
      -- Recupera os dados da consolidação do imposto pis utilizado como referência
      pkb_dados_cons_pis_m200( en_conscontrpis_id => en_conscontrpis_id );
      gv_mensagem_log := 'Validação da '||gv_mensagem_log;
      --
      vn_fase := 6;
      --
      if gt_row_cons_contr_pis.dm_situacao <> 1 then -- não estiver calculada
         --
         vn_fase := 7;
         gv_resumo_log := 'A consolidação informada não está com situação calculada, portanto, a validação não será realizada.';
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_validacao
                                          , en_referencia_id   => gn_referencia_id
                                          , ev_obj_referencia  => gv_obj_referencia );
         --
      else
         --
         vn_fase := 8;
         --
         if nvl(gt_row_per_cons_contr_pis.id,0) = 0 then
            --
            vn_fase := 9;
            gt_row_per_cons_contr_pis := null;
            open c_per_cons_contr_pis( en_perconscontrpis_id => gt_row_cons_contr_pis.perconscontrpis_id );
            fetch c_per_cons_contr_pis into gt_row_per_cons_contr_pis;
            close c_per_cons_contr_pis;
            --
         end if;
         --
         vn_fase := 10;
         --
         if pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => gt_row_per_cons_contr_pis.empresa_id
                                                  , ed_data       => gt_row_per_cons_contr_pis.dt_ini ) or  -- true
            pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => gt_row_per_cons_contr_pis.empresa_id
                                                  , ed_data       => gt_row_per_cons_contr_pis.dt_fin ) then -- true
            --
            vn_fase := 11;
            --
            gv_resumo_log := 'Existe(m) período(s) de abertura efd pis/cofins com situação: Validado ou Arquivo Gerado. Desprocesse a geração do arquivo.';
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                             , ev_mensagem        => gv_mensagem_log
                                             , ev_resumo          => gv_resumo_log
                                             , en_tipo_log        => erro_de_validacao
                                             , en_referencia_id   => gn_referencia_id
                                             , ev_obj_referencia  => gv_obj_referencia );
            --
         else
            --
            vn_fase := 12;
            --| Validar os campos do Bloco M210 - Detalhe da consolidação
            for r_reg in c_det_cons
            loop
               --
               vn_fase := 13;
               --
               if nvl(r_reg.vl_rec_brt,0) < 0 then
                  gv_resumo_log := 'No detalhe da consolidação, o valor da receita bruta ('||
                                   ltrim(to_char(nvl(r_reg.vl_rec_brt,0),'999G999G999G999G990D00'))||') não pode ser negativo.';
                  pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                                  , ev_mensagem        => gv_mensagem_log
                                                  , ev_resumo          => gv_resumo_log
