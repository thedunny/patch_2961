--| Procedimento para armazenar os dados em vetores - Bloco M400
-----------------------------------------------------------------
PROCEDURE PKB_MONTA_VETOR_M400( EN_CH_REC_CSTPLACTA IN NUMBER
                              , EN_CODST_ID         IN NUMBER
                              , EN_PLANOCONTA_ID    IN NUMBER
                              , EN_NATRECPC_ID      IN NUMBER
                              , EN_VL_REC           IN NUMBER
                              , EN_NCM_ID           IN NUMBER ) IS
   --
   vn_fase            number := 0;
   vb_achou           boolean := false;
   vn_recisentapis_id number := 0;
   --
   vn_planoconta_id   plano_conta.id%type;
   --
BEGIN
   --
   vn_fase := 1;
   --| Montar bloco M400
   begin
      vb_achou := vt_tab_reg_m400.exists(en_ch_rec_cstplacta);
   exception
      when others then
         vb_achou := false;
   end;
   --
   vn_fase := 2;
   --
   if not vb_achou then
      --
      vn_fase := 3;
      --
      begin
         select recisentapis_seq.nextval
           into vn_recisentapis_id
           from dual;
      exception
         when others then
            vn_recisentapis_id := null;
      end;
      --
      vn_fase := 4;
      --
      if nvl(vn_recisentapis_id,0) > 0 then
         --
         vn_fase := 5;
         --
         vt_tab_reg_m400(en_ch_rec_cstplacta).recisentapis_id := vn_recisentapis_id;
         vt_tab_reg_m400(en_ch_rec_cstplacta).dm_situacao     := 0; -- 0-aberto
         vt_tab_reg_m400(en_ch_rec_cstplacta).codst_id        := en_codst_id;
         vt_tab_reg_m400(en_ch_rec_cstplacta).vl_tot_rec      := en_vl_rec;
         vt_tab_reg_m400(en_ch_rec_cstplacta).planoconta_id   := en_planoconta_id;
         vt_tab_reg_m400(en_ch_rec_cstplacta).desc_compl      := null; -- processo informado/digitado
         --
      end if;
      --
   else
      --
      vn_fase := 6;
      --
      vt_tab_reg_m400(en_ch_rec_cstplacta).vl_tot_rec := nvl(vt_tab_reg_m400(en_ch_rec_cstplacta).vl_tot_rec,0) + nvl(en_vl_rec,0);
      --
   end if;
   --
   vn_fase := 7;
   --
   vn_recisentapis_id := vt_tab_reg_m400(en_ch_rec_cstplacta).recisentapis_id;
   --
   vn_fase := 8;
   --| Montar bloco M410
   begin
      vb_achou := vt_tri_tab_reg_m410(vn_recisentapis_id)(en_ch_rec_cstplacta).exists(en_natrecpc_id);
   exception
      when others then
         vb_achou := false;
   end;
   --
   vn_fase := 9;
   --
   if not vb_achou then
      --
      vn_fase := 10;
      --
      begin
         select detrecisentapis_seq.nextval
           into vt_tri_tab_reg_m410(vn_recisentapis_id)(en_ch_rec_cstplacta)(en_natrecpc_id).detrecisentapis_id
           from dual;
      exception
         when others then
            vt_tri_tab_reg_m410(vn_recisentapis_id)(en_ch_rec_cstplacta)(en_natrecpc_id).detrecisentapis_id := null;
      end;
      --
      vn_fase := 11;
      --
      if nvl(vt_tri_tab_reg_m410(vn_recisentapis_id)(en_ch_rec_cstplacta)(en_natrecpc_id).detrecisentapis_id,0) > 0 then
         --
         vn_fase := 12;
         --
         vn_planoconta_id:= pk_csf.fkg_ncmnatrecpc_plc_id(en_natrecpc_id => en_natrecpc_id,en_ncm_id => en_ncm_id);
         --
         vn_fase := 12.1;
         --
         vt_tri_tab_reg_m410(vn_recisentapis_id)(en_ch_rec_cstplacta)(en_natrecpc_id).recisentapis_id := vn_recisentapis_id;
         vt_tri_tab_reg_m410(vn_recisentapis_id)(en_ch_rec_cstplacta)(en_natrecpc_id).planoconta_id   := nvl(vn_planoconta_id,en_planoconta_id);
         vt_tri_tab_reg_m410(vn_recisentapis_id)(en_ch_rec_cstplacta)(en_natrecpc_id).natrecpc_id     := en_natrecpc_id;
         vt_tri_tab_reg_m410(vn_recisentapis_id)(en_ch_rec_cstplacta)(en_natrecpc_id).vl_rec          := en_vl_rec;
         vt_tri_tab_reg_m410(vn_recisentapis_id)(en_ch_rec_cstplacta)(en_natrecpc_id).desc_compl      := null;
         --
      end if;
   else
      --
      vn_fase := 13;
      --
      vt_tri_tab_reg_m410(vn_recisentapis_id)(en_ch_rec_cstplacta)(en_natrecpc_id).vl_rec := nvl(vt_tri_tab_reg_m410(vn_recisentapis_id)(en_ch_rec_cstplacta)(en_natrecpc_id).vl_rec,0) +
                                                                                             nvl(en_vl_rec,0);
      --
   end if;
   --
EXCEPTION
   when others then
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.pkb_monta_vetor_m400 (fase = '||vn_fase||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => gn_referencia_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
END PKB_MONTA_VETOR_M400;
