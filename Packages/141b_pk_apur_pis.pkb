--| Procedimento para armazenar os dados em vetores - Bloco M100
-----------------------------------------------------------------
PROCEDURE PKB_MONTA_VETOR_M100( EN_CH_CC_TPCR_OR_TP_ALIQ IN NUMBER
                              , EN_TIPOCREDPC_ID         IN NUMBER
                              , EN_DM_IND_CRED_ORI       IN NUMBER
                              , EN_TIPO                  IN NUMBER
                              , EN_ALIQ_PIS              IN NUMBER
                              , EN_VL_ALIQ_PIS_QUANT     IN NUMBER
                              , EN_VL_BC_PIS             IN NUMBER
                              , EN_QUANT_BC_PIS          IN NUMBER
                              , EN_BASECALCCREDPC_ID     IN NUMBER
                              , EN_CODST_ID              IN NUMBER
                              , EN_VL_BC_PIS_TOT         IN NUMBER
                              , EN_VL_BC_PIS_CUM         IN NUMBER
                              , EN_VL_BC_PIS_NC          IN NUMBER
                              , EN_QUANT_BC_PIS_TOT      IN NUMBER ) IS
   --
   vn_fase               number := 0;
   vb_achou              boolean := false;
   vn_apurcredpis_id     apur_cred_pis.id%type;
   vn_ch_cc_apur_bs_cdst varchar2(20); -- number := 0;
   --
BEGIN
   --
   vn_fase := 1;
   --| Montar bloco M100 - Alterado o indexador para tipo caracter com tamanho de 20
   begin
      vb_achou := vt_tab_reg_m100.exists(en_ch_cc_tpcr_or_tp_aliq);
   exception
      when others then
         vb_achou := false;
   end;
   --
   vn_fase := 2;
   --
   if not vb_achou then
      --
      vn_fase := 3;
      --
      begin
         select apurcredpis_seq.nextval
           into vt_tab_reg_m100(en_ch_cc_tpcr_or_tp_aliq).apurcredpis_id
           from dual;
      exception
         when others then
            vt_tab_reg_m100(en_ch_cc_tpcr_or_tp_aliq).apurcredpis_id := null;
      end;
      --
      vn_fase := 4;
      --
      if nvl(vt_tab_reg_m100(en_ch_cc_tpcr_or_tp_aliq).apurcredpis_id,0) > 0 then
         --
         vn_fase := 5;
         --
         vt_tab_reg_m100(en_ch_cc_tpcr_or_tp_aliq).tipocredpc_id     := en_tipocredpc_id;
         vt_tab_reg_m100(en_ch_cc_tpcr_or_tp_aliq).dm_ind_cred_ori   := en_dm_ind_cred_ori;
         vt_tab_reg_m100(en_ch_cc_tpcr_or_tp_aliq).tipo              := en_tipo;
         vt_tab_reg_m100(en_ch_cc_tpcr_or_tp_aliq).dm_situacao       := 0; -- Aberto
         vt_tab_reg_m100(en_ch_cc_tpcr_or_tp_aliq).vl_bc_pis         := en_vl_bc_pis;
         vt_tab_reg_m100(en_ch_cc_tpcr_or_tp_aliq).aliq_pis          := en_aliq_pis;
         vt_tab_reg_m100(en_ch_cc_tpcr_or_tp_aliq).quant_bc_pis      := en_quant_bc_pis;
         vt_tab_reg_m100(en_ch_cc_tpcr_or_tp_aliq).vl_aliq_pis_quant := en_vl_aliq_pis_quant;
         vt_tab_reg_m100(en_ch_cc_tpcr_or_tp_aliq).vl_ajus_acres     := 0;
         vt_tab_reg_m100(en_ch_cc_tpcr_or_tp_aliq).vl_ajus_reduc     := 0;
         vt_tab_reg_m100(en_ch_cc_tpcr_or_tp_aliq).vl_cred_dif       := 0;
         vt_tab_reg_m100(en_ch_cc_tpcr_or_tp_aliq).vl_cred_disp      := 0;
         vt_tab_reg_m100(en_ch_cc_tpcr_or_tp_aliq).dm_ind_desc_cred  := 0;
         vt_tab_reg_m100(en_ch_cc_tpcr_or_tp_aliq).vl_cred_desc      := 0;
         vt_tab_reg_m100(en_ch_cc_tpcr_or_tp_aliq).vl_sld_cred       := 0;
         --
      end if;
      --
   else
      --
      vn_fase := 6;
      --
      vt_tab_reg_m100(en_ch_cc_tpcr_or_tp_aliq).vl_bc_pis    := vt_tab_reg_m100(en_ch_cc_tpcr_or_tp_aliq).vl_bc_pis    + en_vl_bc_pis;
      vt_tab_reg_m100(en_ch_cc_tpcr_or_tp_aliq).quant_bc_pis := vt_tab_reg_m100(en_ch_cc_tpcr_or_tp_aliq).quant_bc_pis + en_quant_bc_pis;
      --
   end if;
   --
   vn_fase := 7;
   --
   vn_apurcredpis_id := nvl(vt_tab_reg_m100(en_ch_cc_tpcr_or_tp_aliq).apurcredpis_id,0);
   --
   vn_fase := 8;
   --| Montar bloco M105
   vn_ch_cc_apur_bs_cdst := nvl(vn_apurcredpis_id,0)||nvl(en_basecalccredpc_id,0)||nvl(en_codst_id,0);
   --
   vn_fase := 9;
   --
   begin
      vb_achou := vt_tab_reg_m105.exists(vn_ch_cc_apur_bs_cdst);
   exception
      when others then
         vb_achou := false;
   end;
   --
   vn_fase := 10;
   --
   if not vb_achou then
      --
      vn_fase := 11;
      --
      begin
         select detapurcredpis_seq.nextval
           into vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).detapurcredpis_id
           from dual;
      exception
         when others then
            vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).detapurcredpis_id := null;
      end;
      --
      vn_fase := 12;
      --
      if nvl(vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).detapurcredpis_id,0) > 0 then
         --
         vn_fase := 13;
         --
         vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).apurcredpis_id    := vn_apurcredpis_id;
         vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).basecalccredpc_id := en_basecalccredpc_id;
         vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).codst_id          := en_codst_id;
         vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).vl_bc_pis_tot     := en_vl_bc_pis_tot;
         vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).vl_bc_pis_cum     := en_vl_bc_pis_cum;
         vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).vl_bc_pis_nc      := en_vl_bc_pis_nc;
         vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).vl_bc_pis         := en_vl_bc_pis;
         vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).quant_bc_pis_tot  := en_quant_bc_pis_tot;
         vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).quant_bc_pis      := en_quant_bc_pis;
         --
         if pk_csf_efd_pc.fkg_base_calc_cred_pc_cd(en_id => en_basecalccredpc_id) = '13' then
            vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).desc_cred := pk_csf_efd_pc.fkg_descr_basecalccredpc(en_basecalccredpc_id => en_basecalccredpc_id);
         else
            vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).desc_cred := null;
         end if;
         --
      end if;
      --
   else
      --
      vn_fase := 14;
      --
      vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).vl_bc_pis_tot    := vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).vl_bc_pis_tot    + en_vl_bc_pis_tot;
      vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).vl_bc_pis_cum    := vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).vl_bc_pis_cum    + en_vl_bc_pis_cum;
      vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).vl_bc_pis_nc     := vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).vl_bc_pis_nc     + en_vl_bc_pis_nc;
      vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).vl_bc_pis        := vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).vl_bc_pis        + en_vl_bc_pis;
      vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).quant_bc_pis_tot := vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).quant_bc_pis_tot + en_quant_bc_pis_tot;
      vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).quant_bc_pis     := vt_tab_reg_m105(vn_ch_cc_apur_bs_cdst).quant_bc_pis     + en_quant_bc_pis;
      --
   end if;
   --
EXCEPTION
   when others then
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.pkb_monta_vetor_m100 (fase = '||vn_fase||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => gn_referencia_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
END PKB_MONTA_VETOR_M100;
