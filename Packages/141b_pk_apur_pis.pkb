      pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                       , ev_mensagem        => gv_mensagem_log
                                       , ev_resumo          => gv_resumo_log
                                       , en_tipo_log        => erro_de_validacao
                                       , en_referencia_id   => null
                                       , ev_obj_referencia  => gv_obj_referencia );
   else
      --
      vn_fase := 5;
      --
      if pk_csf_efd_pc.fkg_qtde_per_rec_pis( en_perrecisentapis_id => en_perrecisentapis_id ) = 0 then -- devem existir receitas geradas
         vn_fase := 6;
         gv_resumo_log := 'O período de receita isenta informado não possui receitas geradas, portanto, o cálculo não será realizado.';
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_validacao
                                          , en_referencia_id   => en_perrecisentapis_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      else
         --
         vn_fase := 7;
         --
         gt_row_per_rec_isenta_pis := null;
         open c_per_rec_isenta_pis( en_perrecisentapis_id => en_perrecisentapis_id );
         fetch c_per_rec_isenta_pis into gt_row_per_rec_isenta_pis;
         close c_per_rec_isenta_pis;
         --
         vn_fase := 8;
         --
         if pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => gt_row_per_rec_isenta_pis.empresa_id
                                                  , ed_data       => gt_row_per_rec_isenta_pis.dt_ini ) or  -- true
            pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => gt_row_per_rec_isenta_pis.empresa_id
                                                  , ed_data       => gt_row_per_rec_isenta_pis.dt_fin ) then -- true
            --
            vn_fase := 9;
            --
            gv_resumo_log := 'Existe(m) período(s) de abertura efd pis/cofins com situação: Validado ou Arquivo Gerado. Desprocesse a geração do arquivo.';
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                             , ev_mensagem        => gv_mensagem_log
                                             , ev_resumo          => gv_resumo_log
                                             , en_tipo_log        => erro_de_validacao
                                             , en_referencia_id   => gt_row_per_rec_isenta_pis.id
                                             , ev_obj_referencia  => gv_obj_referencia );
            --
         else
            --
            vn_fase := 10;
            --
            for r_reg in c_rec
            loop
               --
               vn_fase := 11;
               --
               pkb_calcular_rec_pis_m400( en_recisentapis_id => r_reg.recisentapis_id );
               --
            end loop;
            --
         end if;
         --
      end if;
   end if;
   --
   vn_fase := 12;
   --
   commit;
   --
   vn_fase := 13;
   --
   gv_resumo_log := 'Término do processo (identificador do período = '||en_perrecisentapis_id||').';
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => en_perrecisentapis_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
EXCEPTION
   when others then
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.pkb_calc_per_rec_pis_m400 (fase = '||vn_fase||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => 'Calcular por período a receita isenta do PIS'
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => en_perrecisentapis_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
END PKB_CALC_PER_REC_PIS_M400;
---------------------------------------------------------------
--| Procedimento para validar as receitas isentas - Bloco M400
--| Serão validados alguns campos dos blocos M400 e M410
---------------------------------------------------------------
PROCEDURE PKB_VALIDAR_REC_PIS_M400( EN_RECISENTAPIS_ID IN REC_ISENTA_PIS.ID%TYPE ) IS
   --
   vn_fase           number := 0;
   vn_loggenerico_id log_generico.id%type := null;
   vn_empresa_id     per_rec_isenta_pis.empresa_id%type;
   vd_dt_ini         per_rec_isenta_pis.dt_ini%type;
   vd_dt_fin         per_rec_isenta_pis.dt_fin%type;
   --
   cursor c_detalhe is
      select dr.id
           , dr.vl_rec
           , dr.natrecpc_id
           , dr.planoconta_id
        from det_rec_isenta_pis dr
       where dr.recisentapis_id = gt_row_rec_isenta_pis.id;
   --
BEGIN
   --
   vn_fase := 1;
   --
   pkb_excluir_log( en_referencia_id  => en_recisentapis_id
                  , ev_obj_referencia => 'REC_ISENTA_PIS' );
   --
   vn_fase := 2;
   --
   gv_obj_referencia := 'REC_ISENTA_PIS';
   gv_mensagem_log   := 'Validação da receita isenta do imposto PIS/PASEP';
   gv_resumo_log     := 'Início do processo (identificador da receita = '||en_recisentapis_id||').';
   gn_referencia_id  := en_recisentapis_id;
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => gn_referencia_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
   gv_resumo_log := null;
   --
   vn_fase := 3;
   --
   if nvl(en_recisentapis_id,0) = 0 then
      vn_fase := 4;
      gv_resumo_log := 'O identificador da receita isenta deve ser informado para que a validação seja efetuada.';
      pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                       , ev_mensagem        => gv_mensagem_log
                                       , ev_resumo          => gv_resumo_log
                                       , en_tipo_log        => erro_de_validacao
                                       , en_referencia_id   => null
                                       , ev_obj_referencia  => gv_obj_referencia );
   else
      --
      vn_fase := 5;
      --| Recupera os dados da apuração do imposto pis utilizado como referência
      pkb_dados_rec_pis_m400( en_recisentapis_id => en_recisentapis_id );
      gv_mensagem_log := 'Validação das '||gv_mensagem_log;
      --
      vn_fase := 6;
      --
      begin
         select pa.empresa_id
              , pa.dt_ini
              , pa.dt_fin
           into vn_empresa_id
              , vd_dt_ini
              , vd_dt_fin
           from per_rec_isenta_pis pa
          where pa.id = gt_row_rec_isenta_pis.perrecisentapis_id;
      exception
         when others then
            gv_resumo_log := 'Período de Apuração não encontrado. Verifique o identificador informado (id = '||gt_row_rec_isenta_pis.perrecisentapis_id||').';
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                             , ev_mensagem        => gv_mensagem_log
                                             , ev_resumo          => gv_resumo_log
                                             , en_tipo_log        => erro_de_validacao
                                             , en_referencia_id   => gn_referencia_id
                                             , ev_obj_referencia  => gv_obj_referencia );
      end;
      --
      vn_fase := 7;
      --
      if pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => vn_empresa_id
                                               , ed_data       => vd_dt_ini ) or  -- true
         pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => vn_empresa_id
                                               , ed_data       => vd_dt_fin ) then -- true
         --
         vn_fase := 8;
         --
         gv_resumo_log := 'Existe(m) período(s) de abertura efd pis/cofins com situação: Validado ou Arquivo Gerado. Desprocesse a geração do arquivo.';
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_validacao
                                          , en_referencia_id   => gn_referencia_id
                                          , ev_obj_referencia  => gv_obj_referencia );
         --
      else
         --
         vn_fase := 9;
         --
         if gt_row_rec_isenta_pis.dm_situacao <> 1 then -- não estiver calculada
            vn_fase := 10;
            gv_resumo_log := 'A receita isenta informada não está com situação calculada, portanto, a validação não será realizada.';
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                             , ev_mensagem        => gv_mensagem_log
                                             , ev_resumo          => gv_resumo_log
                                             , en_tipo_log        => erro_de_validacao
                                             , en_referencia_id   => gn_referencia_id
                                             , ev_obj_referencia  => gv_obj_referencia );
         elsif pk_csf_efd_pc.fkg_qtde_det_rec_pis( en_recisentapis_id => gt_row_rec_isenta_pis.id ) = 0 then -- devem existir detalhes de receita gerados
               vn_fase := 11;
               gv_resumo_log := 'A receita isenta informada não possui detalhes gerados, portanto, a validação não será realizada.';
               pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                , ev_mensagem        => gv_mensagem_log
                                                , ev_resumo          => gv_resumo_log
                                                , en_tipo_log        => erro_de_validacao
                                                , en_referencia_id   => gn_referencia_id
                                                , ev_obj_referencia  => gv_obj_referencia );
         else
            --
            vn_fase := 12;
            --
            pkb_recupera_tpimp;
            --
            vn_fase := 13;
            --
            for r_det in c_detalhe
            loop
               --
               vn_fase := 14;
               --
               if nvl(pk_csf_efd_pc.fkg_conf_id_nat_rec_pc ( r_det.natrecpc_id ),0) = 0 then
                  gv_resumo_log := 'No detalhe da receita isenta, o identificador da natureza da receita (id = '||
                                   nvl(r_det.natrecpc_id,0)||') está inválido.';
                  pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                                  , ev_mensagem        => gv_mensagem_log
                                                  , ev_resumo          => gv_resumo_log
                                                  , en_tipo_log        => erro_de_validacao
                                                  , en_referencia_id   => gn_referencia_id
                                                  , ev_obj_referencia  => gv_obj_referencia );
               end if;
               --
               vn_fase := 15;
               --
               if nvl(r_det.vl_rec,0) < 0 then
                  gv_resumo_log := 'No detalhe da receita isenta, o valor da receita bruta ('||
                                   to_char(r_det.vl_rec,'999G999G999G999G990D00')||') não pode ser negativo.';
                  pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                                  , ev_mensagem        => gv_mensagem_log
                                                  , ev_resumo          => gv_resumo_log
                                                  , en_tipo_log        => erro_de_validacao
                                                  , en_referencia_id   => gn_referencia_id
                                                  , ev_obj_referencia  => gv_obj_referencia );
               end if;
               --
               vn_fase := 16;
               --
               if nvl(r_det.planoconta_id,0) <> 0 and
                  pk_csf_efd_pc.fkg_id_plano_conta_id ( en_id => r_det.planoconta_id ) <> nvl(r_det.planoconta_id,0) then
                  gv_resumo_log := 'No detalhe da receita isenta, o identificador do plano de conta (id = '||
                                   nvl(r_det.planoconta_id,0)||') está inválido.';
                  pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                                  , ev_mensagem        => gv_mensagem_log
                                                  , ev_resumo          => gv_resumo_log
                                                  , en_tipo_log        => erro_de_validacao
                                                  , en_referencia_id   => gn_referencia_id
                                                  , ev_obj_referencia  => gv_obj_referencia );
               end if;
               --
            end loop;
            --
            vn_fase := 17;
            --
            if nvl(pk_csf.fkg_cod_st_cod( en_id_st => gt_row_rec_isenta_pis.codst_id ),0) = 0 then
               gv_resumo_log := 'No detalhe da receita isenta, o identificador da situação tributária (id = '||
                                nvl(gt_row_rec_isenta_pis.codst_id,0)||') está inválido.';
               pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                               , ev_mensagem        => gv_mensagem_log
                                               , ev_resumo          => gv_resumo_log
                                               , en_tipo_log        => erro_de_validacao
                                               , en_referencia_id   => gn_referencia_id
                                               , ev_obj_referencia  => gv_obj_referencia );
            elsif nvl(pk_csf.fkg_cod_st_id ( ev_cod_st      => nvl(pk_csf.fkg_cod_st_cod( en_id_st => gt_row_rec_isenta_pis.codst_id ),0)
                                           , en_tipoimp_id  => gn_tipoimp_id ),0) = 0 then
                  gv_resumo_log := 'No detalhe da receita isenta, o identificador da situação tributária (id = '||
                                   nvl(gt_row_rec_isenta_pis.codst_id,0)||') está inválido com relação do tipo de imposto (pis).';
                  pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                                  , ev_mensagem        => gv_mensagem_log
                                                  , ev_resumo          => gv_resumo_log
                                                  , en_tipo_log        => erro_de_validacao
                                                  , en_referencia_id   => gn_referencia_id
                                                  , ev_obj_referencia  => gv_obj_referencia );
            end if;
            --
            vn_fase := 18;
            --
            if nvl(gt_row_rec_isenta_pis.vl_tot_rec,0) < 0 then
               gv_resumo_log := 'Na receita isenta, o valor total da receita bruta ('||
                                to_char(gt_row_rec_isenta_pis.vl_tot_rec,'999G999G999G999G990D00')||') não pode ser negativo.';
               pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                               , ev_mensagem        => gv_mensagem_log
                                               , ev_resumo          => gv_resumo_log
                                               , en_tipo_log        => erro_de_validacao
                                               , en_referencia_id   => gn_referencia_id
                                               , ev_obj_referencia  => gv_obj_referencia );
            end if;
            --
            vn_fase := 19;
            --
            if nvl(gt_row_rec_isenta_pis.planoconta_id,0) <> 0 and
               pk_csf_efd_pc.fkg_id_plano_conta_id ( en_id => gt_row_rec_isenta_pis.planoconta_id ) <> nvl(gt_row_rec_isenta_pis.planoconta_id,0) then
               gv_resumo_log := 'No detalhe da receita isenta, o identificador do plano de conta (id = '||
                                nvl(gt_row_rec_isenta_pis.planoconta_id,0)||') está inválido.';
               pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                               , ev_mensagem        => gv_mensagem_log
                                               , ev_resumo          => gv_resumo_log
                                               , en_tipo_log        => erro_de_validacao
                                               , en_referencia_id   => gn_referencia_id
                                               , ev_obj_referencia  => gv_obj_referencia );
            end if;
            --
            vn_fase := 20;
            --
            if gv_resumo_log is null then
               --
               vn_fase := 21;
               --| Atualizar a descrição complementar nos detalhes da receita isenta
               begin
                  update det_rec_isenta_pis dr
                     set dr.desc_compl = ltrim(rtrim(pk_csf.fkg_converte(dr.desc_compl)))
                   where dr.recisentapis_id = gt_row_rec_isenta_pis.id;
               exception
                  when others then
                     gv_resumo_log := 'Problemas (validação) ao atualizar a descrição dos detalhes da receita isenta. Erro = '||sqlerrm;
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                      , ev_mensagem        => gv_mensagem_log
                                                      , ev_resumo          => gv_resumo_log
                                                      , en_tipo_log        => erro_de_sistema
                                                      , en_referencia_id   => gn_referencia_id
                                                      , ev_obj_referencia  => gv_obj_referencia );
               end;
               --
            end if;
            --
         end if;
         --
      end if;
      --
   end if;
   --
   vn_fase := 22;
   --
   if gv_resumo_log is null then
      --
      vn_fase := 23;
      --| Atualizar a situação da receita isenta
      begin
         update rec_isenta_pis ri
            set ri.dm_situacao = 3 -- Processada
          where ri.id = en_recisentapis_id;
      exception
         when others then
            gv_resumo_log := 'Problemas ao alterar a situação da receita isenta para 3(Processada). Erro = '||sqlerrm;
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                             , ev_mensagem       => gv_mensagem_log
                                             , ev_resumo         => gv_resumo_log
                                             , en_tipo_log       => erro_de_sistema
                                             , en_referencia_id  => gn_referencia_id
                                             , ev_obj_referencia => gv_obj_referencia );
      end;
      --
   else
      --
      vn_fase := 24;
      --| Atualizar a situação da receita isenta
      begin
         update rec_isenta_pis ri
            set ri.dm_situacao = 4 -- Erro de validação
          where ri.id = en_recisentapis_id;
      exception
         when others then
            gv_resumo_log := 'Problemas ao alterar a situação da receita isenta para 4(Erro de validação). Erro = '||sqlerrm;
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                             , ev_mensagem       => gv_mensagem_log
                                             , ev_resumo         => gv_resumo_log
                                             , en_tipo_log       => erro_de_sistema
                                             , en_referencia_id  => gn_referencia_id
                                             , ev_obj_referencia => gv_obj_referencia );
      end;
      --
   end if;
   --
   vn_fase := 25;
   --
   commit;
   --
   vn_fase := 26;
   --
   gv_resumo_log := 'Término do processo (identificador da receita = '||en_recisentapis_id||').';
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => gn_referencia_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
EXCEPTION
   when others then
      --
      begin
         update rec_isenta_pis ri
            set ri.dm_situacao = 4 -- Erro de validação
          where ri.id = en_recisentapis_id;
      exception
         when others then
            gv_resumo_log := 'Problemas ao atualizar a situação da receita isenta para 4(Erro de validação). Erro = '||sqlerrm;
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                             , ev_mensagem        => gv_mensagem_log
                                             , ev_resumo          => gv_resumo_log
                                             , en_tipo_log        => erro_de_sistema
                                             , en_referencia_id   => en_recisentapis_id
                                             , ev_obj_referencia  => gv_obj_referencia );
      end;
      --
      commit;
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.pkb_validar_rec_pis_m400 (fase = '||vn_fase||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => en_recisentapis_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
END PKB_VALIDAR_REC_PIS_M400;
-------------------------------------------------------------------------------
--| Procedimento para validar por período a receita isenta do PIS - Bloco M400
--| Serão validados alguns campos dos blocos M400 e M410
-------------------------------------------------------------------------------
PROCEDURE PKB_VAL_PER_REC_PIS_M400( EN_PERRECISENTAPIS_ID IN PER_REC_ISENTA_PIS.ID%TYPE ) IS
   --
   vn_fase           number := 0;
   vn_loggenerico_id log_generico.id%type;
   --
   cursor c_per_rec_isenta_pis( en_perrecisentapis_id in per_rec_isenta_pis.id%type ) is
      select *
        from per_rec_isenta_pis pr
       where pr.id  = en_perrecisentapis_id;
   --
   cursor c_rec is
      select ri.id recisentapis_id
        from rec_isenta_pis ri
       where ri.perrecisentapis_id = en_perrecisentapis_id;
BEGIN
   --
   vn_fase := 1;
   --
   pkb_excluir_log( en_referencia_id  => en_perrecisentapis_id
