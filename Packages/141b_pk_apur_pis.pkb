-----------------------------------------------------------------------------

-----------------------------------------------------------------------------
--| Função para retornar o row da Tabela de Periodo de Consolidação da Contribuição de PIS
-----------------------------------------------------------------------------
procedure pkb_ret_perconscontrpis ( en_empresa_id         in per_cons_contr_pis.empresa_id%type
                                  , ed_dt_ini             in per_cons_contr_pis.dt_ini%type
                                  , ed_dt_fin             in per_cons_contr_pis.dt_fin%type
                                  , st_per_cons_contr_pis in out per_cons_contr_pis%rowtype )
is
begin
   if st_per_cons_contr_pis.id is not null then
     return;
   else
      --
      select *
        into st_per_cons_contr_pis
        from per_cons_contr_pis
      where empresa_id = en_empresa_id
        and dt_ini    >= ed_dt_ini
        and dt_fin    <= ed_dt_fin;
      --     
   end if;     
   --
exception
  when no_data_found then
     null;
  when others then
     gv_resumo_log := 'Erro na procedure pkb_ret_perconscontrpis : '||sqlerrm;
     raise_application_error (-20101, gv_resumo_log);
   -- 
end pkb_ret_perconscontrpis; 
--
-----------------------------------------------------------------------------
--| Função para retornar o id da tabela REGISTR_EFD_PC
-----------------------------------------------------------------------------
function fkg_ret_registrefdpc_id (ev_registrefdpc_cd registr_efd_pc.cd%type)
return registr_efd_pc.id%type 
is
  vn_registrefdpc_id registr_efd_pc.id%type;
begin
   select id
     into vn_registrefdpc_id
     from registr_efd_pc
   where cd = ev_registrefdpc_cd;   
   --
   return vn_registrefdpc_id;
   --
exception
  when others then
      gv_resumo_log := 'Erro na função fkg_ret_registrefdpc_id : '||sqlerrm;
      raise_application_error (-20101, gv_resumo_log);
end fkg_ret_registrefdpc_id; 
--
-----------------------------------------------------------------------------
--| Procedimento para vetorizar a memória de calculo
-----------------------------------------------------------------------------
procedure pkb_monta_vetor_mem_calc_pis (et_mem_calc_apur_pis in mem_calc_apur_pis%rowtype)
is
  idx number := ga_mem_calc_apur_pis.Count + 1;
begin
   -- Insere no array -- 
   ga_mem_calc_apur_pis.Extend;
   ga_mem_calc_apur_pis(idx) := tp_mem_calc_apur_pis( et_mem_calc_apur_pis.id
                                                    , et_mem_calc_apur_pis.perconscontrpis_id
                                                    , et_mem_calc_apur_pis.registrefdpc_id
                                                    , et_mem_calc_apur_pis.referencia_id
                                                    , et_mem_calc_apur_pis.ref_item_id
                                                    , et_mem_calc_apur_pis.tipo_operacao
                                                    , et_mem_calc_apur_pis.ident_doc
                                                    , et_mem_calc_apur_pis.dm_ind_emit
                                                    , et_mem_calc_apur_pis.vl_base_calc
                                                    , et_mem_calc_apur_pis.aliq_perc
                                                    , et_mem_calc_apur_pis.aliq_prod
                                                    , et_mem_calc_apur_pis.vl_imp_trib
                                                    , et_mem_calc_apur_pis.reg_controlador
                                                    );
   --   
exception
   when others then
      gv_resumo_log := 'Erro na procedure pkb_monta_vetor_mem_calc_pis : '||sqlerrm;
      raise_application_error (-20101, gv_resumo_log);
end pkb_monta_vetor_mem_calc_pis;
--
-----------------------------------------------------------------------------
--| Procedimento para gravar a memória de calculo
-----------------------------------------------------------------------------
procedure pkb_grava_vetor_mem_calc_pis
is
begin
   -- Caso o vetor não esteja populado, sai da procedure.
   if ga_mem_calc_apur_pis.Count = 0 then
      return;
   end if;
   -- 
   -- Descarreta o vetor na tabela
   for i in ga_mem_calc_apur_pis.First .. ga_mem_calc_apur_pis.Last
   loop
      begin
         insert into mem_calc_apur_pis (id,
                                        perconscontrpis_id,
                                        registrefdpc_id,
                                        referencia_id,
                                        ref_item_id,
                                        tipo_operacao,
                                        ident_doc,
                                        dm_ind_emit,
                                        vl_base_calc,
                                        aliq_perc,
                                        aliq_prod,
                                        vl_imp_trib,
                                        reg_controlador) 
                                   values (memcalcapurpis_seq.NextVal,
                                        gt_row_per_cons_contr_pis.id,
                                        ga_mem_calc_apur_pis(i).registrefdpc_id,
                                        ga_mem_calc_apur_pis(i).referencia_id,
                                        ga_mem_calc_apur_pis(i).ref_item_id,
                                        ga_mem_calc_apur_pis(i).tipo_operacao,
                                        ga_mem_calc_apur_pis(i).ident_doc,
                                        ga_mem_calc_apur_pis(i).dm_ind_emit,
                                        nvl(ga_mem_calc_apur_pis(i).vl_base_calc,0),
                                        nvl(ga_mem_calc_apur_pis(i).aliq_perc,0),
                                        nvl(ga_mem_calc_apur_pis(i).aliq_prod,0),
                                        nvl(ga_mem_calc_apur_pis(i).vl_imp_trib,0),
                                        ga_mem_calc_apur_pis(i).reg_controlador
                                        );
      exception
         when dup_val_on_index then
            --
            update mem_calc_apur_pis set
               tipo_operacao     = ga_mem_calc_apur_pis(i).tipo_operacao,
               ident_doc         = ga_mem_calc_apur_pis(i).ident_doc,
               dm_ind_emit       = ga_mem_calc_apur_pis(i).dm_ind_emit,
               vl_base_calc      = ga_mem_calc_apur_pis(i).vl_base_calc,
               aliq_perc         = ga_mem_calc_apur_pis(i).aliq_perc,
               aliq_prod         = ga_mem_calc_apur_pis(i).aliq_prod,
               vl_imp_trib       = ga_mem_calc_apur_pis(i).vl_imp_trib,
               reg_controlador   = ga_mem_calc_apur_pis(i).reg_controlador
            where perconscontrpis_id    = gt_row_per_cons_contr_pis.id
              and registrefdpc_id       = ga_mem_calc_apur_pis(i).registrefdpc_id
              and referencia_id         = ga_mem_calc_apur_pis(i).referencia_id
              and ref_item_id           = ga_mem_calc_apur_pis(i).ref_item_id;
         --
      end;
      --
   end loop;
   --   
   commit;
   --
exception
   when others then
      gv_resumo_log := 'Erro na procedure pkb_grava_vetor_mem_calc_pis : '||sqlerrm;
      raise_application_error (-20101, gv_resumo_log);
end pkb_grava_vetor_mem_calc_pis;
--
-----------------------------------------------------------------------------
--| Procedimento para excluir a memória de calculo no desprocessamento
-----------------------------------------------------------------------------
procedure pkb_exclui_mem_calc_pis ( en_perconscontrpis_id per_cons_contr_pis.id%type
                                  , ev_reg_controlador    mem_calc_apur_pis.reg_controlador%type)
is
begin
   --
   -- Exclui os dados da tabela
   delete MEM_CALC_APUR_PIS t
     where t.perconscontrpis_id = en_perconscontrpis_id
       and t.reg_controlador    = ev_reg_controlador;
   --   
   commit;
   --
exception
   when others then
      gv_resumo_log := 'Erro na procedure pkb_exclui_mem_calc_pis : '||sqlerrm;
      raise_application_error (-20101, gv_resumo_log);
end pkb_exclui_mem_calc_pis;
--
-----------------------------------------------------------------------------
--| Procedimento recupera os dados dos processos dos Bloco F800 - Bloco M100
-----------------------------------------------------------------------------
PROCEDURE PKB_MONTA_DADOS_M100_F800 IS
   --
   vn_fase            number := 0;
   vn_loggenerico_id  log_generico.id%type;
   --
   --| Recuperar a empresa e suas filiais
   cursor c_empresa is
      select em.id empresa_id
           , em.dm_dt_escr_dfepoe
        from empresa em
       start with em.id = gt_row_per_apur_cred_pis.empresa_id
     connect by prior em.id = em.ar_empresa_id;
   --
   --| CRÉDITOS DECORRENTES DE EVENTOS DE INCORPORAÇÃO, FUSÃO E CISÃO
   cursor c_f800( en_empresa_id in empresa.id%type ) is
      select cd.id creddecoreventopc_id
           , cd.dt_evento
           , gn_tipoimp_id tipoimp_id
           , 1 dm_ind_cred_ori -- 0-Operações próprias, 1-Evento de incorporação, cisão ou fusão
           , cd.tipocredpc_id
           , nvl(cd.vl_cred_pis,0) vl_cred_pis
        from cred_decor_evento_pc cd
       where cd.empresa_id = en_empresa_id
         and cd.dt_evento  between gt_row_per_apur_cred_pis.dt_ini and gt_row_per_apur_cred_pis.dt_fin
         and cd.dm_st_proc = 1 -- validado
       order by cd.id
           , gn_tipoimp_id;
   --
BEGIN
   --
   vn_fase := 1;
   --| Créditos decorrentes de eventos de incorporação, fusão e cisão
   for r_empresa in c_empresa
   loop
      --
      vn_fase := 2;
      --
      for r_reg in c_f800( en_empresa_id => r_empresa.empresa_id )
      loop
         --
         vn_fase := 3;
         --
         begin
            insert into apur_cred_pis( id
                                     , dm_situacao
                                     , tipocredpc_id
                                     , dm_ind_cred_ori
                                     , vl_bc_pis
                                     , aliq_pis
                                     , quant_bc_pis
                                     , vl_aliq_pis_quant
                                     , vl_cred
                                     , vl_ajus_acres
                                     , vl_ajus_reduc
                                     , vl_cred_dif
                                     , vl_cred_disp
                                     , dm_ind_desc_cred
                                     , vl_cred_desc
                                     , vl_sld_cred
                                     , perapurcredpis_id )
                               values( apurcredpis_seq.nextval
                                     , 0 -- dm_situacao - 0-Aberto
                                     , r_reg.tipocredpc_id
                                     , 1 -- dm_ind_cred_ori - Evento de incorporação, cisão ou fusão
                                     , 0 -- vl_bc_pis
                                     , 0 -- aliq_pis
                                     , 0 -- quant_bc_pis
                                     , 0 -- vl_aliq_pis_quant
                                     , nvl(r_reg.vl_cred_pis,0) -- vl_cred
                                     , 0 -- vl_ajus_acres
                                     , 0 -- vl_ajus_reduc
                                     , 0 -- vl_cred_dif
                                     , nvl(r_reg.vl_cred_pis,0) -- vl_cred_disp
                                     , 0 -- dm_ind_desc_cred - 0-Utilização do valor total para desconto da contribuição apurada no período, no Registro M200, 1-Utilização de valor parcial para desconto da contribuição apurada no período, no Registro M200
                                     , nvl(r_reg.vl_cred_pis,0) -- vl_cred_desc
                                     , 0 -- vl_sld_cred
                                     , gt_row_per_apur_cred_pis.id );
         exception
            when others then
               --
               gv_resumo_log := 'Problemas ao incluir apuração do Bloco F800 (id='||r_reg.creddecoreventopc_id||' fase = '||vn_fase||'). Erro = '||sqlerrm;
               --
               declare
                  vn_loggenerico_id  log_generico.id%type;
               begin
                  pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                   , ev_mensagem        => gv_mensagem_log
                                                   , ev_resumo          => gv_resumo_log
                                                   , en_tipo_log        => erro_de_sistema
                                                   , en_referencia_id   => gn_referencia_id
                                                   , ev_obj_referencia  => gv_obj_referencia );
               exception
                  when others then
                     null;
               end;
               --
               raise_application_error (-20101, gv_resumo_log);
               --
         end;
         --
      end loop;
      --
      vn_fase := 4;
      --
      commit;
      --
   end loop; -- cursor c_empresa
   --
EXCEPTION
   when others then
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.pkb_monta_dados_m100_f800 (fase = '||vn_fase||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => gn_referencia_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
END PKB_MONTA_DADOS_M100_F800;
--------------------------------------------------------------------------
--| Procedimento para gravar os valores nas tabelas de banco - Bloco M100
--------------------------------------------------------------------------
PROCEDURE PKB_GRAVA_DADOS_M100 IS
   --
   vn_fase   number := 0;
   vn_ind    number := 0;
   --
BEGIN
   --
   vn_fase := 1;
   --| Gravar os dados de apuração do pis - apur_cred_pis
   vn_ind := nvl(vt_tab_reg_m100.first,0);
   --
   vn_fase := 2;
   --
   loop
      --
      vn_fase := 3;
      --
      if nvl(vn_ind,0) = 0 then
         exit;
      end if;
      --
      vn_fase := 4;
      --
      begin
         insert into apur_cred_pis( id
                                  , dm_situacao
                                  , tipocredpc_id
                                  , dm_ind_cred_ori
                                  , vl_bc_pis
                                  , aliq_pis
                                  , quant_bc_pis
                                  , vl_aliq_pis_quant
                                  , vl_cred
                                  , vl_ajus_acres
                                  , vl_ajus_reduc
                                  , vl_cred_dif
                                  , vl_cred_disp
                                  , dm_ind_desc_cred
                                  , vl_cred_desc
                                  , vl_sld_cred
                                  , perapurcredpis_id )
                            values( vt_tab_reg_m100(vn_ind).apurcredpis_id
                                  , vt_tab_reg_m100(vn_ind).dm_situacao
                                  , vt_tab_reg_m100(vn_ind).tipocredpc_id
                                  , vt_tab_reg_m100(vn_ind).dm_ind_cred_ori
                                  , vt_tab_reg_m100(vn_ind).vl_bc_pis
                                  , vt_tab_reg_m100(vn_ind).aliq_pis
                                  , vt_tab_reg_m100(vn_ind).quant_bc_pis
                                  , vt_tab_reg_m100(vn_ind).vl_aliq_pis_quant
                                  , (vt_tab_reg_m100(vn_ind).vl_bc_pis * vt_tab_reg_m100(vn_ind).aliq_pis / 100) +
                                    (vt_tab_reg_m100(vn_ind).quant_bc_pis * vt_tab_reg_m100(vn_ind).vl_aliq_pis_quant)
                                  , vt_tab_reg_m100(vn_ind).vl_ajus_acres
                                  , vt_tab_reg_m100(vn_ind).vl_ajus_reduc
                                  , vt_tab_reg_m100(vn_ind).vl_cred_dif
                                  , vt_tab_reg_m100(vn_ind).vl_cred_disp
                                  , vt_tab_reg_m100(vn_ind).dm_ind_desc_cred
                                  , vt_tab_reg_m100(vn_ind).vl_cred_desc
                                  , vt_tab_reg_m100(vn_ind).vl_sld_cred
                                  , gt_row_per_apur_cred_pis.id );
      exception
         when dup_val_on_index then
            --
            vn_fase := 5;
            --
            begin
               update apur_cred_pis ac
                  set ac.dm_situacao       = vt_tab_reg_m100(vn_ind).dm_situacao
                    , ac.tipocredpc_id     = vt_tab_reg_m100(vn_ind).tipocredpc_id
                    , ac.dm_ind_cred_ori   = vt_tab_reg_m100(vn_ind).dm_ind_cred_ori
                    , ac.vl_bc_pis         = vt_tab_reg_m100(vn_ind).vl_bc_pis
                    , ac.aliq_pis          = vt_tab_reg_m100(vn_ind).aliq_pis
                    , ac.quant_bc_pis      = vt_tab_reg_m100(vn_ind).quant_bc_pis
                    , ac.vl_aliq_pis_quant = vt_tab_reg_m100(vn_ind).vl_aliq_pis_quant
                    , ac.vl_cred           = (vt_tab_reg_m100(vn_ind).vl_bc_pis * vt_tab_reg_m100(vn_ind).aliq_pis / 100) +
                                             (vt_tab_reg_m100(vn_ind).quant_bc_pis * vt_tab_reg_m100(vn_ind).vl_aliq_pis_quant)
                    , ac.vl_ajus_acres     = vt_tab_reg_m100(vn_ind).vl_ajus_acres
                    , ac.vl_ajus_reduc     = vt_tab_reg_m100(vn_ind).vl_ajus_reduc
                    , ac.vl_cred_dif       = vt_tab_reg_m100(vn_ind).vl_cred_dif
                    , ac.vl_cred_disp      = vt_tab_reg_m100(vn_ind).vl_cred_disp
                    , ac.dm_ind_desc_cred  = vt_tab_reg_m100(vn_ind).dm_ind_desc_cred
                    , ac.vl_cred_desc      = vt_tab_reg_m100(vn_ind).vl_cred_desc
                    , ac.vl_sld_cred       = vt_tab_reg_m100(vn_ind).vl_sld_cred
                    , ac.perapurcredpis_id = gt_row_per_apur_cred_pis.id
                where ac.id = vt_tab_reg_m100(vn_ind).apurcredpis_id;
            exception
               when others then
                  --
                  gv_resumo_log := 'Problemas ao alterar apuração (id='||vt_tab_reg_m100(vn_ind).apurcredpis_id||' fase = '||vn_fase||'). Erro = '||sqlerrm;
                  --
                  declare
                     vn_loggenerico_id  log_generico.id%type;
                  begin
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                      , ev_mensagem        => gv_mensagem_log
                                                      , ev_resumo          => gv_resumo_log
                                                      , en_tipo_log        => erro_de_sistema
                                                      , en_referencia_id   => gn_referencia_id
                                                      , ev_obj_referencia  => gv_obj_referencia );
                  exception
                     when others then
                        null;
                  end;
                  --
                  raise_application_error (-20101, gv_resumo_log);
                  --
            end;
            --
         when others then
            --
            gv_resumo_log := 'Problemas ao incluir apuração (id='||vt_tab_reg_m100(vn_ind).apurcredpis_id||' fase = '||vn_fase||'). Erro = '||sqlerrm;
            --
            declare
               vn_loggenerico_id  log_generico.id%type;
            begin
               pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                , ev_mensagem        => gv_mensagem_log
                                                , ev_resumo          => gv_resumo_log
                                                , en_tipo_log        => erro_de_sistema
                                                , en_referencia_id   => gn_referencia_id
                                                , ev_obj_referencia  => gv_obj_referencia );
            exception
               when others then
                  null;
            end;
            --
            raise_application_error (-20101, gv_resumo_log);
            --
      end;
      --
      vn_fase := 6;
      --
      if vn_ind = vt_tab_reg_m100.last then
         exit;
      else
         vn_ind := vt_tab_reg_m100.next(vn_ind);
      end if;
      --
   end loop;
   --
   vn_fase := 7;
   --| Gravar os dados de detalhe de apuração do pis - det_apur_cred_pis
   vn_ind := nvl(vt_tab_reg_m105.first,0);
   --
   vn_fase := 8;
   --
   loop
      --
      vn_fase := 9;
      --
      if nvl(vn_ind,0) = 0 then
         exit;
      end if;
      --
      vn_fase := 10;
      -- Ajuste de valores para que o PVA não acuse "Avisos/Advertências", por centavos
      if (nvl(vt_tab_reg_m105(vn_ind).vl_bc_pis_cum,0) - (nvl(vt_tab_reg_m105(vn_ind).vl_bc_pis_tot,0) * nvl(gn_recbru_cum,0))) <= 0.5 then
          vt_tab_reg_m105(vn_ind).vl_bc_pis_cum := (nvl(vt_tab_reg_m105(vn_ind).vl_bc_pis_tot,0) * nvl(gn_recbru_cum,0));
      end if;
      --
      vn_fase := 11;
      -- Ajuste de valores para que o PVA não acuse "Avisos/Advertências", por centavos
      if nvl(vt_tab_reg_m105(vn_ind).vl_bc_pis_nc,0) <> (nvl(vt_tab_reg_m105(vn_ind).vl_bc_pis_tot,0) - nvl(vt_tab_reg_m105(vn_ind).vl_bc_pis_cum,0)) then
         vt_tab_reg_m105(vn_ind).vl_bc_pis_nc := (nvl(vt_tab_reg_m105(vn_ind).vl_bc_pis_tot,0) - nvl(vt_tab_reg_m105(vn_ind).vl_bc_pis_cum,0));
      end if;
      --
      vn_fase := 12;
      --
      begin
         insert into det_apur_cred_pis( id
                                      , apurcredpis_id
                                      , basecalccredpc_id
                                      , codst_id
                                      , vl_bc_pis_tot
                                      , vl_bc_pis_cum
                                      , vl_bc_pis_nc
                                      , vl_bc_pis
                                      , quant_bc_pis_tot
                                      , quant_bc_pis
                                      , desc_cred )
                                values( vt_tab_reg_m105(vn_ind).detapurcredpis_id
                                      , vt_tab_reg_m105(vn_ind).apurcredpis_id
                                      , vt_tab_reg_m105(vn_ind).basecalccredpc_id
                                      , vt_tab_reg_m105(vn_ind).codst_id
                                      , vt_tab_reg_m105(vn_ind).vl_bc_pis_tot
                                      , vt_tab_reg_m105(vn_ind).vl_bc_pis_cum
                                      , vt_tab_reg_m105(vn_ind).vl_bc_pis_nc
                                      , vt_tab_reg_m105(vn_ind).vl_bc_pis
                                      , vt_tab_reg_m105(vn_ind).quant_bc_pis_tot
                                      , vt_tab_reg_m105(vn_ind).quant_bc_pis
                                      , vt_tab_reg_m105(vn_ind).desc_cred );
      exception
         when others then
            --
            gv_resumo_log := 'Problemas ao incluir detalhes de apuração (det_apur_cred_pis id='||
                             vt_tab_reg_m105(vn_ind).detapurcredpis_id||' fase = '||vn_fase||'). Erro = '||sqlerrm;
            --
            declare
               vn_loggenerico_id  log_generico.id%type;
            begin
               pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                , ev_mensagem        => gv_mensagem_log
                                                , ev_resumo          => gv_resumo_log
                                                , en_tipo_log        => erro_de_sistema
                                                , en_referencia_id   => gn_referencia_id
                                                , ev_obj_referencia  => gv_obj_referencia );
            exception
               when others then
                  null;
            end;
            --
            raise_application_error (-20101, gv_resumo_log);
            --
      end;
      --
      vn_fase := 13;
      --
      if vn_ind = vt_tab_reg_m105.last then
         exit;
      else
         vn_ind := vt_tab_reg_m105.next(vn_ind);
      end if;
      --
   end loop;
   --
   vn_fase := 12;
   --
   commit;
   --
EXCEPTION
   when others then
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.pkb_grava_dados_m100 (fase = '||vn_fase||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => gn_referencia_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
END PKB_GRAVA_DADOS_M100;
