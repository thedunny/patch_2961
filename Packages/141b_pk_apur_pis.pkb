-----------------------------------------------------------------------------------------------------
--| Procedimento para Compôr os valores Não-Cumulativos e Cumulativos devido a Geração do Bloco 1300
-----------------------------------------------------------------------------------------------------
PROCEDURE PKB_CALCULA_BLOCO_1300( EN_CONSCONTRPIS_ID      IN CONS_CONTR_PIS.ID%TYPE
                                , EN_TOTCONTNCDEV_M200    IN NUMBER
                                , SN_VL_RET_NC           OUT NUMBER
                                , SN_VL_OUT_DED_NC       OUT NUMBER
                                , SN_VL_CONT_NC_REC      OUT NUMBER
                                , SN_VL_TOT_CONT_CUM_PER OUT NUMBER
                                , SN_VL_RET_CUM          OUT NUMBER
                                , SN_VL_OUT_DED_CUM      OUT NUMBER
                                , SN_VL_CONT_CUM_REC     OUT NUMBER ) IS
   --
   vn_fase                     number := 0;
   vn_loggenerico_id           log_generico.id%type;
   vn_vl_ret_nc                number := 0;
   vn_tot_vl_ret_nc            number := 0;
   vn_vl_out_ded_nc            number := 0;
   vn_vl_bc_oper               number := 0;
   vn_tot_vl_out_ded_nc        number := 0;
   vn_vl_cont_nc_rec           number := 0;
   vn_vl_ret_apu_bl_1300_nc    number := 0;
   vn_vl_ret_ded_bl_1300_nc    number := 0;
   vn_vl_ret_apu_bl_1300_cum   number := 0;
   vn_vl_ret_ded_bl_1300_cum   number := 0;
   vv_proc_ded                 varchar2(1) := 'N';
   vn_vl_tot_cont_nc_dev_rat   number := 0;
   vn_vl_tot_cont_cum_per      cons_contr_pis.vl_tot_cont_cum_per%type;
   vn_vl_ret_cum               cons_contr_pis.vl_ret_cum%type;
   vn_tot_vl_ret_cum           cons_contr_pis.vl_ret_cum%type;
   vn_vl_out_ded_cum           cons_contr_pis.vl_out_ded_cum%type;
   vn_bc_out_ded_cum           cons_contr_pis.vl_out_ded_cum%type;
   vn_tot_vl_out_ded_cum       cons_contr_pis.vl_out_ded_cum%type;
   vn_vl_tot_cont_cum_per_rat  cons_contr_pis.vl_tot_cont_cum_per%type;
   vn_vl_cont_cum_rec          cons_contr_pis.vl_cont_cum_rec%type;
   vn_contrvlrretfontepis_id   contr_vlr_ret_fonte_pis.id%type;
   vt_mem_calc_apur_pis        mem_calc_apur_pis%rowtype;
   vn_registrefdpc_id          number := null;
   vn_id_mem_f600              contr_ret_fonte_pc.id%type;
   --
   cursor c_contr is
      select cv.id contrvlrretfontepis_id
        from contr_vlr_ret_fonte_pis cv
       where cv.empresa_id     = gt_row_per_cons_contr_pis.empresa_id
         and cv.ano            = to_char(gt_row_per_cons_contr_pis.dt_ini,'rrrr')
         and cv.mes            = to_char(gt_row_per_cons_contr_pis.dt_ini,'mm')
         and cv.dm_situacao    = 3 -- validado
         and cv.per_rec_ret    = to_char(gt_row_per_cons_contr_pis.dt_ini,'mmrrrr')
         and cv.dm_origem      = 1; -- 0-digitado, 1-gerado no bloco m200
   --
   cursor c_vl_ded_pis( en_ind_nat_ded in number ) is -- deduções diversas - bloco f700
      select nvl(sum(nvl(dd.vl_ded_pis,0)),0) 
            ,nvl(sum(nvl(dd.vl_bc_oper ,0)),0)
        from deducao_diversa_pc dd
       where dd.empresa_id in (select em.id empresa_id -- recuperar a matriz e as filiais
                                 from empresa em
                                start with em.id = gt_row_per_cons_contr_pis.empresa_id
                              connect by prior em.id = em.ar_empresa_id)
         and to_date(('01'||lpad(dd.mes_ref,2,'0')||dd.ano_ref),'ddmmrrrr') between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
         and dd.dm_ind_nat_ded = en_ind_nat_ded -- 0-dedução de natureza não cumulativa, 1-dedução de natureza cumulativa
         and dd.dm_st_proc     = 1; -- Validada
   --
   -- Controle dos valores retidos apurados na fonte de pis - F600 - Processo não-cumulativo e Processo cumulativo
   cursor c_vl_ret_apu( en_dm_ind_nat_rec in number ) is -- 0-Receita de Natureza Não-Cumulativa, 1-Receita de Natureza Cumulativa
   select cr.dm_ind_nat_ret
        , nvl(sum(nvl(cr.vl_ret_pis,0)),0) vl_ret_apu
     from contr_ret_fonte_pc cr -- dados do Bloco F600
    where cr.empresa_id in (select em.id empresa_id -- recuperar a matriz e as filiais
                              from empresa em
                             start with em.id = gt_row_per_cons_contr_pis.empresa_id
                           connect by prior em.id = em.ar_empresa_id)
      and cr.dm_st_proc     = 1 -- validada
      and cr.dm_ind_nat_rec = en_dm_ind_nat_rec -- 0-Receita de Natureza Não-Cumulativa, 1-Receita de Natureza Cumulativa
      --and cr.dm_st_integra in (0,10) -- 0-Indefinido, 10-Gerado por Impostos Retidos sobre Receita
      and cr.dm_st_integra in (0,7,10) -- 0-Indefinido, 7-Integração por view de banco de dados, 10-Gerado por Impostos Retidos sobre Receita
      and cr.dt_ret   between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
    group by cr.dm_ind_nat_ret;
   --
   -- Controle de valores retidos da fonte - Bloco F600
   cursor c_vl_imp_pis( en_dm_ind_nat_rec in number
                      , ev_dm_ind_nat_ret in varchar2 ) is
      select ir.dt_ret
           , pk_csf.fkg_cnpjcpf_pessoa_id(pe.id) cod_part ---69023
           , nvl(sum(nvl(ir.vl_ret_pis,0)),0) vl_ret_pis
           , nvl(sum(nvl(ir.vl_base_calc,0)),0) vl_base_calc
        from contr_ret_fonte_pc         cr
           , contrretfonte_impretrec_pc ci
           , imp_ret_rec_pc             ir
           , pessoa                     pe
       where cr.empresa_id in (select em.id empresa_id -- recuperar a matriz e as filiais
                                 from empresa em
                                start with em.id = gt_row_per_cons_contr_pis.empresa_id
                              connect by prior em.id = em.ar_empresa_id)
         and cr.dm_st_proc         = 1 -- validada
         and cr.dm_ind_nat_rec     = en_dm_ind_nat_rec -- 0-Receita de Natureza Não-Cumulativa, 1-Receita de Natureza Cumulativa
         and cr.dm_ind_nat_ret     = ev_dm_ind_nat_ret
         and cr.dt_ret       between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
         and ci.contrretfontepc_id = cr.id
         and ir.id                 = ci.impretrecpc_id
         and ir.dm_st_proc         = 1 -- validado
         and ir.empresa_id         = cr.empresa_id
         and ir.dm_ind_nat_rec     = cr.dm_ind_nat_rec
         and ir.dm_ind_nat_ret     = cr.dm_ind_nat_ret
         and pe.id                 = ir.pessoa_id
       group by ir.dt_ret
           , pe.id
      union
      select cr.dt_ret
           , cr.cnpj
           , nvl(sum(nvl(cr.vl_ret_pis,0)),0) vl_ret_pis
           , nvl(sum(nvl(cr.vl_bc_ret,0)),0) vl_base_calc--armando
        from contr_ret_fonte_pc cr
       where cr.empresa_id in (select em.id empresa_id -- recuperar a matriz e as filiais
                                 from empresa em
                                start with em.id = gt_row_per_cons_contr_pis.empresa_id
                              connect by prior em.id = em.ar_empresa_id)
         and cr.dm_st_proc     = 1 -- validada
         and cr.dm_ind_nat_rec = en_dm_ind_nat_rec -- 0-Receita de Natureza Não-Cumulativa, 1-Receita de Natureza Cumulativa
         and cr.dm_ind_nat_ret = ev_dm_ind_nat_ret
         and cr.dt_ret   between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
         and not exists  (select ci.id
                            from contrretfonte_impretrec_pc ci
                           where ci.contrretfontepc_id = cr.id)
       group by cr.dt_ret
           , cr.cnpj
       order by 1 -- dt_ret
           , 2; -- pe.cod_part ou cr.cnpj
   --
   -- Controle dos valores retidos apurados na fonte de pis - 1300 - Processo não-cumulativo e Processo cumulativo
   cursor c_vl_ret_ded( en_dm_ind_nat_rec in number ) is -- 0-Receita de Natureza Não-Cumulativa, 1-Receita de Natureza Cumulativa
   select cv.id contrvlrretfontepis_id
        , cv.dm_ind_nat_ret
        , cv.vl_ret_ded
     from contr_vlr_ret_fonte_pis cv -- dados do Bloco 1300
    where cv.empresa_id in (select em.id empresa_id -- recuperar a matriz e as filiais
                              from empresa em
                             start with em.id = gt_row_per_cons_contr_pis.empresa_id
                           connect by prior em.id = em.ar_empresa_id)
      and cv.dm_situacao = 3 -- validada
      and cv.dm_origem   = 0 -- digitado
      --and ((cv.ano  = to_char(gt_row_per_cons_contr_pis.dt_ini,'rrrr')
      --      and
      --      cv.mes <= to_char(gt_row_per_cons_contr_pis.dt_ini,'mm'))
      --      or
      --     (cv.ano  < to_char(gt_row_per_cons_contr_pis.dt_ini,'rrrr')))
      and cv.ano         = to_char(gt_row_per_cons_contr_pis.dt_ini,'rrrr')
      and cv.mes         = to_char(gt_row_per_cons_contr_pis.dt_ini,'mm')
      and ((en_dm_ind_nat_rec = 0 and-- 0-Receita de Natureza Não-Cumulativa, 1-Receita de Natureza Cumulativa
            cv.dm_ind_nat_ret in ('01','02','03','04','05','99'))
            or
           (en_dm_ind_nat_rec = 1 and-- 0-Receita de Natureza Não-Cumulativa, 1-Receita de Natureza Cumulativa
            cv.dm_ind_nat_ret in ('51','52','53','54','55','59')))
      and cv.vl_ret_ded > 0
      and cv.vl_sld_ret > 0
    order by cv.ano, cv.mes;
   --
   cursor c_vl_tot_cont_cum_per is -- detalhe da consolidação - bloco m200
      select nvl(sum(nvl(dc.vl_cont_per,0)),0)
        from det_cons_contr_pis dc
       where dc.conscontrpis_id = gt_row_cons_contr_pis.id
         and ((gn_dm_cod_inc_trib = 2 and -- 2-regime cumulativo
               pk_csf_efd_pc.fkg_cd_contr_soc_apur_pc ( dc.contrsocapurpc_id ) in ('31', '32'))
               or
              (gn_dm_cod_inc_trib in (2,3) and -- 2-regime cumulativo e 3-regime cumulativo e não-cumulativo
               pk_csf_efd_pc.fkg_cd_contr_soc_apur_pc ( dc.contrsocapurpc_id ) in ('51', '52', '53', '54', '72')));
   --
BEGIN
   --
   vn_fase := 1;
   --
   begin
      delete from contr_vlr_ret_fonte_pis cv
       where cv.empresa_id  = gt_row_per_cons_contr_pis.empresa_id
         and cv.ano         = to_char(gt_row_per_cons_contr_pis.dt_ini,'rrrr')
         and cv.mes         = to_char(gt_row_per_cons_contr_pis.dt_ini,'mm')
         and cv.per_rec_ret = to_char(gt_row_per_cons_contr_pis.dt_ini,'mmrrrr')
         and cv.dm_origem   = 1; -- 0-Digitado, 1-Gerado no Bloco M200
   exception
      when others then
         gv_resumo_log := 'Problemas ao excluir controle de valor de retenção apurada e deduzida - Bloco 1300 (pk_apur_pis.pkb_calcula_bloco_1300). '||
                          'Erro = '||sqlerrm;
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                          , ev_mensagem       => gv_mensagem_log
                                          , ev_resumo         => gv_resumo_log
                                          , en_tipo_log       => erro_de_sistema
                                          , en_referencia_id  => gn_referencia_id
                                          , ev_obj_referencia => gv_obj_referencia );
   end;
   --
   vn_fase := 2;
   --
   vn_vl_ret_nc             := 0;
   vn_tot_vl_ret_nc         := 0;
   vn_vl_out_ded_nc         := 0;
   vn_tot_vl_out_ded_nc     := 0;
   vn_vl_cont_nc_rec        := 0;
   vn_vl_ret_apu_bl_1300_nc := 0;
   vn_vl_ret_ded_bl_1300_nc := 0;
   vv_proc_ded              := 'N';
   --
   if gn_dm_cod_inc_trib = 2 or -- 2-Exclusivamente no regime cumulativo
      nvl(en_totcontncdev_m200,0) = 0 then
      --
      vn_fase := 3;
      --
   else
      --
      vn_fase := 4;
      --
      vn_contrvlrretfontepis_id := 0;
      open c_contr;
      fetch c_contr into vn_contrvlrretfontepis_id; -- verifica se já existe lançto de controle de valor de natureza não-cumulativa
      close c_contr;

      --
      vn_fase := 5;
      -- soma deduções diversas - bloco f700 -> deducao_diversa_pc.vl_ded_pis
      open c_vl_ded_pis( en_ind_nat_ded => 0 ); -- 0-dedução de natureza não-cumulativa, 1-dedução de natureza cumulativa
      fetch c_vl_ded_pis into vn_vl_out_ded_nc, vn_vl_bc_oper;
      close c_vl_ded_pis;

      -- Monta a memória de calculo -------------------------------- 69023
         if nvl(vn_vl_out_ded_nc,0) > 0 then
      --
           vn_registrefdpc_id                       := fkg_ret_registrefdpc_id('F700');
           vt_mem_calc_apur_pis                     := null;
           vt_mem_calc_apur_pis.referencia_id       := gt_row_per_cons_contr_pis.id;
           vt_mem_calc_apur_pis.perconscontrpis_id  := gt_row_per_cons_contr_pis.id;
           vt_mem_calc_apur_pis.registrefdpc_id     := vn_registrefdpc_id;
           vt_mem_calc_apur_pis.tipo_operacao       := 'C';
           vt_mem_calc_apur_pis.ident_doc           := 'Deduções Diversas';
           vt_mem_calc_apur_pis.vl_base_calc        := vn_vl_bc_oper;
           vt_mem_calc_apur_pis.aliq_perc           := 0;
           vt_mem_calc_apur_pis.aliq_prod           := 0;
           vt_mem_calc_apur_pis.vl_imp_trib         := nvl(vn_vl_out_ded_nc, 0);
           vt_mem_calc_apur_pis.reg_controlador     := 'M200';
           pkb_monta_vetor_mem_calc_pis(vt_mem_calc_apur_pis);
         --
         end if;
      --
      vn_fase := 6;
      vn_vl_tot_cont_nc_dev_rat := nvl(en_totcontncdev_m200,0);
      --
      vn_fase := 7;
      --
      for r_vl_ret_apu in c_vl_ret_apu( en_dm_ind_nat_rec => 0 ) -- 0-Receita de Natureza Não-Cumulativa, 1-Receita de Natureza Cumulativa
      loop
         --
         exit when c_vl_ret_apu%notfound or (c_vl_ret_apu%notfound) is null;
         --
         vn_fase := 8;
         --
         vv_proc_ded              := 'S';
         vn_vl_ret_nc             := nvl(r_vl_ret_apu.vl_ret_apu,0);
         vn_vl_ret_apu_bl_1300_nc := nvl(r_vl_ret_apu.vl_ret_apu,0);
         --
