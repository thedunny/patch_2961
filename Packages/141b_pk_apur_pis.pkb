--| Procedimento para validar a folha de salários - PIS/PASEP - Bloco M350
---------------------------------------------------------------------------
PROCEDURE PKB_VALIDAR_FOL_PIS_M350( EN_INFPISFOLHA_ID IN INF_PIS_FOLHA.ID%TYPE ) IS
   --
   vn_fase            number := 0;
   vn_loggenerico_id  log_generico.id%type := null;
   --
BEGIN
   --
   vn_fase := 1;
   --
   pkb_excluir_log( en_referencia_id  => en_infpisfolha_id
                  , ev_obj_referencia => 'INF_PIS_FOLHA' );
   --
   vn_fase := 2;
   --
   gv_obj_referencia := 'INF_PIS_FOLHA';
   gv_mensagem_log   := 'Validação da folha de salários do imposto PIS/PASEP';
   gv_resumo_log     := 'Início do processo (identificador da folha = '||en_infpisfolha_id||').';
   gn_referencia_id  := en_infpisfolha_id;
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => gn_referencia_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
   gv_resumo_log := null;
   --
   vn_fase := 3;
   --
   if nvl(en_infpisfolha_id,0) = 0 then
      vn_fase := 4;
      gv_resumo_log := 'O identificador da folha de salários deve ser informado para que a validação seja efetuada.';
      pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                       , ev_mensagem        => gv_mensagem_log
                                       , ev_resumo          => gv_resumo_log
                                       , en_tipo_log        => erro_de_validacao
                                       , en_referencia_id   => null
                                       , ev_obj_referencia  => gv_obj_referencia );
   else
      --
      vn_fase := 5;
      -- Recupera os dados da folha de salários utilizado como referência
      pkb_dados_fol_pis_m350( en_infpisfolha_id => en_infpisfolha_id );
      gv_mensagem_log := 'Validação da '||gv_mensagem_log;
      --
      vn_fase := 6;
      --
      if pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => gt_row_inf_pis_folha.empresa_id
                                               , ed_data       => gt_row_inf_pis_folha.dt_ini ) or  -- true
         pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => gt_row_inf_pis_folha.empresa_id
                                               , ed_data       => gt_row_inf_pis_folha.dt_fin ) then -- true
         --
         vn_fase := 7;
         --
         gv_resumo_log := 'Existe(m) período(s) de abertura efd pis/cofins com situação: Validado ou Arquivo Gerado. Desprocesse a geração do arquivo.';
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_validacao
                                          , en_referencia_id   => gn_referencia_id
                                          , ev_obj_referencia  => gv_obj_referencia );
         --
      else
         --
         vn_fase := 8;
         --
         if gt_row_inf_pis_folha.dm_situacao not in (0, 1) then -- não estiver 0-em aberto, 1-calculada
            vn_fase := 9;
            gv_resumo_log := 'A folha de salários informada não está com situação "Em aberto" ou "Calculada", portanto, a validação não será realizada. '||
                             'Salve os dados para que a alteração da situação seja alterada para calculada.';
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                             , ev_mensagem        => gv_mensagem_log
                                             , ev_resumo          => gv_resumo_log
                                             , en_tipo_log        => erro_de_validacao
                                             , en_referencia_id   => gn_referencia_id
                                             , ev_obj_referencia  => gv_obj_referencia );
            --
         else
            --
            vn_fase := 10;
            --| Validar os campos do Bloco M350
            if nvl(gt_row_inf_pis_folha.vl_tot_fol,0) < 0 then
               gv_resumo_log := 'Na folha de salários, o valor total da folha de salários ('||
                                ltrim(to_char(nvl(gt_row_inf_pis_folha.vl_tot_fol,0),'999G999G999G999G990D00'))||') não pode ser negativo.';
               pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                               , ev_mensagem        => gv_mensagem_log
                                               , ev_resumo          => gv_resumo_log
                                               , en_tipo_log        => erro_de_validacao
                                               , en_referencia_id   => gn_referencia_id
                                               , ev_obj_referencia  => gv_obj_referencia );
            end if;
            --
            vn_fase := 11;
            --
            if nvl(gt_row_inf_pis_folha.vl_exc_bc,0) < 0 then
               gv_resumo_log := 'Na folha de salários, o valor total das exclusões à base de cálculo ('||
                                ltrim(to_char(nvl(gt_row_inf_pis_folha.vl_exc_bc,0),'999G999G999G999G990D00'))||') não pode ser negativo.';
               pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                               , ev_mensagem        => gv_mensagem_log
                                               , ev_resumo          => gv_resumo_log
                                               , en_tipo_log        => erro_de_validacao
                                               , en_referencia_id   => gn_referencia_id
                                               , ev_obj_referencia  => gv_obj_referencia );
            end if;
            --
            vn_fase := 12;
            --
            if nvl(gt_row_inf_pis_folha.vl_tot_bc,0) < 0 then
               gv_resumo_log := 'Na folha de salários, o valor total da base de cálculo ('||
                                ltrim(to_char(nvl(gt_row_inf_pis_folha.vl_tot_bc,0),'999G999G999G999G990D00'))||') não pode ser negativo.';
               pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                               , ev_mensagem        => gv_mensagem_log
                                               , ev_resumo          => gv_resumo_log
                                               , en_tipo_log        => erro_de_validacao
                                               , en_referencia_id   => gn_referencia_id
                                               , ev_obj_referencia  => gv_obj_referencia );
            end if;
            --
            vn_fase := 13;
            --
            if nvl(gt_row_inf_pis_folha.aliq_pis_fol,0) < 0 then
               gv_resumo_log := 'Na folha de salários, o valor da alíquota ('||
                                ltrim(to_char(nvl(gt_row_inf_pis_folha.aliq_pis_fol,0),'999G999G999G999G990D00'))||') não pode ser negativo.';
               pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                               , ev_mensagem        => gv_mensagem_log
                                               , ev_resumo          => gv_resumo_log
                                               , en_tipo_log        => erro_de_validacao
                                               , en_referencia_id   => gn_referencia_id
                                               , ev_obj_referencia  => gv_obj_referencia );
            end if;
            --
            vn_fase := 14;
            --
            if nvl(gt_row_inf_pis_folha.vl_tot_cont_fol,0) < 0 then
               gv_resumo_log := 'Na folha de salários, o valor total da contribuição social sobre a folha de salários ('||
                                ltrim(to_char(nvl(gt_row_inf_pis_folha.vl_tot_cont_fol,0),'999G999G999G999G990D00'))||') não pode ser negativo.';
               pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                               , ev_mensagem        => gv_mensagem_log
                                               , ev_resumo          => gv_resumo_log
                                               , en_tipo_log        => erro_de_validacao
                                               , en_referencia_id   => gn_referencia_id
                                               , ev_obj_referencia  => gv_obj_referencia );
            end if;
            --
            vn_fase := 15;
            --
            if nvl(gt_row_inf_pis_folha.vl_tot_cont_fol,0) <> (nvl(gt_row_inf_pis_folha.vl_tot_bc,0) * nvl(gt_row_inf_pis_folha.aliq_pis_fol,0) / 100) then
               gv_resumo_log := 'Na folha de salários, o valor total da contribuição social sobre a folha de salários ('||
                                ltrim(to_char(nvl(gt_row_inf_pis_folha.vl_tot_cont_fol,0),'999G999G999G999G990D00'))||') deve ser igual ao cálculo: '||
