               gv_resumo_log := 'A soma dos valores informado em "Pagamentos" de destino Cumulativo('||ltrim(to_char(nvl(vn_vl_rec,0),'999G999G999G999G990D00'))||
                                '), não confere com os valores informados para contribuição cumulativa ('||
                                ltrim(to_char(nvl(gt_row_cons_contr_pis.vl_cont_cum_rec,0),'999G999G999G999G990D00'))||').';
               pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                               , ev_mensagem        => gv_mensagem_log
                                               , ev_resumo          => gv_resumo_log
                                               , en_tipo_log        => erro_de_validacao
                                               , en_referencia_id   => gn_referencia_id
                                               , ev_obj_referencia  => gv_obj_referencia );
            end if;
            --
            vn_fase := 104;
            --
            open c_pagtos_ncum;
            fetch c_pagtos_ncum into vn_vl_rec;
            close c_pagtos_ncum;
            --
            vn_fase := 105;
            --
            if nvl(vn_vl_rec,0) <> nvl(gt_row_cons_contr_pis.vl_cont_nc_rec,0) then
               gv_resumo_log := 'A soma dos valores informado em "Pagamentos" de destino Não-Cumulativo('||ltrim(to_char(nvl(vn_vl_rec,0),'999G999G999G999G990D00'))||
                                '), não confere com os valores informados para contribuição não-cumulativa ('||
                                ltrim(to_char(nvl(gt_row_cons_contr_pis.vl_cont_nc_rec,0),'999G999G999G999G990D00'))||').';
               pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                               , ev_mensagem        => gv_mensagem_log
                                               , ev_resumo          => gv_resumo_log
                                               , en_tipo_log        => erro_de_validacao
                                               , en_referencia_id   => gn_referencia_id
                                               , ev_obj_referencia  => gv_obj_referencia );
            end if;
            --
            vn_fase := 106;
            --
            vn_conscontrpisor_id := 0;
            open c_pagtos_numcampo;
            fetch c_pagtos_numcampo into vn_conscontrpisor_id;
            close c_pagtos_numcampo;
            --
            vn_fase := 107;
            --
            if nvl(vn_conscontrpisor_id,0) <> 0 then
               gv_resumo_log := 'Existe(m) "Referente ao Campo" informado(s) em "Pagamentos" incorreto. Deve ser "08-Valor Não-Cumulativo" ou "12-Valor Cumulativo".';
               pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                               , ev_mensagem        => gv_mensagem_log
                                               , ev_resumo          => gv_resumo_log
                                               , en_tipo_log        => erro_de_validacao
                                               , en_referencia_id   => gn_referencia_id
                                               , ev_obj_referencia  => gv_obj_referencia );
            end if;
            --
            vn_fase := 108;
            --
            vn_conscontrpisor_id := 0;
            open c_pagtos_origem;
            fetch c_pagtos_origem into vn_conscontrpisor_id;
            close c_pagtos_origem;
            --
            vn_fase := 109;
            --
            if nvl(vn_conscontrpisor_id,0) <> 0 then
               gv_resumo_log := 'Existe(m) "Origem" informado(s) em "Pagamentos" incorreto. Deve ser "0-Digitado" ou "1-Gerado no Bloco M200".';
               pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                               , ev_mensagem        => gv_mensagem_log
                                               , ev_resumo          => gv_resumo_log
                                               , en_tipo_log        => erro_de_validacao
                                               , en_referencia_id   => gn_referencia_id
                                               , ev_obj_referencia  => gv_obj_referencia );
            end if;
            --
            vn_fase := 110;
            --
            vn_conscontrpisor_id := 0;
            open c_pagtos_tpret;
            fetch c_pagtos_tpret into vn_conscontrpisor_id;
            close c_pagtos_tpret;
            --
            vn_fase := 111;
            --
            if nvl(vn_conscontrpisor_id,0) <> 0 then
               gv_resumo_log := 'Existe(m) tipo(s) de retenção(ões) informado(s) em "Pagamentos" que não está(ão) relacionado(s) com o tipo de Imposto PIS.';
               pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                               , ev_mensagem        => gv_mensagem_log
                                               , ev_resumo          => gv_resumo_log
                                               , en_tipo_log        => erro_de_validacao
                                               , en_referencia_id   => gn_referencia_id
                                               , ev_obj_referencia  => gv_obj_referencia );
            end if;
            --
            vn_fase := 112;
            --
            vn_conscontrpisor_id := 0;
            open c_pagtos_tpretrec;
            fetch c_pagtos_tpretrec into vn_conscontrpisor_id;
            close c_pagtos_tpretrec;
            --
            vn_fase := 113;
            --
            if nvl(vn_conscontrpisor_id,0) <> 0 then
               gv_resumo_log := 'Existe(m) tipo(s) de retenção(ões) de receita informado(s) em "Pagamentos" que não está(ão) relacionado(s) com o tipo de retenção.';
               pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                               , ev_mensagem        => gv_mensagem_log
                                               , ev_resumo          => gv_resumo_log
                                               , en_tipo_log        => erro_de_validacao
                                               , en_referencia_id   => gn_referencia_id
                                               , ev_obj_referencia  => gv_obj_referencia );
            end if;
            --
            vn_fase := 114;
            --
            if gv_resumo_log is null then
               --
               vn_fase := 115;
               --| Atualizar número do documento e descrição do ajuste - Bloco M220
               begin
                  update ajust_contr_pis_apur ac
                     set ac.num_doc  = ltrim(rtrim(pk_csf.fkg_converte(ac.num_doc)))
                       , ac.descr_aj = ltrim(rtrim(pk_csf.fkg_converte(ac.descr_aj)))
                   where ac.detconscontrpis_id in (select dc.id
                                                     from det_cons_contr_pis dc
                                                    where dc.conscontrpis_id = en_conscontrpis_id);
               exception
                  when others then
                     gv_resumo_log := 'Problemas ao atualizar número do documento e descrição do ajuste - Bloco M220. Erro = '||sqlerrm;
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                      , ev_mensagem        => gv_mensagem_log
                                                      , ev_resumo          => gv_resumo_log
                                                      , en_tipo_log        => erro_de_sistema
                                                      , en_referencia_id   => gn_referencia_id
                                                      , ev_obj_referencia  => gv_obj_referencia );
               end;
               --
            end if;
            --
         end if;
         --
      end if;
      --
   end if;
   --
   vn_fase := 116;
   --
   if gv_resumo_log is null then
      --
      vn_fase := 117;
      --| Atualizar a situação do Bloco M200
      begin
         update cons_contr_pis cc
            set cc.dm_situacao = 3 -- Processada
          where cc.id = en_conscontrpis_id;
      exception
         when others then
            gv_resumo_log := 'Problemas ao atualizar a situação do Bloco M200 para 3(Processada). Erro = '||sqlerrm;
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                             , ev_mensagem        => gv_mensagem_log
                                             , ev_resumo          => gv_resumo_log
                                             , en_tipo_log        => erro_de_sistema
                                             , en_referencia_id   => gn_referencia_id
                                             , ev_obj_referencia  => gv_obj_referencia );
      end;
      --
   else
      --
      vn_fase := 118;
      --| Atualizar a situação do Bloco M200
      begin
         update cons_contr_pis cc
            set cc.dm_situacao = 4 -- Erro de validação
          where cc.id = en_conscontrpis_id;
      exception
         when others then
            gv_resumo_log := 'Problemas ao atualizar a situação do Bloco M200 para 4(Erro de validação). Erro = '||sqlerrm;
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                             , ev_mensagem        => gv_mensagem_log
                                             , ev_resumo          => gv_resumo_log
                                             , en_tipo_log        => erro_de_sistema
                                             , en_referencia_id   => gn_referencia_id
                                             , ev_obj_referencia  => gv_obj_referencia );
      end;
      --
   end if;
   --
   vn_fase := 119;
   --
   commit;
   --
   vn_fase := 120;
   --
   gv_resumo_log := 'Término do processo (identificador da consolidação = '||en_conscontrpis_id||').';
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => gn_referencia_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
EXCEPTION
   when others then
      --
      begin
         update cons_contr_pis cc
            set cc.dm_situacao = 4 -- Erro de validação
          where cc.id = en_conscontrpis_id;
      exception
         when others then
            gv_resumo_log := 'Problemas (geral) ao atualizar a situação da consolidação para 4(Erro de validação). Erro = '||sqlerrm;
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                             , ev_mensagem        => gv_mensagem_log
                                             , ev_resumo          => gv_resumo_log
                                             , en_tipo_log        => erro_de_sistema
                                             , en_referencia_id   => en_conscontrpis_id
                                             , ev_obj_referencia  => gv_obj_referencia );
      end;
      --
      commit;
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.pkb_validar_cons_pis_m200 (fase = '||vn_fase||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => en_conscontrpis_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
END PKB_VALIDAR_CONS_PIS_M200;
-----------------------------------------------------------------------------
--| Procedimento para validar por período a consolidação do PIS - Bloco M200
--| Serão validados alguns campos dos blocos M200, M210 e M220
-----------------------------------------------------------------------------
PROCEDURE PKB_VAL_PER_CONS_PIS_M200( EN_PERCONSCONTRPIS_ID IN PER_CONS_CONTR_PIS.ID%TYPE ) IS
   --
   vn_fase           number := 0;
   vn_loggenerico_id log_generico.id%type;
   --
   cursor c_per_cons_contr_pis( en_perconscontrpis_id in per_cons_contr_pis.id%type ) is
      select *
        from per_cons_contr_pis pc
       where pc.id = en_perconscontrpis_id;
   --
   cursor c_cons is
      select cc.id conscontrpis_id
        from cons_contr_pis cc
       where cc.perconscontrpis_id = en_perconscontrpis_id;
BEGIN
   --
   vn_fase := 1;
   --
   pkb_excluir_log( en_referencia_id  => en_perconscontrpis_id
                  , ev_obj_referencia => 'PER_CONS_CONTR_PIS' );
   --
   vn_fase := 2;
   --
   gv_mensagem_log   := 'Validação por período das consolidações de contribuição do imposto PIS';
   gv_obj_referencia := 'PER_CONS_CONTR_PIS';
   gv_resumo_log     := 'Início do processo (identificador do período = '||en_perconscontrpis_id||').';
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => en_perconscontrpis_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
   gv_resumo_log := null;
   --
   vn_fase := 3;
   --
   if nvl(en_perconscontrpis_id,0) = 0 then
      --
      vn_fase := 4;
      gv_resumo_log := 'O identificador do período de consolidação deve ser informado para que a validação seja efetuada.';
      pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                       , ev_mensagem        => gv_mensagem_log
                                       , ev_resumo          => gv_resumo_log
                                       , en_tipo_log        => erro_de_validacao
                                       , en_referencia_id   => null
                                       , ev_obj_referencia  => gv_obj_referencia );
      --
   else
      --
      vn_fase := 5;
      --
      if pk_csf_efd_pc.fkg_qtde_cons_pis( en_perconscontrpis_id => en_perconscontrpis_id ) = 0 then -- devem existir consolidações geradas
         --
         vn_fase := 6;
         gv_resumo_log := 'O período de consolidação informado não possui consolidações geradas, portanto, a validação não será realizada.';
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_validacao
                                          , en_referencia_id   => en_perconscontrpis_id
                                          , ev_obj_referencia  => gv_obj_referencia );
         --
      else
         --
         vn_fase := 7;
         --
         gt_row_per_cons_contr_pis := null;
         open c_per_cons_contr_pis( en_perconscontrpis_id => en_perconscontrpis_id );
         fetch c_per_cons_contr_pis into gt_row_per_cons_contr_pis;
         close c_per_cons_contr_pis;
         --
         vn_fase := 8;
         --
         if pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => gt_row_per_cons_contr_pis.empresa_id
                                                  , ed_data       => gt_row_per_cons_contr_pis.dt_ini ) or  -- true
            pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => gt_row_per_cons_contr_pis.empresa_id
                                                  , ed_data       => gt_row_per_cons_contr_pis.dt_fin ) then -- true
            --
            vn_fase := 9;
            --
            gv_resumo_log := 'Existe(m) período(s) de abertura efd pis/cofins com situação: Validado ou Arquivo Gerado. Desprocesse a geração do arquivo.';
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                             , ev_mensagem        => gv_mensagem_log
                                             , ev_resumo          => gv_resumo_log
                                             , en_tipo_log        => erro_de_validacao
                                             , en_referencia_id   => gt_row_per_cons_contr_pis.id
                                             , ev_obj_referencia  => gv_obj_referencia );
            --
         else
            --
            vn_fase := 10;
            --
            for r_reg in c_cons
            loop
               --
               vn_fase := 11;
               --
               pkb_validar_cons_pis_m200( en_conscontrpis_id => r_reg.conscontrpis_id );
               --
            end loop;
            --
         end if;
         --
      end if;
      --
   end if;
   --
   vn_fase := 12;
   --
   commit;
   --
   vn_fase := 13;
   --
   gv_resumo_log := 'Término do processo (identificador do período = '||en_perconscontrpis_id||').';
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => en_perconscontrpis_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
EXCEPTION
   when others then
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.pkb_val_per_cons_pis_m200 (fase = '||vn_fase||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => 'Validar por período a consolidação da contribuição do PIS'
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => en_perconscontrpis_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
END PKB_VAL_PER_CONS_PIS_M200;
----------------------------------------------------------------------
--| Procedimento para desprocessar a consolidação do PIS - Bloco M200
----------------------------------------------------------------------
PROCEDURE PKB_DESPROCESSAR_CONS_PIS_M200 ( EN_CONSCONTRPIS_ID IN CONS_CONTR_PIS.ID%TYPE ) IS
   --
   vn_fase            number := 0;
   vn_loggenerico_id  log_generico.id%type;
   vn_empresa_id      per_cons_contr_pis.empresa_id%type;
   vd_dt_ini          per_cons_contr_pis.dt_ini%type;
   vd_dt_fin          per_cons_contr_pis.dt_fin%type;
   --
   cursor c_relac( en_empresa_id in number
                 , ed_dt_ini     in date ) is
      select ra.id relacapurcontrpis_id
           , ra.contrcredfiscalpis_id
           , ra.ano
           , ra.mes
           , ra.de_vl_sld_cred_fim
           , ra.de_vl_cred_desc_efd
           , ra.de_vl_sd_cred_disp_efd
           , ra.de_vl_cred_desc_pa_ant
        from relac_apur_contr_pis ra
       where ra.contrcredfiscalpis_id in (select cc.id
                                            from contr_cred_fiscal_pis cc
                                           where cc.empresa_id = en_empresa_id)
         and (ra.ano||lpad(ra.mes,2,'0')) = to_char(ed_dt_ini,'rrrrmm')
       order by (ra.ano||lpad(ra.mes,2,'0')) desc
           , ra.id desc;
   --
BEGIN
   --
   vn_fase := 1;
   --
   pkb_excluir_log( en_referencia_id  => en_conscontrpis_id
                  , ev_obj_referencia => 'CONS_CONTR_PIS' );
   --
   vn_fase := 2;
   --
   gv_obj_referencia := 'CONS_CONTR_PIS';
   gv_mensagem_log   := 'Desprocesso da consolidação do imposto PIS/PASEP';
   gv_resumo_log     := 'Início do processo (identificador da consolidação = '||en_conscontrpis_id||').';
   gn_referencia_id  := en_conscontrpis_id;
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => gn_referencia_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
   gv_resumo_log := null;
   --
   vn_fase := 3;
   --
   if nvl(en_conscontrpis_id,0) = 0 then
      --
      vn_fase := 4;
      gv_resumo_log := 'O identificador da consolidação deve ser informado para que o desprocessamento seja efetuado.';
      pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                       , ev_mensagem        => gv_mensagem_log
                                       , ev_resumo          => gv_resumo_log
                                       , en_tipo_log        => erro_de_validacao
                                       , en_referencia_id   => null
                                       , ev_obj_referencia  => gv_obj_referencia );
      --
   else
      --
      vn_fase := 5;
      -- Recupera os dados da consolidação do imposto pis utilizado como referência
      pkb_dados_cons_pis_m200( en_conscontrpis_id => en_conscontrpis_id );
      gv_mensagem_log := 'Desprocessar a '||gv_mensagem_log;
      --
      vn_fase := 6;
      --
      begin
         select pc.empresa_id
              , pc.dt_ini
              , pc.dt_fin
           into vn_empresa_id
              , vd_dt_ini
              , vd_dt_fin
           from per_cons_contr_pis pc
          where pc.id = gt_row_cons_contr_pis.perconscontrpis_id;
      exception
         when others then
            gv_resumo_log := 'Problemas ao recuperar dados do período da consolidação da contribuição do pis - bloco m200 - desprocessar. Erro = '||sqlerrm;
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                             , ev_mensagem        => gv_mensagem_log
                                             , ev_resumo          => gv_resumo_log
                                             , en_tipo_log        => erro_de_sistema
                                             , en_referencia_id   => gn_referencia_id
                                             , ev_obj_referencia  => gv_obj_referencia );
      end;
      --
      vn_fase := 7;
      --
      if pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => vn_empresa_id
                                               , ed_data       => vd_dt_ini ) or  -- true
         pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => vn_empresa_id
                                               , ed_data       => vd_dt_fin ) then -- true
         --
         vn_fase := 8;
         --
         gv_resumo_log := 'Existe(m) período(s) de abertura efd pis/cofins com situação: Validado ou Arquivo Gerado. Desprocesse a geração do arquivo.';
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_validacao
                                          , en_referencia_id   => gn_referencia_id
                                          , ev_obj_referencia  => gv_obj_referencia );
         --
      else
         --
         vn_fase := 9;
         -- Se o DM_SITUACAO = 0 "Em Aberto", manter situação = 0 "Em Aberto"
         if gt_row_cons_contr_pis.dm_situacao in (0) then
            --
            vn_fase := 10;
            --
            begin
               delete from contr_vlr_ret_fonte_pis cv
                where cv.empresa_id  = vn_empresa_id
                  and cv.ano         = to_char(vd_dt_ini,'rrrr')
                  and cv.mes         = to_char(vd_dt_ini,'mm')
                  and cv.per_rec_ret = to_char(vd_dt_ini,'mmrrrr')
                  and cv.dm_origem   = 1; -- 0-digitado, 1-gerado no bloco m200
            exception
               when others then
                  gv_resumo_log := 'Problemas ao excluir valor de retenção apurada e deduzida - Bloco 1300 (pk_apur_pis.pkb_desprocessar_cons_pis_m200) - '||
                                   'Situação anterior 0-Em Aberto. Erro = '||sqlerrm;
                  pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                   , ev_mensagem        => gv_mensagem_log
                                                   , ev_resumo          => gv_resumo_log
                                                   , en_tipo_log        => erro_de_sistema
                                                   , en_referencia_id   => gn_referencia_id
                                                   , ev_obj_referencia  => gv_obj_referencia );
            end;
            --
            vn_fase := 11;
            --
            begin
               delete from cons_contr_pis_or cc
                where cc.conscontrpis_id = gt_row_cons_contr_pis.id;
            exception
               when others then
                  gv_resumo_log := 'Problemas ao excluir Pagamentos - Bloco M205 (pk_apur_pis.pkb_desprocessar_cons_pis_m200) - '||
                                   'Situação anterior 0-Em Aberto. Erro = '||sqlerrm;
                  pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                   , ev_mensagem        => gv_mensagem_log
                                                   , ev_resumo          => gv_resumo_log
                                                   , en_tipo_log        => erro_de_sistema
                                                   , en_referencia_id   => gn_referencia_id
                                                   , ev_obj_referencia  => gv_obj_referencia );
            end;
            --
            vn_fase := 12;
            --
            begin
               delete from det_cons_contr_pis dc
                where dc.conscontrpis_id = gt_row_cons_contr_pis.id;
            exception
               when others then
                  gv_resumo_log := 'Problemas ao excluir detalhes da consolidação - Situação anterior 0-Em Aberto. Erro = '||sqlerrm;
                  pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                   , ev_mensagem        => gv_mensagem_log
                                                   , ev_resumo          => gv_resumo_log
                                                   , en_tipo_log        => erro_de_sistema
                                                   , en_referencia_id   => gn_referencia_id
                                                   , ev_obj_referencia  => gv_obj_referencia );
            end;
            --
            vn_fase := 13;
            --
            begin
               update cons_contr_pis cc
                  set cc.dm_situacao          = 0 -- Aberto
                    , cc.vl_tot_cont_nc_per   = 0
                    , cc.vl_tot_cred_desc     = 0
                    , cc.vl_tot_cred_desc_ant = 0
                    , cc.vl_tot_cont_nc_dev   = 0
                    , cc.vl_ret_nc            = 0
                    , cc.vl_out_ded_nc        = 0
                    , cc.vl_cont_nc_rec       = 0
                    , cc.vl_tot_cont_cum_per  = 0
                    , cc.vl_ret_cum           = 0
                    , cc.vl_out_ded_cum       = 0
                    , cc.vl_cont_cum_rec      = 0
                    , cc.vl_tot_cont_rec      = 0
                where cc.id = gt_row_cons_contr_pis.id;
            exception
               when others then
                  gv_resumo_log := 'Problemas ao zerar os valores e manter situação da consolidação 0(Em aberto) - Situação anterior 0-Em Aberto. '||
                                   'Erro = '||sqlerrm;
                  pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                   , ev_mensagem        => gv_mensagem_log
                                                   , ev_resumo          => gv_resumo_log
                                                   , en_tipo_log        => erro_de_sistema
                                                   , en_referencia_id   => gn_referencia_id
                                                   , ev_obj_referencia  => gv_obj_referencia );
            end;
         -- Se o DM_SITUACAO = 1 "Calculado" ou 2 "Erro no Cálculo", altera para 0 "Aberto"
         elsif gt_row_cons_contr_pis.dm_situacao in (1, 2) then
               --
               vn_fase := 14;
               --
               begin
                  delete from contr_vlr_ret_fonte_pis cv
                   where cv.empresa_id  = vn_empresa_id
                     and cv.ano         = to_char(vd_dt_ini,'rrrr')
                     and cv.mes         = to_char(vd_dt_ini,'mm')
                     and cv.per_rec_ret = to_char(vd_dt_ini,'mmrrrr')
                     and cv.dm_origem   = 1; -- 0-digitado, 1-gerado no bloco m200
               exception
                  when others then
                     gv_resumo_log := 'Problemas ao excluir valor de retenção apurada e deduzida - Bloco 1300 (pk_apur_pis.pkb_desprocessar_cons_pis_m200). '||
                                      'Situação anterior 1-Calculado ou 2-Erro no Cálculo. Erro = '||sqlerrm;
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                      , ev_mensagem        => gv_mensagem_log
                                                      , ev_resumo          => gv_resumo_log
                                                      , en_tipo_log        => erro_de_sistema
                                                      , en_referencia_id   => gn_referencia_id
                                                      , ev_obj_referencia  => gv_obj_referencia );
               end;
               --
               vn_fase := 15;
               --
               begin
                  delete from cons_contr_pis_or cc
                   where cc.conscontrpis_id = gt_row_cons_contr_pis.id;
               exception
                  when others then
                     gv_resumo_log := 'Problemas ao excluir Pagamentos - Bloco M205 (pk_apur_pis.pkb_desprocessar_cons_pis_m200) - '||
                                      'Situação anterior 1-Calculado ou 2-Erro no Cálculo. Erro = '||sqlerrm;
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                      , ev_mensagem        => gv_mensagem_log
                                                      , ev_resumo          => gv_resumo_log
                                                      , en_tipo_log        => erro_de_sistema
                                                      , en_referencia_id   => gn_referencia_id
                                                      , ev_obj_referencia  => gv_obj_referencia );
               end;
               --
               vn_fase := 16;
               --
               begin
                  update det_cons_contr_pis dc
                     set dc.vl_ajust_acrec    = 0
                       , dc.vl_ajust_reduc    = 0
                       , dc.vl_cont_difer     = 0
                       , dc.vl_cont_difer_ant = 0
                       , dc.vl_cont_per       = 0
                   where dc.conscontrpis_id = gt_row_cons_contr_pis.id;
               exception
                  when others then
                     gv_resumo_log := 'Problemas ao atualizar os valores dos detalhes da consolidação - Situação anterior 1-Calculado ou 2-Erro no Cálculo. '||
                                      'Erro = '||sqlerrm;
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                      , ev_mensagem        => gv_mensagem_log
                                                      , ev_resumo          => gv_resumo_log
                                                      , en_tipo_log        => erro_de_sistema
                                                      , en_referencia_id   => gn_referencia_id
                                                      , ev_obj_referencia  => gv_obj_referencia );
               end;
               --
               vn_fase := 17;
               -- Excluir/Alterar os relacionamentos entre os Blocos M100 e 1100
               for r_relac in c_relac( en_empresa_id => vn_empresa_id
                                     , ed_dt_ini     => vd_dt_ini )
               loop
                  --
                  vn_fase := 17.1;
                  -- Voltar os valores dos campos na tabela contr_cred_fiscal_pis, e excluir da tabela relac_apur_contr_pis.
                  begin
                     update contr_cred_fiscal_pis cc
                        set cc.vl_sld_cred_fim     = r_relac.de_vl_sld_cred_fim
                          , cc.vl_cred_desc_efd    = r_relac.de_vl_cred_desc_efd
                          , cc.vl_sd_cred_disp_efd = r_relac.de_vl_sd_cred_disp_efd
                          , cc.vl_cred_desc_pa_ant = r_relac.de_vl_cred_desc_pa_ant
                      where cc.id = r_relac.contrcredfiscalpis_id;
                  exception
                     when others then
                        gv_resumo_log := 'Problemas ao atualizar os valores dos Controle de Créditos Fiscais  Bloco 1100. Erro = '||sqlerrm;
                        pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                         , ev_mensagem        => gv_mensagem_log
                                                         , ev_resumo          => gv_resumo_log
                                                         , en_tipo_log        => erro_de_sistema
                                                         , en_referencia_id   => gn_referencia_id
                                                         , ev_obj_referencia  => gv_obj_referencia );
                  end;
                  --
                  vn_fase := 17.2;
                  -- Excluir os registro de relacionamento de valores entre os blocos m100 e 1100.
                  begin
                     delete from relac_apur_contr_pis ra
                      where ra.id = r_relac.relacapurcontrpis_id;
                  exception
                     when others then
                        gv_resumo_log := 'Problemas ao excluir os registro de relacionamento de valores entre os blocos m100 e 1100. Erro = '||sqlerrm;
                        pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                         , ev_mensagem        => gv_mensagem_log
                                                         , ev_resumo          => gv_resumo_log
                                                         , en_tipo_log        => erro_de_sistema
                                                         , en_referencia_id   => gn_referencia_id
                                                         , ev_obj_referencia  => gv_obj_referencia );
                  end;
                  --
               end loop;
               --
               vn_fase := 17.3;
               -- Excluir os registros da tabela r_apurcredpis_contrcredfpis (contr_cred_fiscal_pis (empresa, ano, mes, dm_origem)), de acordo com a data do registro a ser recalculado.
               begin
                  delete from r_apurcredpis_contrcredfpis ra
                   where ra.contrcredfiscalpis_id in (select cc.id
                                                        from contr_cred_fiscal_pis cc
                                                       where cc.empresa_id = vn_empresa_id
                                                         and cc.ano        = to_char(vd_dt_ini,'rrrr')
                                                         and cc.mes        = to_char(vd_dt_ini,'mm')
                                                         and cc.dm_origem  = 1); -- 0-digitado, 1-gerado pelo bloco m200
               exception
                  when others then
                     gv_resumo_log := 'Problemas ao excluir os registro de relacionamento entre os blocos m100 e 1100. Erro = '||sqlerrm;
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                      , ev_mensagem        => gv_mensagem_log
                                                      , ev_resumo          => gv_resumo_log
                                                      , en_tipo_log        => erro_de_sistema
                                                      , en_referencia_id   => gn_referencia_id
                                                      , ev_obj_referencia  => gv_obj_referencia );
               end;
               --
               vn_fase := 17.4;
               -- Excluir os registros da tabela contr_cred_fiscal_pis (empresa, ano, mes, dm_origem), de acordo com a data do registro a ser recalculado.
               begin
                  delete from contr_cred_fiscal_pis cc
                   where cc.empresa_id = vn_empresa_id
                     and cc.ano        = to_char(vd_dt_ini,'rrrr')
                     and cc.mes        = to_char(vd_dt_ini,'mm')
                     and cc.dm_origem  = 1; -- 0-digitado, 1-gerado pelo bloco m200
               exception
                  when others then
                     gv_resumo_log := 'Problemas ao excluir os registros de Controle de Créditos Fiscais  PIS/PASEP - Bloco 1100. Erro = '||sqlerrm;
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                      , ev_mensagem        => gv_mensagem_log
                                                      , ev_resumo          => gv_resumo_log
                                                      , en_tipo_log        => erro_de_sistema
                                                      , en_referencia_id   => gn_referencia_id
                                                      , ev_obj_referencia  => gv_obj_referencia );
               end;
               --
               vn_fase := 17.5;
               -- Alterar/Anular o relacionamento do registro 1100 que seria utilizado na geração do arquivo
               begin
                  update contr_cred_fiscal_pis cf
                     set cf.conscontrpis_id = null
                   where cf.conscontrpis_id = gt_row_cons_contr_pis.id;
               exception
                  when others then
                     gv_resumo_log := 'Problemas ao alterar os registros de Controle de Créditos Fiscais  PIS/PASEP - Bloco 1100. Erro = '||sqlerrm;
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                      , ev_mensagem        => gv_mensagem_log
                                                      , ev_resumo          => gv_resumo_log
                                                      , en_tipo_log        => erro_de_sistema
                                                      , en_referencia_id   => gn_referencia_id
                                                      , ev_obj_referencia  => gv_obj_referencia );
               end;
               --
               vn_fase := 18;
               --
               begin
                  update cons_contr_pis cc
                     set cc.dm_situacao          = 0 -- Aberto
                       , cc.vl_tot_cont_nc_per   = 0
                       , cc.vl_tot_cred_desc     = 0
                       , cc.vl_tot_cred_desc_ant = 0
                       , cc.vl_tot_cont_nc_dev   = 0
                       , cc.vl_ret_nc            = 0
                       , cc.vl_out_ded_nc        = 0
                       , cc.vl_cont_nc_rec       = 0
                       , cc.vl_tot_cont_cum_per  = 0
                       , cc.vl_ret_cum           = 0
                       , cc.vl_out_ded_cum       = 0
                       , cc.vl_cont_cum_rec      = 0
                       , cc.vl_tot_cont_rec      = 0
                   where cc.id = gt_row_cons_contr_pis.id;
               exception
                  when others then
                     gv_resumo_log := 'Problemas ao zerar os valores e alterar situação da consolidação para 0(Em aberto) - Situação anterior 1-Calculado '||
                                      'ou 2-Erro no Cálculo. Erro = '||sqlerrm;
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                      , ev_mensagem        => gv_mensagem_log
                                                      , ev_resumo          => gv_resumo_log
                                                      , en_tipo_log        => erro_de_sistema
                                                      , en_referencia_id   => gn_referencia_id
                                                      , ev_obj_referencia  => gv_obj_referencia );
               end;
         -- Se o DM_SITUACAO = 3 "Processada" ou 4 "Erro de Validação", altera para 1 "Calculada"
         elsif gt_row_cons_contr_pis.dm_situacao in (3, 4) then
               --
               vn_fase := 19;
               --
               begin
                  update cons_contr_pis cc
                     set cc.dm_situacao = 1 -- Calculado
                   where cc.id = gt_row_cons_contr_pis.id;
               exception
                  when others then
                     gv_resumo_log := 'Problemas ao atualizar a situação da consolidação para 1(Calculado). Erro = '||sqlerrm;
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                      , ev_mensagem        => gv_mensagem_log
                                                      , ev_resumo          => gv_resumo_log
                                                      , en_tipo_log        => erro_de_sistema
                                                      , en_referencia_id   => gn_referencia_id
                                                      , ev_obj_referencia  => gv_obj_referencia );
               end;
               --
         end if;
         --
      end if;
      --
   end if;
   --
   vn_fase := 20;
   --
   commit;
   --
   vn_fase := 21;
   --
   gv_resumo_log := 'Término do processo (identificador da consolidação = '||en_conscontrpis_id||').';
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => gn_referencia_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
EXCEPTION
   when others then
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.pkb_desprocessar_cons_pis_m200 (fase = '||vn_fase||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => en_conscontrpis_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
END PKB_DESPROCESSAR_CONS_PIS_M200;
----------------------------------------------------------------------------------
--| Procedimento para desprocessar por período a consolidação do PIS - Bloco M200
----------------------------------------------------------------------------------
PROCEDURE PKB_DESPR_PER_CONS_PIS_M200( EN_PERCONSCONTRPIS_ID IN PER_CONS_CONTR_PIS.ID%TYPE ) IS
   --
   vn_fase           number := 0;
   vn_loggenerico_id log_generico.id%type;
   --
   cursor c_per_cons_contr_pis( en_perconscontrpis_id in per_cons_contr_pis.id%type ) is
      select *
        from per_cons_contr_pis pc
       where pc.id = en_perconscontrpis_id;
   --
   cursor c_cons is
      select cc.id conscontrpis_id
        from cons_contr_pis cc
       where cc.perconscontrpis_id = en_perconscontrpis_id;
   --
BEGIN
   --
   vn_fase := 1;
   --
   pkb_excluir_log( en_referencia_id  => en_perconscontrpis_id
                  , ev_obj_referencia => 'PER_CONS_CONTR_PIS' );
   --
   vn_fase := 2;
   --
   gv_mensagem_log   := 'Desprocesso por período das consolidações da contribuição do imposto PIS';
   gv_obj_referencia := 'PER_CONS_CONTR_PIS';
   gv_resumo_log     := 'Início do processo (identificador do período = '||en_perconscontrpis_id||').';
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => en_perconscontrpis_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
   gv_resumo_log := null;
   --
   vn_fase := 3;
   --
   if nvl(en_perconscontrpis_id,0) = 0 then
      --
      vn_fase := 4;
      gv_resumo_log := 'O identificador do período de consolidação deve ser informado para que o desprocessamento seja efetuado.';
      pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                       , ev_mensagem        => gv_mensagem_log
                                       , ev_resumo          => gv_resumo_log
                                       , en_tipo_log        => erro_de_validacao
                                       , en_referencia_id   => null
                                       , ev_obj_referencia  => gv_obj_referencia );
      --
   else
      --
      vn_fase := 5;
      --
      gt_row_per_cons_contr_pis := null;
      open c_per_cons_contr_pis( en_perconscontrpis_id => en_perconscontrpis_id );
      fetch c_per_cons_contr_pis into gt_row_per_cons_contr_pis;
      close c_per_cons_contr_pis;
      --
      vn_fase := 6;
      --
      if pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => gt_row_per_cons_contr_pis.empresa_id
                                               , ed_data       => gt_row_per_cons_contr_pis.dt_ini ) or  -- true
         pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => gt_row_per_cons_contr_pis.empresa_id
                                               , ed_data       => gt_row_per_cons_contr_pis.dt_fin ) then -- true
         --
         vn_fase := 7;
         --
         gv_resumo_log := 'Existe(m) período(s) de abertura efd pis/cofins com situação: Validado ou Arquivo Gerado. Desprocesse a geração do arquivo.';
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_validacao
                                          , en_referencia_id   => gt_row_per_cons_contr_pis.id
                                          , ev_obj_referencia  => gv_obj_referencia );
         --
      else
         --
         vn_fase := 8;
         --
         for r_reg in c_cons
         loop
            --
            vn_fase := 9;
            --
            pkb_desprocessar_cons_pis_m200( en_conscontrpis_id => r_reg.conscontrpis_id );
            --
         end loop;
         --
         vn_fase := 10;
         --
         -- Exclui a memória de Calculo do M100 --
         pkb_exclui_mem_calc_pis ( en_perconscontrpis_id => gt_row_per_cons_contr_pis.id
                                 , ev_reg_controlador    => 'M200');
         --
      end if;
      --
   end if;
   --
   vn_fase := 11;
   --
   commit;
   --
   vn_fase := 12;
   --
   gv_resumo_log := 'Término do processo (identificador do período = '||en_perconscontrpis_id||').';
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => en_perconscontrpis_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
EXCEPTION
   when others then
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.pkb_despr_per_cons_pis_m200 (fase = '||vn_fase||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => 'Desprocessar por período a consolidação da contribuição do PIS'
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => en_perconscontrpis_id
                                          , ev_obj_referencia  => gv_obj_referencia );
