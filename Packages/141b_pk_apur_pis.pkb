--| Função para retornar o valor do campo 7 - VL_BC_PIS para o Bloco M105
--------------------------------------------------------------------------
FUNCTION FKG_RECUP_VL_BC_PIS( EN_SEQ_LANCTO   IN NUMBER
                            , EV_COD_ST       IN COD_ST.COD_ST%TYPE
                            , EN_VL_BC_PIS_NC IN NUMBER )
         RETURN NUMBER
IS
   --
   vn_fase      number := 0;
   vn_vl_bc_pis number := 0;
   --
BEGIN
   --
   vn_fase := 1;
   --
   if nvl(en_seq_lancto,0) = 1 then -- primeiro lançamento
      --
      vn_fase := 2;
      --
      if ev_cod_st in ('50','51','52','60','61','62') then
         --
         vn_fase := 3;
         vn_vl_bc_pis := nvl(en_vl_bc_pis_nc,0); -- campo 7 (vl_bc_pis) = campo 6 (vl_bc_pis_nc)
         --
      else
         --
         vn_fase := 4;
         --
         if gn_dm_ind_apro_cred = 1 then -- apropriação direta
            --
            vn_fase := 5;
            vn_vl_bc_pis := nvl(en_vl_bc_pis_nc,0); -- campo 7 (vl_bc_pis) = campo 6 (vl_bc_pis_nc)
            --
         else -- gn_dm_ind_apro_cred = 2 -- rateio proporcional
            --
            vn_fase := 6;
            --
            if ev_cod_st in ('53','63') then
               --
               vn_fase := 7;
               vn_vl_bc_pis := nvl(en_vl_bc_pis_nc,0) * gn_recbru_nc_trntr_tri; -- campo 7 (vl_bc_pis) = campo 6 (vl_bc_pis_nc) * rateio
               --
            elsif ev_cod_st in ('54','64') then
                  --
                  vn_fase := 8;
                  vn_vl_bc_pis := nvl(en_vl_bc_pis_nc,0) * gn_recbru_nc_trexp_tri; -- campo 7 (vl_bc_pis) = campo 6 (vl_bc_pis_nc) * rateio
                  --
            elsif ev_cod_st in ('55','65') then
                  --
                  vn_fase := 9;
                  vn_vl_bc_pis := nvl(en_vl_bc_pis_nc,0) * gn_recbru_nc_ntrexp_ntr; -- campo 7 (vl_bc_pis) = campo 6 (vl_bc_pis_nc) * rateio
                  --
            elsif ev_cod_st in ('56','66') then
                  --
                  vn_fase := 10;
                  vn_vl_bc_pis := nvl(en_vl_bc_pis_nc,0) * gn_recbru_nc_trntrexp_tri; -- campo 7 (vl_bc_pis) = campo 6 (vl_bc_pis_nc) * rateio
                  --
            else
               --
               vn_fase := 11;
               vn_vl_bc_pis := 0;
               --
            end if;
            --
         end if;
         --
      end if;
      --
   elsif nvl(en_seq_lancto,0) = 2 then -- segundo lançamento
         --
         vn_fase := 12;
         --
         if ev_cod_st in ('50','51','52','60','61','62') then
            --
            vn_fase := 13;
            vn_vl_bc_pis := 0;
            --
         else
            --
            vn_fase := 14;
            --
            if gn_dm_ind_apro_cred = 1 then -- apropriação direta
               --
               vn_fase := 15;
               vn_vl_bc_pis := nvl(en_vl_bc_pis_nc,0); -- campo 7 (vl_bc_pis) = campo 6 (vl_bc_pis_nc)
               --
            else -- gn_dm_ind_apro_cred = 2 -- rateio proporcional
               --
               vn_fase := 16;
               --
               if ev_cod_st in ('53','63') then
                  --
                  vn_fase := 17;
                  vn_vl_bc_pis := nvl(en_vl_bc_pis_nc,0) * gn_recbru_nc_trntr_ntr; -- campo 7 (vl_bc_pis) = campo 6 (vl_bc_pis_nc) * rateio
                  --
               elsif ev_cod_st in ('54','64') then
                     --
                     vn_fase := 18;
                     vn_vl_bc_pis := nvl(en_vl_bc_pis_nc,0) * gn_recbru_nc_trexp_exp; -- campo 7 (vl_bc_pis) = campo 6 (vl_bc_pis_nc) * rateio
                     --
               elsif ev_cod_st in ('55','65') then
                     --
                     vn_fase := 19;
                     vn_vl_bc_pis := nvl(en_vl_bc_pis_nc,0) * gn_recbru_nc_ntrexp_exp; -- campo 7 (vl_bc_pis) = campo 6 (vl_bc_pis_nc) * rateio
                     --
               elsif ev_cod_st in ('56','66') then
                     --
                     vn_fase := 20;
                     vn_vl_bc_pis := nvl(en_vl_bc_pis_nc,0) * gn_recbru_nc_trntrexp_ntr; -- campo 7 (vl_bc_pis) = campo 6 (vl_bc_pis_nc) * rateio
                     --
               else
                  --
                  vn_fase := 21;
                  vn_vl_bc_pis := 0;
                  --
               end if;
               --
            end if;
            --
         end if;
         --
   elsif nvl(en_seq_lancto,0) = 3 then -- terceiro lançamento
         --
         vn_fase := 22;
         --
         if ev_cod_st in ('50','51','52','60','61','62','53','63','54','64','55','65') then
            --
            vn_fase := 23;
            vn_vl_bc_pis := 0;
            --
         else
            --
            vn_fase := 24;
            --
            if gn_dm_ind_apro_cred = 1 then -- apropriação direta
               --
               vn_fase := 25;
               vn_vl_bc_pis := nvl(en_vl_bc_pis_nc,0); -- campo 7 (vl_bc_pis) = campo 6 (vl_bc_pis_nc)
               --
            else -- gn_dm_ind_apro_cred = 2 -- rateio proporcional
               --
               vn_fase := 26;
               --
               if ev_cod_st in ('56','66') then
                  --
                  vn_fase := 27;
                  vn_vl_bc_pis := nvl(en_vl_bc_pis_nc,0) * gn_recbru_nc_trntrexp_exp; -- campo 7 (vl_bc_pis) = campo 6 (vl_bc_pis_nc) * rateio
                  --
               else
                  --
                  vn_fase := 28;
                  vn_vl_bc_pis := 0;
                  --
               end if;
               --
            end if;
            --
         end if;
         --
   end if;
   --
   vn_fase := 29;
   --
   return(nvl(vn_vl_bc_pis,0));
   --
EXCEPTION
   WHEN OTHERS THEN
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.fkg_recup_vl_bc_pis (fase = '||vn_fase||' en_seq_lancto = '||en_seq_lancto||' ev_cod_st = '||ev_cod_st||
                       ' en_vl_bc_pis_nc = '||en_vl_bc_pis_nc||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => gn_referencia_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
END FKG_RECUP_VL_BC_PIS;
