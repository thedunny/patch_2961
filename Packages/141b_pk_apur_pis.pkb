--| Procedimento para armazenar os dados em vetores - Bloco M200
-----------------------------------------------------------------
procedure pkb_monta_vetor_m200(en_ch_conc_cs_cr_aliq in number,
                               en_contrsocapurpc_id  in number,
                               en_aliq_pis           in number,
                               en_aliq_pis_quant     in number,
                               en_vl_rec_brt         in number,
                               en_vl_bc_cont         in number,
                               en_quant_bc_pis       in number,
                               en_vl_cont_apur       in number ) is
   --
   vn_fase                    number := 0;
   vb_achou                   boolean := false;
   vn_conscontrpis_id         number;
   vn_ch_conc_cs_cr_aliq      number;
   vn_ch_conc_cs_cr_aliq_m200 number := 1;
   --
begin
   --
   vn_fase := 1;
   --
   --| Montar bloco M200
   begin
      vb_achou := vt_tab_reg_m200.exists(vn_ch_conc_cs_cr_aliq_m200);
   exception
      when others then
         vb_achou := false;
   end;
   --
   vn_fase := 2;
   --
   if not vb_achou then
      --
      vn_fase := 3;
      --
      begin
         select conscontrpis_seq.nextval
           into vn_conscontrpis_id
           from dual;
      exception
         when others then
            vn_conscontrpis_id := null;
      end;
      --
      vn_fase := 4;
      --
      if nvl(vn_conscontrpis_id,0) > 0 then
         --
         vn_fase := 5;
         --
         vt_tab_reg_m200(vn_ch_conc_cs_cr_aliq_m200).conscontrpis_id      := vn_conscontrpis_id;
         vt_tab_reg_m200(vn_ch_conc_cs_cr_aliq_m200).dm_situacao          := 0; -- 0-aberto
         vt_tab_reg_m200(vn_ch_conc_cs_cr_aliq_m200).vl_tot_cont_nc_per   := 0; -- processo de cálculo
         vt_tab_reg_m200(vn_ch_conc_cs_cr_aliq_m200).vl_tot_cred_desc     := 0; -- processo de cálculo
         vt_tab_reg_m200(vn_ch_conc_cs_cr_aliq_m200).vl_tot_cred_desc_ant := 0; -- processo de cálculo
         vt_tab_reg_m200(vn_ch_conc_cs_cr_aliq_m200).vl_tot_cont_nc_dev   := 0; -- processo de cálculo
         vt_tab_reg_m200(vn_ch_conc_cs_cr_aliq_m200).vl_ret_nc            := 0; -- processo de cálculo
         vt_tab_reg_m200(vn_ch_conc_cs_cr_aliq_m200).vl_out_ded_nc        := 0; -- processo de cálculo
         vt_tab_reg_m200(vn_ch_conc_cs_cr_aliq_m200).vl_cont_nc_rec       := 0; -- processo de cálculo
         vt_tab_reg_m200(vn_ch_conc_cs_cr_aliq_m200).vl_tot_cont_cum_per  := 0; -- processo de cálculo
         vt_tab_reg_m200(vn_ch_conc_cs_cr_aliq_m200).vl_ret_cum           := 0; -- processo de cálculo
         vt_tab_reg_m200(vn_ch_conc_cs_cr_aliq_m200).vl_out_ded_cum       := 0; -- processo de cálculo
         vt_tab_reg_m200(vn_ch_conc_cs_cr_aliq_m200).vl_cont_cum_rec      := 0; -- processo de cálculo
         vt_tab_reg_m200(vn_ch_conc_cs_cr_aliq_m200).vl_tot_cont_rec      := 0; -- processo de cálculo
         --
      end if;
      --
   end if;
   --
   vn_fase := 6;
   --
   if vn_ch_conc_cs_cr_aliq_m200 is not null then -- gerar os detalhes da consolidação
      --
      vn_fase := 7;
      --
      vn_conscontrpis_id    := vt_tab_reg_m200(vn_ch_conc_cs_cr_aliq_m200).conscontrpis_id;
      vn_ch_conc_cs_cr_aliq := en_contrsocapurpc_id||replace(replace(nvl(en_aliq_pis,0),',',''),'.','')||
                               replace(replace(nvl(en_aliq_pis_quant,0),',',''),'.','');
      --
      vn_fase := 8;
      --| Montar bloco M210
      begin
         vb_achou := vt_bi_tab_reg_m210(vn_conscontrpis_id).exists(vn_ch_conc_cs_cr_aliq);
      exception
         when others then
            vb_achou := false;
      end;
      --
      vn_fase := 9;
      --
      if not vb_achou then
         --
         vn_fase := 10;
         --
         begin
            select detconscontrpis_seq.nextval
              into vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).detconscontrpis_id
              from dual;
         exception
            when others then
               vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).detconscontrpis_id := null;
         end;
         --
         vn_fase := 11;
         --
         if nvl(vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).detconscontrpis_id,0) > 0 then
            --
            vn_fase := 12;
            --
            vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).conscontrpis_id       := vn_conscontrpis_id;
            vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).contrsocapurpc_id     := en_contrsocapurpc_id;
            vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).aliq_pis              := en_aliq_pis;
            vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).aliq_pis_quant        := en_aliq_pis_quant;
            vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).vl_rec_brt            := en_vl_rec_brt;
            vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).vl_bc_cont            := en_vl_bc_cont;
            vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).quant_bc_pis          := en_quant_bc_pis;
            vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).vl_cont_apur          := en_vl_cont_apur;
            vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).vl_ajust_acrec        := 0; -- processo de cálculo
            vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).vl_ajust_reduc        := 0; -- processo de cálculo
            vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).vl_cont_difer         := 0; -- processo de cálculo
            vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).vl_cont_difer_ant     := 0; -- processo de cálculo
            vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).vl_cont_per           := 0; -- processo de cálculo
            vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).vl_ajus_acres_bc_pis  := 0; -- processo de cálculo
            vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).vl_ajus_reduc_bc_pis  := 0; -- processo de cálculo
            vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).vl_bc_cont_ajus       := 0; -- processo de cálculo
            --
         end if;
      else
         --
         vn_fase := 13;
         --
         vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).vl_rec_brt   := vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).vl_rec_brt   + en_vl_rec_brt;
         vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).vl_bc_cont   := vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).vl_bc_cont   + en_vl_bc_cont;
         vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).quant_bc_pis := vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).quant_bc_pis + en_quant_bc_pis;
         vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).vl_cont_apur := vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).vl_cont_apur + en_vl_cont_apur;
         --
      end if;
      --
   end if; -- gerar somente o cabeçalho da consolidação, não gerar os detalhes
   --
exception
   when others then
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.pkb_monta_vetor_m200 (fase = '||vn_fase||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => gn_referencia_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
end pkb_monta_vetor_m200;
-----------------------------------------------------------------
--| Procedimento para armazenar os dados em vetores - Bloco M220
-----------------------------------------------------------------
PROCEDURE PKB_MONTA_VETOR_M220( EN_CH_CONC_CS_CR_ALIQ IN NUMBER
                              , EN_CONTRSOCAPURPC_ID  IN NUMBER
                              , EN_DM_IND_AJ          IN NUMBER
                              , EN_ALIQ_PIS           IN NUMBER
                              , EN_ALIQ_PIS_QUANT     IN NUMBER
                              , EN_VL_AJUSTE          IN NUMBER
                              , EN_CD_CFOP            IN NUMBER ) IS
   --
   vn_fase               number := 0;
   vb_achou              boolean := false;
   vn_conscontrpis_id    number;
   vn_ch_conc_cs_cr_aliq number;
   vn_detconscontrpis_id number;
   --
BEGIN
   --
   vn_fase := 1;
   --| Recuperar bloco M200
   begin
      vb_achou := vt_tab_reg_m200.exists(en_ch_conc_cs_cr_aliq);
   exception
      when others then
         vb_achou := false;
   end;
   --
   vn_fase := 2;
   --
   if not vb_achou then
      --
      vn_fase := 3;
      --
      pkb_monta_vetor_m200( en_ch_conc_cs_cr_aliq => en_ch_conc_cs_cr_aliq
                          , en_contrsocapurpc_id  => en_contrsocapurpc_id
                          , en_aliq_pis           => en_aliq_pis
                          , en_aliq_pis_quant     => en_aliq_pis_quant
                          , en_vl_rec_brt         => 0
                          , en_vl_bc_cont         => 0
                          , en_quant_bc_pis       => 0
                          , en_vl_cont_apur       => 0 );
      --
      vn_fase := 4;
      --| Recuperar bloco M200
      begin
         vb_achou := vt_tab_reg_m200.exists(en_ch_conc_cs_cr_aliq);
      exception
         when others then
            vb_achou := false;
      end;
      --
   end if;
   --
   vn_fase := 5;
   --
   if vb_achou then
      --
      vn_fase := 6;
      --
      vn_conscontrpis_id    := vt_tab_reg_m200(en_ch_conc_cs_cr_aliq).conscontrpis_id;
      vn_ch_conc_cs_cr_aliq := en_contrsocapurpc_id||replace(replace(nvl(en_aliq_pis,0),',',''),'.','')||replace(replace(nvl(en_aliq_pis_quant,0),',',''),'.','');
      --
      vn_fase := 7;
      --| Recuperar bloco M210
      begin
         vb_achou := vt_bi_tab_reg_m210(vn_conscontrpis_id).exists(vn_ch_conc_cs_cr_aliq);
      exception
         when others then
            vb_achou := false;
      end;
      --
      vn_fase := 8;
      --
      if vb_achou then
         --
         vn_fase := 9;
         --
         vn_detconscontrpis_id := vt_bi_tab_reg_m210(vn_conscontrpis_id)(vn_ch_conc_cs_cr_aliq).detconscontrpis_id;
         --
         vn_fase := 10;
         --| Montar bloco M220
         begin
            vb_achou := vt_tri_tab_reg_m220(vn_conscontrpis_id)(vn_detconscontrpis_id).exists(en_dm_ind_aj);
         exception
            when others then
               vb_achou := false;
         end;
         --
         vn_fase := 11;
         --
         if not vb_achou then
            --
            vn_fase := 12;
            --
            begin
               select ajustcontrpisapur_seq.nextval
                 into vt_tri_tab_reg_m220(vn_conscontrpis_id)(vn_detconscontrpis_id)(en_dm_ind_aj).ajustcontrpisapur_id
                 from dual;
            exception
               when others then
                  vt_tri_tab_reg_m220(vn_conscontrpis_id)(vn_detconscontrpis_id)(en_dm_ind_aj).ajustcontrpisapur_id := null;
            end;
            --
            vn_fase := 13;
            --
            if nvl(vt_tri_tab_reg_m220(vn_conscontrpis_id)(vn_detconscontrpis_id)(en_dm_ind_aj).ajustcontrpisapur_id,0) <> 0 then
               --
               vn_fase := 14;
               --
               vt_tri_tab_reg_m220(vn_conscontrpis_id)(vn_detconscontrpis_id)(en_dm_ind_aj).dm_ind_aj := en_dm_ind_aj;
               vt_tri_tab_reg_m220(vn_conscontrpis_id)(vn_detconscontrpis_id)(en_dm_ind_aj).vl_ajuste := nvl(en_vl_ajuste,0);
               vt_tri_tab_reg_m220(vn_conscontrpis_id)(vn_detconscontrpis_id)(en_dm_ind_aj).cd_cfop   := en_cd_cfop;
               --
            end if;
            --
         else
            --
            vn_fase := 15;
            --
            vt_tri_tab_reg_m220(vn_conscontrpis_id)(vn_detconscontrpis_id)(en_dm_ind_aj).vl_ajuste := nvl(vt_tri_tab_reg_m220(vn_conscontrpis_id)(vn_detconscontrpis_id)(en_dm_ind_aj).vl_ajuste,0) + nvl(en_vl_ajuste,0);
            vt_tri_tab_reg_m220(vn_conscontrpis_id)(vn_detconscontrpis_id)(en_dm_ind_aj).cd_cfop   := vt_tri_tab_reg_m220(vn_conscontrpis_id)(vn_detconscontrpis_id)(en_dm_ind_aj).cd_cfop||', '||en_cd_cfop;
            --
         end if; -- Montar bloco M220
         --
      end if; -- Recuperar bloco M210
      --
   end if; -- Recuperar bloco M200
   --
EXCEPTION
   when others then
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.pkb_monta_vetor_m220 (fase = '||vn_fase||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => gn_referencia_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
END PKB_MONTA_VETOR_M220;
---------------------------------------------------------------------------------------------------------
--| Procedimento recupera os dados dos processos - Bloco M200 - Blocos F - Demais Documentos e Operações
---------------------------------------------------------------------------------------------------------
PROCEDURE PKB_MONTA_DADOS_M200_F IS
   --
   vn_fase               number := 0;
   vn_contrsocapurpc_id  contr_soc_apur_pc.id%type;
   vv_cd_contrsocapurpc  contr_soc_apur_pc.cd%type;
   vn_vl_rec_brt         number := 0;
   vn_vl_bc_cont         number := 0;
   vn_vl_cont_apur       number := 0;
   vn_quant_bc_pis       number := 0;
   vv_cabec_log          varchar2(1000) := null;
   vn_ch_conc_cs_cr_aliq number := 0;
   vn_cfop_id            number := 0;
   vn_dm_ind_aj          number := 0;
   vn_vl_aj              number := 0;
   vt_mem_calc_apur_pis  mem_calc_apur_pis%rowtype;
   vn_registrefdpc_id    number := null;
   --
   --| Recuperar a empresa e suas filiais
   cursor c_empresa is
      select em.id empresa_id
           , em.dm_dt_escr_dfepoe
        from empresa em
       start with em.id = gt_row_per_cons_contr_pis.empresa_id
     connect by prior em.id = em.ar_empresa_id;
   --
   --| DEMAIS DOCUMENTOS E OPERAÇÕES GERADORAS DE CONTRIBUIÇÃO E CRÉDITOS -- para consolidação
   cursor c_f100( en_empresa_id in empresa.id%type ) is
      select dd.id demdocopergercc_id
           , dd.codst_id_pis codst_id
           , dd.dm_ind_orig_cred
           , dd.dm_ind_oper
           , dd.dt_oper
           , nvl(dd.aliq_pis,0)  aliq_pis
           , nvl(dd.vl_oper,0)   vl_rec_brt
           , nvl(dd.vl_bc_pis,0) vl_bc_cont
           , nvl(dd.vl_pis,0)    vl_pis
           , 0 quant_bc_pis
           , 0 aliq_pis_quant
           , cst.cod_st
           , case when dm_ind_oper = 0 then 'C'
                  else 'D'
                  end tipo_operacao
           , 0                   nro_item
           , 0                   dm_ind_emit
           , 0                   qtde_base_calc_prod
        from dem_doc_oper_ger_cc dd
	   , cod_st              cst
       where dd.empresa_id     = en_empresa_id
         and dd.dt_oper  between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
         and dd.dm_st_proc     = 1 -- validado
         and dd.dm_ind_oper   in (1,2)
	 and cst.id            = dd.codst_id_pis
       order by dd.id
           , dd.codst_id_pis
           , dd.dm_ind_orig_cred
           , nvl(dd.aliq_pis,0);
   --
   -- VL_TOT_REC do registro F200 - Atividade Imobiliária
   cursor c_f200( en_empresa_id in empresa.id%type ) is
      select oa.id operativimobvend_id
           , oa.codst_id_pis codst_id
           , oa.dm_ind_oper
           , oa.dt_oper
           , nvl(oa.aliq_pis,0)   aliq_pis
           , nvl(oa.vl_tot_rec,0) vl_rec_brt
           , nvl(oa.vl_bc_pis,0)  vl_bc_cont
           , nvl(oa.vl_pis,0)     vl_pis
           , 0 quant_bc_pis
           , 0 aliq_pis_quant
           , cs.cod_st
           , 'C'                 tipo_operacao
           , 0                   nro_item
           , 0                   dm_ind_emit
           , 0                   qtde_base_calc_prod
        from oper_ativ_imob_vend oa
           , cod_st              cs
       where oa.empresa_id    = en_empresa_id
         and oa.dt_oper between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
         and oa.dm_st_proc    = 1 -- validado
         and cs.id            = oa.codst_id_pis
       order by oa.id
           , oa.codst_id_pis
           , nvl(oa.aliq_pis,0);
   --
   -- VL_REC_CAIXA do registro F500 - Consolidação das Operações da Pessoa Jurídica Submetida ao Regime de Tributação com Base no Lucro Presumido  Incidência do PIS/Pasep e da Cofins pelo Regime de Caixa
   cursor c_f500( en_empresa_id in empresa.id%type ) is
      select co.id consoperinspcrc_id
           , co.codst_id_pis codst_id
           , co.dt_ref
           , nvl(co.aliq_pis,0)     aliq_pis
           , nvl(co.vl_rec_caixa,0) vl_rec_brt
           , nvl(co.vl_bc_pis,0)    vl_bc_cont
           , nvl(co.vl_pis,0)       vl_pis
           , 0 quant_bc_pis
           , 0 aliq_pis_quant
           , cs.cod_st
           , 'C'                    tipo_operacao
           , 0                      nro_item
           , 0                      dm_ind_emit
           , 0                      qtde_base_calc_prod
        from cons_oper_ins_pc_rc co
           , cod_st              cs
       where co.empresa_id   = en_empresa_id
         and co.dt_ref between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
         and co.dm_st_proc   = 1 -- validado
         and cs.id           = co.codst_id_pis;
   --
   -- VL_BC_PIS do registro F550 - CONSOL.DAS OPER.DA PJ SUBMETIDA AO REGIME DE TRIB.BASE LUCRO PRESUMIDO  INCIDÊNCIA DO PIS/PASEP E DA COFINS REG.COMPETÊNCIA
   cursor c_f550( en_empresa_id in empresa.id%type ) is
      select co.id consoperinspcrcomp_id
           , co.codst_id_pis codst_id
           , co.dt_ref
           , nvl(co.aliq_pis,0)    aliq_pis
           , nvl(co.vl_rec_comp,0) vl_rec_brt
           , nvl(co.vl_bc_pis,0)   vl_bc_cont
           , nvl(co.vl_pis,0)      vl_pis
           , 0 quant_bc_pis
           , 0 aliq_pis_quant
           , cs.cod_st
           , 'C'                   tipo_operacao
           , 0                     nro_item
           , 0                     dm_ind_emit
           , 0                     qtde_base_calc_prod
        from cons_oper_ins_pc_rcomp co
           , cod_st                 cs
       where co.empresa_id   = en_empresa_id
         and co.dt_ref between gt_row_per_cons_contr_pis.dt_ini and gt_row_per_cons_contr_pis.dt_fin
         and co.dm_st_proc   = 1 -- validado
         and cs.id           = co.codst_id_pis
       order by co.id
           , co.codst_id_pis
           , nvl(co.aliq_pis,0);
   --
BEGIN
   --
   vn_fase := 1;
   --| Demais documentos e operações geradoras de contribuição e créditos - Bloco F100, F200, F525 e F550
   for r_empresa in c_empresa
   loop
      --
      vn_fase := 2;
      --
      if ((nvl(gn_dm_cod_inc_trib,0) in (1,3)) or -- 1-Escrituração de operações com incidência exclusivamente no regime não-cumulativo ou 3-Escrituração de operações com incidência nos regimes nãocumulativo e cumulativo
          (nvl(gn_dm_cod_inc_trib,0) = 2 and gn_dm_ind_reg_cum = 9)) then -- 2-Escrituração de operações com incidência exclusivamente no regime cumulativo, 9-Regime de Competência - Escrituração detalhada, com base nos registros dos Blocos A, C, D e F
         --
         vn_fase := 3;
         -- processo para consolidação - Bloco F100
         for r_reg in c_f100( en_empresa_id => r_empresa.empresa_id )
         loop
            --
            exit when c_f100%notfound or (c_f100%notfound is null);
            --
            vn_fase := 3.1;
            --
            vn_contrsocapurpc_id := pk_csf_efd_pc.fkg_relac_cons_contr_id( en_tipoimp_id      => gn_tipoimp_id                 -- identificador do tipo de imposto
                                                                         , en_ind_orig_cred   => nvl(r_reg.dm_ind_orig_cred,0) -- indicador de crédito 0-Oper.Mercado Interno, 1-Oper.Importação
                                                                         , en_codst_id        => r_reg.codst_id                -- identificador do código ST
                                                                         , en_vl_aliq         => r_reg.aliq_pis                -- valor de alíquota em percentual
                                                                         , en_vl_aliq_quant   => 0                             -- valor da alíquota por unidade de produto
                                                                         , en_dm_cod_inc_trib => gn_dm_cod_inc_trib );         -- indicador da incidência tributária
            --
            vn_fase := 3.2;
            --
            if nvl(vn_contrsocapurpc_id,0) <> 0 then
               --
               vn_fase := 3.3;
               --
               vv_cd_contrsocapurpc := pk_csf_efd_pc.fkg_cd_contr_soc_apur_pc( en_contrsocapurpc_id => vn_contrsocapurpc_id );
               --
               vn_fase := 3.4;
               --
               if vv_cd_contrsocapurpc in ('01', '51', '02', '52', '31', '32') and
                  (r_reg.cod_st in ('01', '02', '03', '04', '05') and nvl(r_reg.aliq_pis,0) <> 0) or
                  (r_reg.cod_st in ('49')) then
                  vn_vl_rec_brt := r_reg.vl_rec_brt;
               else
                  vn_vl_rec_brt := 0;
               end if;
               --
               vn_fase := 3.5;
               -- 03-Contribuição não-cumulativa apurada a alíquota por unidade de medida de produto
               -- 53-Contribuição cumulativa apurada a alíquota por unidade de medida de produto
               if vv_cd_contrsocapurpc in ('03', '53') then
                  vn_vl_bc_cont := 0;
               elsif gv_dm_ind_nat_pj in ('00', '02', '03', '05') and -- 00-Sociedade empresária em geral, 02-Entidade sujeita ao PIS/Pasep exclusivamente com base na Folha de Salários
                     vv_cd_contrsocapurpc in ('01', '51', '02', '52', '31', '32') then
                     vn_vl_bc_cont := r_reg.vl_bc_cont;
               else
                  vn_vl_bc_cont := 0;
               end if;
               --
               vn_fase := 3.6;
               --
               if vv_cd_contrsocapurpc in ('01', '51', '02', '52', '31') then
                  vn_quant_bc_pis := null;
               elsif vv_cd_contrsocapurpc in ('03', '53', '32') then
                     vn_quant_bc_pis := r_reg.quant_bc_pis;
               else
                  vn_quant_bc_pis := null;
               end if;
               --
               vn_fase := 3.7;
               --
               if nvl(vn_vl_bc_cont,0) <> 0 then
                  vn_vl_cont_apur := round(((nvl(vn_vl_bc_cont,0) * nvl(r_reg.aliq_pis,0)) / 100),2);
               elsif nvl(r_reg.quant_bc_pis,0) <> 0 then
                     vn_vl_cont_apur := round((nvl(vn_quant_bc_pis,0) * nvl(r_reg.aliq_pis_quant,0)),2);
               else
                  vn_vl_cont_apur := 0;
               end if;
               --
               vn_fase := 3.8;
               --
               if vv_cd_contrsocapurpc in ('01', '02', '03', '04', '32', '71') then
                  vn_ch_conc_cs_cr_aliq := 1;
               elsif vv_cd_contrsocapurpc in ('51') then
                     vn_ch_conc_cs_cr_aliq := 1;
               else
                  vn_ch_conc_cs_cr_aliq := vv_cd_contrsocapurpc;
               end if;
               --
               vn_fase := 3.9;
               --
               pkb_monta_vetor_m200( en_ch_conc_cs_cr_aliq => vn_ch_conc_cs_cr_aliq
                                   , en_contrsocapurpc_id  => vn_contrsocapurpc_id
                                   , en_aliq_pis           => nvl(r_reg.aliq_pis,0)
                                   , en_aliq_pis_quant     => nvl(r_reg.aliq_pis_quant,0)
                                   , en_vl_rec_brt         => nvl(vn_vl_rec_brt,0)
                                   , en_vl_bc_cont         => nvl(vn_vl_bc_cont,0)
                                   , en_quant_bc_pis       => nvl(vn_quant_bc_pis,0)
                                   , en_vl_cont_apur       => nvl(vn_vl_cont_apur,0) );
               --
            else
               --
               vn_fase := 3.10;
               --
               vv_cabec_log := '. Demais documentos - Bloco F100 -> Indicador de operação: '||pk_csf.fkg_dominio( ev_dominio => 'DEM_DOC_OPER_GER_CC.DM_IND_OPER'
                                                                                                                , ev_vl      => r_reg.dm_ind_oper )||
                               ' Data da operação: '||to_char(r_reg.dt_oper,'dd/mm/rrrr')||' Código de ST = '||pk_csf.fkg_cod_st_cod( en_id_st => r_reg.codst_id );
               --
               gv_resumo_log := 'Demais documentos e operações geradoras de contribuição e créditos - Bloco F100. Código de contribuição social não encontrado. '||
                                'Parâmetros: Tipo de imposto = PIS, Indicador de crédito = '||r_reg.dm_ind_orig_cred||', Código de ST = '||
                                pk_csf.fkg_cod_st_cod( en_id_st => r_reg.codst_id )||', Alíquota em % = '||to_char(r_reg.aliq_pis,'999G990D0000')||
                                ', Alíquota em qtde = 0,000, Indicador da incidência tributária = '||gn_dm_cod_inc_trib||'.';
               --
               declare
                  vn_loggenerico_id  log_generico.id%type;
               begin
                  pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                   , ev_mensagem        => gv_mensagem_log||vv_cabec_log
                                                   , ev_resumo          => gv_resumo_log
                                                   , en_tipo_log        => erro_de_validacao
                                                   , en_referencia_id   => gn_referencia_id
                                                   , ev_obj_referencia  => gv_obj_referencia );
               exception
                  when others then
                     null;
               end;
               --
            end if;
            --
            -- Monta a memória de calculo --------------------------------
            vn_registrefdpc_id                       := fkg_ret_registrefdpc_id('F100');
            vt_mem_calc_apur_pis                     := null;
            vt_mem_calc_apur_pis.perconscontrpis_id  := gt_row_per_cons_contr_pis.id;
            vt_mem_calc_apur_pis.registrefdpc_id     := vn_registrefdpc_id;
            vt_mem_calc_apur_pis.referencia_id       := r_reg.demdocopergercc_id;
            vt_mem_calc_apur_pis.ref_item_id         := null;
            vt_mem_calc_apur_pis.tipo_operacao       := r_reg.tipo_operacao;
            vt_mem_calc_apur_pis.ident_doc           := 'Demais Documentos ID (F100): '||r_reg.demdocopergercc_id; --69023
            vt_mem_calc_apur_pis.dm_ind_emit         := r_reg.dm_ind_emit;
            vt_mem_calc_apur_pis.vl_base_calc        := r_reg.vl_bc_cont;
            vt_mem_calc_apur_pis.aliq_perc           := nvl(r_reg.aliq_pis, 0);
            vt_mem_calc_apur_pis.aliq_prod           := 0;
            vt_mem_calc_apur_pis.vl_imp_trib         := nvl(r_reg.vl_pis, 0);
            vt_mem_calc_apur_pis.reg_controlador     := 'M200';
            pkb_monta_vetor_mem_calc_pis(vt_mem_calc_apur_pis);
            --
         end loop; -- cursor c_f100
         --
      end if;
      --
      vn_fase := 4;
      -- processo para consolidação - Bloco F200 - independe do indicador de regime de competência
      for r_reg in c_f200( en_empresa_id => r_empresa.empresa_id )
      loop
         --
         exit when c_f200%notfound or (c_f200%notfound is null);
         --
         vn_fase := 4.1;
         --
         vn_contrsocapurpc_id := pk_csf_efd_pc.fkg_relac_cons_contr_id( en_tipoimp_id      => gn_tipoimp_id      -- identificador do tipo de imposto
                                                                      , en_ind_orig_cred   => 0                  -- indicador de crédito 0-Oper.Mercado Interno, 1-Oper.Importação
                                                                      , en_codst_id        => r_reg.codst_id     -- identificador do código ST
                                                                      , en_vl_aliq         => r_reg.aliq_pis     -- valor de alíquota em percentual
                                                                      , en_vl_aliq_quant   => 0                  -- valor da alíquota por unidade de produto
                                                                      , en_dm_cod_inc_trib => gn_dm_cod_inc_trib -- indicador da incidência tributária
                                                                      , ev_bloco           => 'F200' );          -- bloco a ser processado
         --
         vn_fase := 4.2;
         --
         if nvl(vn_contrsocapurpc_id,0) <> 0 then
            --
            vn_fase := 4.3;
            --
            vv_cd_contrsocapurpc := pk_csf_efd_pc.fkg_cd_contr_soc_apur_pc( en_contrsocapurpc_id => vn_contrsocapurpc_id );
            --
            vn_fase := 4.4;
            --
            if vv_cd_contrsocapurpc in ('01', '51', '02', '52', '31', '32', '04', '54') and
               (r_reg.cod_st in ('01', '02', '03', '04', '05') and nvl(r_reg.aliq_pis,0) <> 0) or
               (r_reg.cod_st in ('49')) then
               vn_vl_rec_brt := r_reg.vl_rec_brt;
            else
               vn_vl_rec_brt := 0;
            end if;
            --
            vn_fase := 4.5;
            -- 03-Contribuição não-cumulativa apurada a alíquota por unidade de medida de produto
            -- 53-Contribuição cumulativa apurada a alíquota por unidade de medida de produto
            if vv_cd_contrsocapurpc in ('03', '53') then
               vn_vl_bc_cont := 0;
