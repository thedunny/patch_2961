--| Procedimento recupera os dados da contribuição do PIS diferido em períodos anteriores - Bloco M300
-------------------------------------------------------------------------------------------------------
PROCEDURE PKB_DADOS_CONTR_PIS_M300( EN_CONTRPISDIFPERANT_ID IN CONTR_PIS_DIF_PER_ANT.ID%TYPE ) IS
   --
   vn_fase         number := 0;
   vv_nome_empresa varchar2(1000) := null;
   ve_invalido     exception;
   --
   cursor c_contr_pis is
      select *
        from contr_pis_dif_per_ant cp
       where cp.id = en_contrpisdifperant_id;
   --
BEGIN
   --
   vn_fase := 1;
   gt_row_contr_pis_dif_per_ant := null;
   --
   vn_fase := 2;
   open c_contr_pis;
   fetch c_contr_pis into gt_row_contr_pis_dif_per_ant;
   close c_contr_pis;
   --
   vn_fase := 3;
   --| Cabeçalho para as mensagens de log de erro e inconsistências
   gv_mensagem_log := 'Contribuição do Imposto PIS/PASEP diferido em períodos anteriores';
   --
   vn_fase := 4;
   --
   if nvl(gt_row_contr_pis_dif_per_ant.id,0) > 0 then
      --
      vn_fase := 5;
      --
      vv_nome_empresa := pk_csf.fkg_nome_empresa( en_empresa_id => gt_row_contr_pis_dif_per_ant.empresa_id );
      --
      vn_fase := 6;
      --| Cabeçalho para as mensagens de log de erro e inconsistências
      gv_mensagem_log := gv_mensagem_log||' no período de '||to_char(gt_row_contr_pis_dif_per_ant.dt_ini,'dd/mm/rrrr')||' até '||
                         to_char(gt_row_contr_pis_dif_per_ant.dt_fin,'dd/mm/rrrr')||' - Empresa: '||vv_nome_empresa;
      --
   else
      --
      vn_fase := 7;
      --
      raise ve_invalido;
   end if;
   --
EXCEPTION
   when ve_invalido then
      --
      gv_resumo_log := 'Contribuição não encontrada. Verifique o identificador informado (id = '||en_contrpisdifperant_id||').';
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_validacao
                                          , en_referencia_id   => gn_referencia_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
   when others then
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.pkb_dados_contr_pis_m300 (fase = '||vn_fase||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => gn_referencia_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
END PKB_DADOS_CONTR_PIS_M300;
------------------------------------------------------------------------------------------------------------------------------
--| Procedimento para validar a contribuição do PIS diferida em períodos anteriores - valores a pagar no período - Bloco M300
------------------------------------------------------------------------------------------------------------------------------
PROCEDURE PKB_VALIDAR_CONTR_PIS_M300( EN_CONTRPISDIFPERANT_ID IN CONTR_PIS_DIF_PER_ANT.ID%TYPE ) IS
   --
   vn_fase            number := 0;
   vn_loggenerico_id  log_generico.id%type := null;
   vd_data            date;
   --
BEGIN
   --
   vn_fase := 1;
   --
   pkb_excluir_log( en_referencia_id  => en_contrpisdifperant_id
                  , ev_obj_referencia => 'CONTR_PIS_DIF_PER_ANT' );
   --
   vn_fase := 2;
   --
   gv_obj_referencia := 'CONTR_PIS_DIF_PER_ANT';
   gv_mensagem_log   := 'Validação da contribuição diferida do imposto PIS/PASEP';
   gv_resumo_log     := 'Início do processo (identificador da contribuição = '||en_contrpisdifperant_id||').';
   gn_referencia_id  := en_contrpisdifperant_id;
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => gn_referencia_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
   gv_resumo_log := null;
   --
   vn_fase := 3;
   --
   if nvl(en_contrpisdifperant_id,0) = 0 then
      vn_fase := 4;
      gv_resumo_log := 'O identificador da contribuição diferida deve ser informado para que a validação seja efetuada.';
      pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                       , ev_mensagem        => gv_mensagem_log
                                       , ev_resumo          => gv_resumo_log
                                       , en_tipo_log        => erro_de_validacao
                                       , en_referencia_id   => null
                                       , ev_obj_referencia  => gv_obj_referencia );
   else
      --
      vn_fase := 5;
      -- Recupera os dados da contribuição do imposto pis utilizado como referência
      pkb_dados_contr_pis_m300( en_contrpisdifperant_id => en_contrpisdifperant_id );
      gv_mensagem_log := 'Validação da '||gv_mensagem_log;
      --
      vn_fase := 6;
      --
      if pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => gt_row_contr_pis_dif_per_ant.empresa_id
                                               , ed_data       => gt_row_contr_pis_dif_per_ant.dt_ini ) or  -- true
         pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => gt_row_contr_pis_dif_per_ant.empresa_id
                                               , ed_data       => gt_row_contr_pis_dif_per_ant.dt_fin ) then -- true
         --
         vn_fase := 7;
         --
         gv_resumo_log := 'Existe(m) período(s) de abertura efd pis/cofins com situação: Validado ou Arquivo Gerado. Desprocesse a geração do arquivo.';
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_validacao
                                          , en_referencia_id   => gn_referencia_id
                                          , ev_obj_referencia  => gv_obj_referencia );
         --
      else
         --
         vn_fase := 8;
         --
         if gt_row_contr_pis_dif_per_ant.dm_situacao <> 0 then -- não estiver calculada
            vn_fase := 9;
            gv_resumo_log := 'A contribuição diferida informada não está com situação "aberto", portanto, a validação não será realizada.';
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                             , ev_mensagem        => gv_mensagem_log
                                             , ev_resumo          => gv_resumo_log
                                             , en_tipo_log        => erro_de_validacao
                                             , en_referencia_id   => gn_referencia_id
                                             , ev_obj_referencia  => gv_obj_referencia );
         else
            --
            vn_fase := 10;
            --| Validar os campos do Bloco M300
            if nvl(gt_row_contr_pis_dif_per_ant.vl_cont_apur_difer,0) < 0 then
               gv_resumo_log := 'Na contribuição diferida em períodos anteriores, o valor da contribuição apurada ('||
                                ltrim(to_char(nvl(gt_row_contr_pis_dif_per_ant.vl_cont_apur_difer,0),'999G999G999G999G990D00'))||
                                ') não pode ser negativo.';
               pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                               , ev_mensagem        => gv_mensagem_log
                                               , ev_resumo          => gv_resumo_log
                                               , en_tipo_log        => erro_de_validacao
                                               , en_referencia_id   => gn_referencia_id
                                               , ev_obj_referencia  => gv_obj_referencia );
            end if;
            --
            vn_fase := 11;
            --
            if gt_row_contr_pis_dif_per_ant.dm_nat_cred_desc is not null and
               gt_row_contr_pis_dif_per_ant.dm_nat_cred_desc not in ('01', '02', '03', '04') then
               gv_resumo_log := 'Na contribuição diferida em períodos anteriores, a natureza de crédito, se informada, deve ser '||
                                'somente: 01, 02, 03 ou 04.';
               pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                               , ev_mensagem        => gv_mensagem_log
                                               , ev_resumo          => gv_resumo_log
                                               , en_tipo_log        => erro_de_validacao
                                               , en_referencia_id   => gn_referencia_id
                                               , ev_obj_referencia  => gv_obj_referencia );
            end if;
            --
            vn_fase := 12;
            --
            if nvl(gt_row_contr_pis_dif_per_ant.vl_cred_desc_difer,0) < 0 then
               gv_resumo_log := 'Na contribuição diferida em períodos anteriores, o valor do crédito a descontar ('||
                                ltrim(to_char(nvl(gt_row_contr_pis_dif_per_ant.vl_cred_desc_difer,0),'999G999G999G999G990D00'))||') não pode ser negativo.';
               pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                               , ev_mensagem        => gv_mensagem_log
                                               , ev_resumo          => gv_resumo_log
                                               , en_tipo_log        => erro_de_validacao
                                               , en_referencia_id   => gn_referencia_id
                                               , ev_obj_referencia  => gv_obj_referencia );
            end if;
            --
            vn_fase := 13;
            --
            if nvl(gt_row_contr_pis_dif_per_ant.vl_cont_difer_ant,0) < 0 then
               gv_resumo_log := 'Na contribuição diferida em períodos anteriores, o valor da contribuição a recolher ('||
                                ltrim(to_char(nvl(gt_row_contr_pis_dif_per_ant.vl_cont_difer_ant,0),'999G999G999G999G990D00'))||') não pode ser negativo.';
               pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                               , ev_mensagem        => gv_mensagem_log
                                               , ev_resumo          => gv_resumo_log
                                               , en_tipo_log        => erro_de_validacao
                                               , en_referencia_id   => gn_referencia_id
                                               , ev_obj_referencia  => gv_obj_referencia );
            end if;
            --
            vn_fase := 14;
            --
            if nvl(gt_row_contr_pis_dif_per_ant.vl_cont_difer_ant,0) <> (nvl(gt_row_contr_pis_dif_per_ant.vl_cont_apur_difer,0) - nvl(gt_row_contr_pis_dif_per_ant.vl_cred_desc_difer,0)) then
               gv_resumo_log := 'Na contribuição diferida em períodos anteriores, o valor da contribuição a recolher ('||
                                ltrim(to_char(nvl(gt_row_contr_pis_dif_per_ant.vl_cont_difer_ant,0),'999G999G999G999G990D00'))||
                                ') deve ser igual ao cálculo: (vl_cont_apur_difer - vl_cred_desc_difer) -> ('||
                                ltrim(to_char(nvl(gt_row_contr_pis_dif_per_ant.vl_cont_apur_difer,0),'999G999G999G999G990D00'))||' - '||
                                ltrim(to_char(nvl(gt_row_contr_pis_dif_per_ant.vl_cred_desc_difer,0),'999G999G999G999G990D00'))||').';
               pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                               , ev_mensagem        => gv_mensagem_log
                                               , ev_resumo          => gv_resumo_log
                                               , en_tipo_log        => erro_de_validacao
                                               , en_referencia_id   => gn_referencia_id
                                               , ev_obj_referencia  => gv_obj_referencia );
            end if;
            --
            vn_fase := 15;
            --
            begin
               vd_data := to_date(lpad(gt_row_contr_pis_dif_per_ant.per_apur,6,'0'),'mmrrrr');
            exception
               when others then
                  gv_resumo_log := 'Na contribuição diferida em períodos anteriores, o período de apuração da contribuição ('||
                                   lpad(gt_row_contr_pis_dif_per_ant.per_apur,6,'0')||') deve ser válido como data, e estar no formato MMAAAA.';
                  pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                                  , ev_mensagem        => gv_mensagem_log
                                                  , ev_resumo          => gv_resumo_log
                                                  , en_tipo_log        => erro_de_validacao
                                                  , en_referencia_id   => gn_referencia_id
                                                  , ev_obj_referencia  => gv_obj_referencia );
            end;
            --
            vn_fase := 16;
            --
            if gt_row_contr_pis_dif_per_ant.dt_receb not between gt_row_contr_pis_dif_per_ant.dt_ini and gt_row_contr_pis_dif_per_ant.dt_fin then
               gv_resumo_log := 'Na contribuição diferida em períodos anteriores, a data de recebimento da receita ('||
                                to_char(gt_row_contr_pis_dif_per_ant.dt_receb,'dd/mm/rrrr')||') deve estar dentro do período da contribuição ('||
                                to_char(gt_row_contr_pis_dif_per_ant.dt_ini,'dd/mm/rrrr')||' - '||
                                to_char(gt_row_contr_pis_dif_per_ant.dt_fin,'dd/mm/rrrr')||').';
               pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                               , ev_mensagem        => gv_mensagem_log
                                               , ev_resumo          => gv_resumo_log
                                               , en_tipo_log        => erro_de_validacao
                                               , en_referencia_id   => gn_referencia_id
                                               , ev_obj_referencia  => gv_obj_referencia );
            end if;
            --
         end if;
         --
      end if;
      --
   end if;
   --
   vn_fase := 17;
   --
   if gv_resumo_log is null then
      --
      vn_fase := 18;
      --
      begin
         update contr_pis_dif_per_ant cp
            set cp.dm_situacao = 3 -- Processada
          where cp.id = en_contrpisdifperant_id;
      exception
         when others then
            gv_resumo_log := 'Problemas ao alterar a situação da abertura para 3(Processada). Erro = '||sqlerrm;
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                             , ev_mensagem       => gv_mensagem_log
                                             , ev_resumo         => gv_resumo_log
                                             , en_tipo_log       => erro_de_sistema
                                             , en_referencia_id  => gn_referencia_id
                                             , ev_obj_referencia => gv_obj_referencia );
      end;
   else
      --
      vn_fase := 19;
      --
      begin
         update contr_pis_dif_per_ant cp
            set cp.dm_situacao = 4 -- Erro de validação
          where cp.id = en_contrpisdifperant_id;
      exception
         when others then
            gv_resumo_log := 'Problemas ao alterar a situação da abertura para 2(Erro de validação). Erro = '||sqlerrm;
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                             , ev_mensagem       => gv_mensagem_log
                                             , ev_resumo         => gv_resumo_log
                                             , en_tipo_log       => erro_de_sistema
                                             , en_referencia_id  => gn_referencia_id
                                             , ev_obj_referencia => gv_obj_referencia );
      end;
   end if;
   --
   vn_fase := 20;
   --
   commit;
   --
   vn_fase := 21;
   --
   gv_resumo_log := 'Término do processo (identificador da contribuição = '||en_contrpisdifperant_id||').';
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => gn_referencia_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
EXCEPTION
   when others then
      --
      begin
         update contr_pis_dif_per_ant cp
            set cp.dm_situacao = 4 -- Erro de validação
          where cp.id = en_contrpisdifperant_id;
      exception
         when others then
            gv_resumo_log := 'Problemas ao atualizar os valores e situação da consolidação para 4(Erro de validação). Erro = '||sqlerrm;
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                             , ev_mensagem        => gv_mensagem_log
                                             , ev_resumo          => gv_resumo_log
                                             , en_tipo_log        => erro_de_sistema
                                             , en_referencia_id   => en_contrpisdifperant_id
                                             , ev_obj_referencia  => gv_obj_referencia );
      end;
      --
      commit;
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.pkb_validar_contr_pis_m300 (fase = '||vn_fase||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => en_contrpisdifperant_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
END PKB_VALIDAR_CONTR_PIS_M300;
