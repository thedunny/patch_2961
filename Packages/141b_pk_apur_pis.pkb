                     vt_mem_calc_apur_pis.ident_doc           := 'Nota Fiscal/Serie/Item: '||r_reg.nro_nf||'/'||r_reg.serie||'/'||r_reg.nro_item;
                     vt_mem_calc_apur_pis.dm_ind_emit         := r_reg.dm_ind_emit;
                     vt_mem_calc_apur_pis.vl_base_calc        := case r_reg.tipo when 1 then r_reg.vl_bc_pis_tot else r_reg.qtde_base_calc_prod end;  -- 1-alíq.perc, 2-alíq.qtde
                     vt_mem_calc_apur_pis.aliq_perc           := nvl(r_reg.aliq_pis, 0);
                     ---                     
                     if gn_dm_ind_apro_cred = 1 and vn_cd_tipocredpc ='201' then                      
                        vt_mem_calc_apur_pis.aliq_prod           := 0;--nvl(r_reg.vl_aliq_pis_quant, 0);
                     else
                        vt_mem_calc_apur_pis.aliq_prod           := nvl(r_reg.vl_aliq_pis_quant, 0);
                     end if;                     
                     ---                     
                     vt_mem_calc_apur_pis.vl_imp_trib         := nvl(r_reg.vl_imp, 0);
                     vt_mem_calc_apur_pis.reg_controlador     := 'M100';
                     pkb_monta_vetor_mem_calc_pis(vt_mem_calc_apur_pis);
                     --                     
                  end if;
                  --
                  vn_fase := 10.19;
                  --
                  if nvl(vn_seq_lancto,0) = nvl(r_reg.seq_lancto,0) then
                     exit;
                  end if;
                  --
               end loop;
               --
            end if; -- não se enquadra nos código ST
