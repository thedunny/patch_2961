                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_validacao
                                          , en_referencia_id   => gn_referencia_id
                                          , ev_obj_referencia  => gv_obj_referencia );
         --
      else
         --
         vn_fase := 8;
         --
         begin
            update inf_pis_folha ip
               set ip.dm_situacao     = 0 -- Aberto
                 , ip.vl_tot_fol      = 0
                 , ip.vl_exc_bc       = 0
                 , ip.vl_tot_bc       = 0
                 , ip.aliq_pis_fol    = 0
                 , ip.vl_tot_cont_fol = 0
             where ip.id = gt_row_inf_pis_folha.id;
         exception
            when others then
               gv_resumo_log := 'Problemas ao zerar os valores e manter situação da folha de salário 0(Em aberto). Erro = '||sqlerrm;
               pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                , ev_mensagem        => gv_mensagem_log
                                                , ev_resumo          => gv_resumo_log
                                                , en_tipo_log        => erro_de_sistema
                                                , en_referencia_id   => gn_referencia_id
                                                , ev_obj_referencia  => gv_obj_referencia );
         end;
         --
      end if;
      --
   end if;
   --
   vn_fase := 9;
   --
   commit;
   --
   vn_fase := 10;
   --
   gv_resumo_log := 'Término do processo (identificador da folha = '||en_infpisfolha_id||').';
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => gn_referencia_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
EXCEPTION
