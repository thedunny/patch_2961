                                                                      , en_codst_id        => r_reg.codst_id        -- identificador do código ST
                                                                      , en_vl_aliq         => r_reg.aliq_pis        -- valor de alíquota em percentual
                                                                      , en_vl_aliq_quant   => r_reg.aliq_pis_quant  -- valor da alíquota por unidade de produto
                                                                      , en_dm_cod_inc_trib => gn_dm_cod_inc_trib ); -- indicador da incidência tributária
         --
