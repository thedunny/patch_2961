                                                  , en_referencia_id   => gn_referencia_id
                                                  , ev_obj_referencia  => gv_obj_referencia );
               end if;
               --
               vn_fase := 36;
               --
               if pk_csf_efd_pc.fkg_base_calc_cred_pc_cd(en_id => r_reg.basecalccredpc_id) = 13 and
                  r_reg.desc_cred is null then
                  gv_resumo_log := 'Quando o código da base de cálculo de crédito for 13-Outras Operações com Direito a Crédito, a descrição do '||
                                   'crédito deve ser informada.';
                  pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                                  , ev_mensagem        => gv_mensagem_log
                                                  , ev_resumo          => gv_resumo_log
                                                  , en_tipo_log        => erro_de_validacao
                                                  , en_referencia_id   => gn_referencia_id
                                                  , ev_obj_referencia  => gv_obj_referencia );
               end if;
               --
            end loop; -- c_detalhe
            --
            vn_fase := 37;
            --
            for r_reg in c_ajuste
            loop
               --
               vn_fase := 38;
               --
               if r_reg.dm_ind_aj not in (0,1) then
                  gv_resumo_log := 'No ajuste de apuração, o indicador do tipo de ajuste deve ser: 0-Ajuste de redução ou 1-Ajuste de acréscimo.';
                  pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                                  , ev_mensagem        => gv_mensagem_log
                                                  , ev_resumo          => gv_resumo_log
                                                  , en_tipo_log        => erro_de_validacao
                                                  , en_referencia_id   => gn_referencia_id
                                                  , ev_obj_referencia  => gv_obj_referencia );
               end if;
               --
               vn_fase := 39;
               --
               if nvl(r_reg.vl_aj,0) < 0 then
                  gv_resumo_log := 'No ajuste de apuração, o valor do ajuste ('||
                                   ltrim(to_char(nvl(r_reg.vl_aj,0),'999G999G999G999G990D00'))||') não pode ser negativo.';
                  pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                                  , ev_mensagem        => gv_mensagem_log
                                                  , ev_resumo          => gv_resumo_log
                                                  , en_tipo_log        => erro_de_validacao
                                                  , en_referencia_id   => gn_referencia_id
                                                  , ev_obj_referencia  => gv_obj_referencia );
               end if;
               --
               vn_fase := 40;
               --
               if r_reg.dt_ref is not null and
                  r_reg.dt_ref not between gt_row_per_apur_cred_pis.dt_ini and gt_row_per_apur_cred_pis.dt_fin then
                  gv_resumo_log := 'No ajuste de apuração, a data de referência ('||to_char(r_reg.dt_ref,'ddmmrrrr')||
                                   ') deve estar dentro do período de apuração (de: '||to_char(gt_row_per_apur_cred_pis.dt_ini,'ddmmrrrr')||
                                   ' até: '||to_char(gt_row_per_apur_cred_pis.dt_fin,'ddmmrrrr')||').';
                  pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                                  , ev_mensagem        => gv_mensagem_log
                                                  , ev_resumo          => gv_resumo_log
                                                  , en_tipo_log        => erro_de_validacao
                                                  , en_referencia_id   => gn_referencia_id
                                                  , ev_obj_referencia  => gv_obj_referencia );
               end if;
               --
               vn_fase := 41;
               --
               open c_det_aju( en_ajustapurcredpis_id => r_reg.id );
               fetch c_det_aju into vn_det_valor_aj;
               close c_det_aju;
               --
               vn_fase := 42;
               --
               if nvl(r_reg.vl_aj,0) <> nvl(vn_det_valor_aj,0) then
                  gv_resumo_log := 'O valor do ajuste ('||ltrim(to_char(nvl(r_reg.vl_aj,0),'999G999G999G999G990D00'))||') não pode ser diferente da soma '||
                                   'dos valores do detalhamento dos ajustes ('||ltrim(to_char(nvl(vn_det_valor_aj,0),'999G999G999G999G990D00'))||').';
                  pk_log_generico.pkb_log_generico( sn_loggenerico_id  => vn_loggenerico_id
                                                  , ev_mensagem        => gv_mensagem_log
                                                  , ev_resumo          => gv_resumo_log
                                                  , en_tipo_log        => erro_de_validacao
                                                  , en_referencia_id   => gn_referencia_id
                                                  , ev_obj_referencia  => gv_obj_referencia );
               end if;
               --
            end loop;
            --
            vn_fase := 43;
            --
            if gv_resumo_log is null then
               --
               vn_fase := 44;
               --| Atualizar a descrição de crédito nos detalhes de apuração
               begin
                  update det_apur_cred_pis da
                     set da.desc_cred = ltrim(rtrim(pk_csf.fkg_converte(da.desc_cred)))
                   where da.apurcredpis_id = gt_row_apur_cred_pis.id;
               exception
                  when others then
                     gv_resumo_log := 'Problemas ao atualizar a descrição de crédito dos detalhes de apuração. Erro = '||sqlerrm;
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                                      , ev_mensagem       => gv_mensagem_log
                                                      , ev_resumo         => gv_resumo_log
                                                      , en_tipo_log       => erro_de_sistema
                                                      , en_referencia_id  => gn_referencia_id
                                                      , ev_obj_referencia => gv_obj_referencia );
               end;
               --
               vn_fase := 45;
               --| Atualizar número de documento e descrição de ajustes
               begin
                  update ajust_apur_cred_pis aa
                     set aa.num_doc  = ltrim(rtrim(pk_csf.fkg_converte(aa.num_doc)))
                       , aa.descr_aj = ltrim(rtrim(pk_csf.fkg_converte(aa.descr_aj)))
                   where aa.apurcredpis_id = gt_row_apur_cred_pis.id;
               exception
                  when others then
                     gv_resumo_log := 'Problemas ao atualizar número de documento e descrição de ajustes (apuração). Erro = '||sqlerrm;
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                                      , ev_mensagem       => gv_mensagem_log
                                                      , ev_resumo         => gv_resumo_log
                                                      , en_tipo_log       => erro_de_sistema
                                                      , en_referencia_id  => gn_referencia_id
                                                      , ev_obj_referencia => gv_obj_referencia );
               end;
               --
               vn_fase := 46;
               --| Atualizar descrição de detalhe de ajustes e informações complementares
               begin
                  update det_ajust_apur_cred_pis da
                     set da.descr_aj   = ltrim(rtrim(pk_csf.fkg_converte(da.descr_aj)))
                       , da.info_compl = ltrim(rtrim(pk_csf.fkg_converte(da.info_compl)))
                   where da.ajustapurcredpis_id in (select aa.id
                                                      from ajust_apur_cred_pis aa
                                                     where aa.apurcredpis_id = gt_row_apur_cred_pis.id);
               exception
                  when others then
                     gv_resumo_log := 'Problemas ao atualizar descrição de detalhe de ajustes e informações complementares (apuração). Erro = '||sqlerrm;
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                                      , ev_mensagem       => gv_mensagem_log
                                                      , ev_resumo         => gv_resumo_log
                                                      , en_tipo_log       => erro_de_sistema
                                                      , en_referencia_id  => gn_referencia_id
                                                      , ev_obj_referencia => gv_obj_referencia );
               end;
               --
            end if;
            --
         end if;
         --
      end if;
      --
   end if;
   --
   vn_fase := 47;
   --
   if gv_resumo_log is null then
      --
      vn_fase := 48;
      --| Atualizar a situação da apuração
      begin
         update apur_cred_pis ac
            set ac.dm_situacao = 3 -- Processada
          where ac.id = gt_row_apur_cred_pis.id;
      exception
         when others then
            gv_resumo_log := 'Problemas ao atualizar situação da apuração para 3(Processada). Erro = '||sqlerrm;
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                             , ev_mensagem       => gv_mensagem_log
                                             , ev_resumo         => gv_resumo_log
                                             , en_tipo_log       => erro_de_sistema
                                             , en_referencia_id  => gn_referencia_id
                                             , ev_obj_referencia => gv_obj_referencia );
      end;
      --
   else
      --
      vn_fase := 49;
      --| Atualizar a situação da apuração
      begin
         update apur_cred_pis ac
            set ac.dm_situacao = 4 -- Erro de validação
          where ac.id = gt_row_apur_cred_pis.id;
      exception
         when others then
            gv_resumo_log := 'Problemas ao atualizar situação da apuração para 4(Erro de validação). Erro = '||sqlerrm;
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                             , ev_mensagem       => gv_mensagem_log
                                             , ev_resumo         => gv_resumo_log
                                             , en_tipo_log       => erro_de_sistema
                                             , en_referencia_id  => gn_referencia_id
                                             , ev_obj_referencia => gv_obj_referencia );
      end;
      --
   end if;
   --
   vn_fase := 50;
   --
   commit;
   --
   vn_fase := 51;
   --
   gv_resumo_log := 'Término do processo (identificador da apuração = '||en_apurcredpis_id||').';
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => gn_referencia_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
EXCEPTION
   when others then
      --
      begin
         update apur_cred_pis ac
            set ac.dm_situacao = 4 -- Erro de validação
          where ac.id = en_apurcredpis_id;
      exception
         when others then
            gv_resumo_log := 'Problemas (geral) ao atualizar situação da apuração para 4(Erro de validação). Erro = '||sqlerrm;
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                             , ev_mensagem       => gv_mensagem_log
                                             , ev_resumo         => gv_resumo_log
                                             , en_tipo_log       => erro_de_sistema
                                             , en_referencia_id  => en_apurcredpis_id
                                             , ev_obj_referencia => gv_obj_referencia );
      end;
      --
      commit;
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.pkb_validar_apur_pis_m100 (fase = '||vn_fase||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => en_apurcredpis_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
END PKB_VALIDAR_APUR_PIS_M100;
------------------------------------------------------------------------------------
--| Procedimento para validar por período a apuração do crédito do PIS - Bloco M100
--| Serão validados alguns campos dos blocos M100, M105 e M110
------------------------------------------------------------------------------------
PROCEDURE PKB_VAL_PER_APUR_PIS_M100( EN_PERAPURCREDPIS_ID IN PER_APUR_CRED_PIS.ID%TYPE ) IS
   --
   vn_fase           number := 0;
   vn_loggenerico_id log_generico.id%type;
   --
   cursor c_per_apur_cred_pis( en_perapurcredpis_id in per_apur_cred_pis.id%type ) is
      select *
        from per_apur_cred_pis pa
       where pa.id  = en_perapurcredpis_id;
   --
   cursor c_apur is
      select ac.id apurcredpis_id
        from apur_cred_pis ac
       where ac.perapurcredpis_id = en_perapurcredpis_id
         and ac.dm_ind_cred_ori   = 0; -- Indicador de Credito Oriundo de: 0-Operações próprias; 1-Evento de incorporação, cisão ou fusão
   --
   cursor c_apur_f800 is
      select ac.id apurcredpis_id
        from apur_cred_pis ac
       where ac.perapurcredpis_id = en_perapurcredpis_id
         and ac.dm_ind_cred_ori   = 1; -- Indicador de Credito Oriundo de: 0-Operações próprias; 1-Evento de incorporação, cisão ou fusão
   --
BEGIN
   --
   vn_fase := 1;
   --
   pkb_excluir_log( en_referencia_id  => en_perapurcredpis_id
                  , ev_obj_referencia => 'PER_APUR_CRED_PIS' );
   --
   vn_fase := 2;
   --
   gv_mensagem_log   := 'Validação por período das apurações de crédito do imposto PIS/PASEP';
   gv_obj_referencia := 'PER_APUR_CRED_PIS';
   gv_resumo_log     := 'Início do processo (identificador do período = '||en_perapurcredpis_id||').';
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => en_perapurcredpis_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
   gv_resumo_log := null;
   --
   vn_fase := 3;
   --
   if nvl(en_perapurcredpis_id,0) = 0 then
      --
      vn_fase := 4;
      gv_resumo_log := 'O identificador do período de apuração deve ser informado para que a validação seja efetuada.';
      pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                       , ev_mensagem        => gv_mensagem_log
                                       , ev_resumo          => gv_resumo_log
                                       , en_tipo_log        => erro_de_validacao
                                       , en_referencia_id   => null
                                       , ev_obj_referencia  => gv_obj_referencia );
      --
   else
      --
      vn_fase := 5;
      --
      if pk_csf_efd_pc.fkg_qtde_apur_pis( en_perapurcredpis_id => en_perapurcredpis_id ) = 0 then -- devem existir apurações geradas
         --
         vn_fase := 6;
         gv_resumo_log := 'O período de apuração informado não possui apurações geradas, portanto, a validação não será realizada.';
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_validacao
                                          , en_referencia_id   => en_perapurcredpis_id
                                          , ev_obj_referencia  => gv_obj_referencia );
         --
      else
         --
         vn_fase := 7;
         --
         gt_row_per_apur_cred_pis := null;
         open c_per_apur_cred_pis( en_perapurcredpis_id => en_perapurcredpis_id );
         fetch c_per_apur_cred_pis into gt_row_per_apur_cred_pis;
         close c_per_apur_cred_pis;
         --
         vn_fase := 8;
         --
         if pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => gt_row_per_apur_cred_pis.empresa_id
                                                  , ed_data       => gt_row_per_apur_cred_pis.dt_ini ) or  -- true
            pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => gt_row_per_apur_cred_pis.empresa_id
                                                  , ed_data       => gt_row_per_apur_cred_pis.dt_fin ) then -- true
            --
            vn_fase := 9;
            --
            gv_resumo_log := 'Existe(m) período(s) de abertura efd pis/cofins com situação: Validado ou Arquivo Gerado. Desprocesse a geração do arquivo.';
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                             , ev_mensagem        => gv_mensagem_log
                                             , ev_resumo          => gv_resumo_log
                                             , en_tipo_log        => erro_de_validacao
                                             , en_referencia_id   => gt_row_per_apur_cred_pis.id
                                             , ev_obj_referencia  => gv_obj_referencia );
            --
         else
            --
            vn_fase := 10;
            --
            for r_reg in c_apur
            loop
               --
               vn_fase := 11;
               --
               pkb_validar_apur_pis_m100( en_apurcredpis_id => r_reg.apurcredpis_id );
               --
            end loop;
            --
            vn_fase := 12;
            --
            for r_reg_f800 in c_apur_f800
            loop
               --
               vn_fase := 13;
               --
               begin
                  update apur_cred_pis ac
                     set ac.dm_situacao = 3 -- Situação: 0-Aberto; 1-Calculada; 2-Erro no cálculo; 3-Processada; 4-Erro de validação
                   where ac.id = r_reg_f800.apurcredpis_id;
               exception
                  when others then
                     raise_application_error (-20101, 'Problemas ao atualizar a apuração para processada, originada pelos dados do Bloco F800. Erro = '||sqlerrm);
               end;
               --
            end loop;
            --
         end if;
         --
      end if;
      --
   end if;
   --
   vn_fase := 14;
   --
   commit;
   --
   vn_fase := 15;
   --
   gv_resumo_log := 'Término do processo (identificador do período = '||en_perapurcredpis_id||').';
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => en_perapurcredpis_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
EXCEPTION
   when others then
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.pkb_val_per_apur_pis_m100 (fase = '||vn_fase||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => 'Validar por período a apuração do crédito do PIS'
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => en_perapurcredpis_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
END PKB_VAL_PER_APUR_PIS_M100;
-----------------------------------------------------------------------------
--| Procedimento para desprocessar a apuração do crédito do PIS - Bloco M100
-----------------------------------------------------------------------------
PROCEDURE PKB_DESPROCESSAR_APUR_PIS_M100 ( EN_APURCREDPIS_ID IN APUR_CRED_PIS.ID%TYPE ) IS
   --
   vn_fase           number := 0;
   vn_loggenerico_id log_generico.id%type;
   vn_empresa_id     per_apur_cred_pis.empresa_id%type;
   vd_dt_ini         per_apur_cred_pis.dt_ini%type;
   vd_dt_fin         per_apur_cred_pis.dt_fin%type;
   --
BEGIN
   --
   vn_fase := 1;
   --
   pkb_excluir_log( en_referencia_id  => en_apurcredpis_id
                  , ev_obj_referencia => 'APUR_CRED_PIS' );
   --
   vn_fase := 2;
   --
   gv_obj_referencia := 'APUR_CRED_PIS';
   gv_mensagem_log   := 'Desprocesso da apuração do imposto PIS/PASEP';
   gv_resumo_log     := 'Início do processo (identificador da apuração = '||en_apurcredpis_id||').';
   gn_referencia_id  := en_apurcredpis_id;
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => gn_referencia_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
   gv_resumo_log := null;
   --
   vn_fase := 3;
   --
   if nvl(en_apurcredpis_id,0) = 0 then
      vn_fase := 4;
      gv_resumo_log := 'O identificador da apuração deve ser informado para que o desprocessamento seja efetuado.';
      pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                       , ev_mensagem        => gv_mensagem_log
                                       , ev_resumo          => gv_resumo_log
                                       , en_tipo_log        => erro_de_validacao
                                       , en_referencia_id   => null
                                       , ev_obj_referencia  => gv_obj_referencia );
   else
      --
      vn_fase := 5;
      -- Recupera os dados da apuração do imposto pis utilizado como referência
      pkb_dados_apur_pis_m100( en_apurcredpis_id => en_apurcredpis_id );
      gv_mensagem_log := 'Desprocessar a '||gv_mensagem_log;
      --
      vn_fase := 6;
      --
      begin
         select pa.empresa_id
              , pa.dt_ini
              , pa.dt_fin
           into vn_empresa_id
              , vd_dt_ini
              , vd_dt_fin
           from per_apur_cred_pis pa
          where pa.id = gt_row_apur_cred_pis.perapurcredpis_id;
      exception
         when others then
            gv_resumo_log := 'Problemas ao recuperar situação e data do período de apuração do pis (id = '||gt_row_apur_cred_pis.perapurcredpis_id||
                             '). Erro = '||sqlerrm;
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                             , ev_mensagem        => gv_mensagem_log
                                             , ev_resumo          => gv_resumo_log
                                             , en_tipo_log        => erro_de_sistema
                                             , en_referencia_id   => gn_referencia_id
                                             , ev_obj_referencia  => gv_obj_referencia );
      end;
      --
      vn_fase := 7;
      --
      if pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => vn_empresa_id
                                               , ed_data       => vd_dt_ini ) or  -- true
         pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => vn_empresa_id
                                               , ed_data       => vd_dt_fin ) then -- true
         --
         vn_fase := 8;
         --
         gv_resumo_log := 'Existe(m) período(s) de abertura efd pis/cofins com situação: Validado ou Arquivo Gerado. Desprocesse a geração do arquivo.';
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_validacao
                                          , en_referencia_id   => gn_referencia_id
                                          , ev_obj_referencia  => gv_obj_referencia );
         --
      else
         --
         vn_fase := 9;
         -- Verificar se existem relacionamentos gerados pelo Bloco M200, entre os Blocos M100 e 1100
         if pk_csf_efd_pc.fkb_existe_relac_apur_pis( en_apurcredpis_id => gt_row_apur_cred_pis.id ) or -- true
            pk_csf_efd_pc.fkb_existe_rel_apur_contr_pis( en_apurcredpis_id => gt_row_apur_cred_pis.id ) then -- true
            --
            vn_fase := 10;
            --
            gv_resumo_log := 'Existe(m) relacionamento(s) entre os Blocos M100 e 1100 gerados pelo Bloco M200. Desprocesse o Bloco M200 - Consolidação da '||
                             'Apuração.';
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                             , ev_mensagem        => gv_mensagem_log
                                             , ev_resumo          => gv_resumo_log
                                             , en_tipo_log        => erro_de_validacao
                                             , en_referencia_id   => gn_referencia_id
                                             , ev_obj_referencia  => gv_obj_referencia );
            --
         else
            --
            vn_fase := 11;
            -- Se o DM_SITUACAO = 0 "Em aberto", manter situação = 0 "Em Aberto"
            if gt_row_apur_cred_pis.dm_situacao in (0) then
               -- Se o DM_SITUACAO = 0 "Em aberto", manter situação = 0 "Em Aberto"
               vn_fase := 12;
               --
               begin
                  delete from det_apur_cred_pis da
                   where da.apurcredpis_id = gt_row_apur_cred_pis.id;
               exception
                  when others then
                     gv_resumo_log := 'Problemas ao excluir detalhes da apuração. Erro = '||sqlerrm;
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                      , ev_mensagem        => gv_mensagem_log
                                                      , ev_resumo          => gv_resumo_log
                                                      , en_tipo_log        => erro_de_sistema
                                                      , en_referencia_id   => gn_referencia_id
                                                      , ev_obj_referencia  => gv_obj_referencia );
               end;
               --
               vn_fase := 13;
               --
               begin
                  update apur_cred_pis ac
                     set ac.dm_situacao   = 0 -- Aberto
                       , ac.vl_ajus_acres = 0
                       , ac.vl_ajus_reduc = 0
                       , ac.vl_cred_dif   = 0
                       , ac.vl_cred_disp  = 0
                       , ac.vl_sld_cred   = 0
                   where ac.id = gt_row_apur_cred_pis.id;
               exception
                  when others then
                     gv_resumo_log := 'Problemas ao zerar valores e manter a situação da apuração 0(Aberto). Erro = '||sqlerrm;
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                      , ev_mensagem        => gv_mensagem_log
                                                      , ev_resumo          => gv_resumo_log
                                                      , en_tipo_log        => erro_de_sistema
                                                      , en_referencia_id   => gn_referencia_id
                                                      , ev_obj_referencia  => gv_obj_referencia );
               end;
               --
            elsif gt_row_apur_cred_pis.dm_situacao in (1, 2) then
                  -- Se o DM_SITUACAO = 1 "Calculado" ou 2 "Erro no Cálculo", altera para 0 "Aberto"
                  vn_fase := 14;
                  --
                  begin
                     update apur_cred_pis ac
                        set ac.dm_situacao   = 0 -- Aberto
                          , ac.vl_ajus_acres = 0
                          , ac.vl_ajus_reduc = 0
                          , ac.vl_cred_dif   = 0
                          , ac.vl_cred_disp  = 0
                          , ac.vl_sld_cred   = 0
                      where ac.id = gt_row_apur_cred_pis.id;
                  exception
                     when others then
                        gv_resumo_log := 'Problemas ao zerar valores e situação da apuração para 0(Aberto). Erro = '||sqlerrm;
                        pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                         , ev_mensagem        => gv_mensagem_log
                                                         , ev_resumo          => gv_resumo_log
                                                         , en_tipo_log        => erro_de_sistema
                                                         , en_referencia_id   => gn_referencia_id
                                                         , ev_obj_referencia  => gv_obj_referencia );
                  end;
                  --
            elsif gt_row_apur_cred_pis.dm_situacao in (3, 4) then
                  -- Se o DM_SITUACAO = 3 "Processado" ou 4 "Erro de Validação", altera para 1 "Calculado"
                  vn_fase := 15;
                  --
                  begin
                     update apur_cred_pis ac
                        set ac.dm_situacao = 1 -- Calculado
                      where ac.id = gt_row_apur_cred_pis.id;
                  exception
                     when others then
                        gv_resumo_log := 'Problemas ao atualizar situação da apuração para 1(Calculado). Erro = '||sqlerrm;
                        pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                         , ev_mensagem        => gv_mensagem_log
                                                         , ev_resumo          => gv_resumo_log
                                                         , en_tipo_log        => erro_de_sistema
                                                         , en_referencia_id   => gn_referencia_id
                                                         , ev_obj_referencia  => gv_obj_referencia );
                  end;
                  --
            end if;
            --
         end if; -- existe relacionamento com Bloco 1100 e Bloco M100, gerados pelo Bloco M200
         --
      end if;
      --
   end if;
   --
   vn_fase := 16;
   --
   commit;
   --
   vn_fase := 17;
   --
   gv_resumo_log := 'Término do processo (identificador da apuração = '||en_apurcredpis_id||').';
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => gn_referencia_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
EXCEPTION
   when others then
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.pkb_desprocessar_apur_pis_m100 (fase = '||vn_fase||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => en_apurcredpis_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
END PKB_DESPROCESSAR_APUR_PIS_M100;
-----------------------------------------------------------------------------------------
--| Procedimento para desprocessar por período a apuração do crédito do PIS - Bloco M100
-----------------------------------------------------------------------------------------
PROCEDURE PKB_DESPR_PER_APUR_PIS_M100( EN_PERAPURCREDPIS_ID IN PER_APUR_CRED_PIS.ID%TYPE ) IS
   --
   vn_fase           number := 0;
   vn_loggenerico_id log_generico.id%type;
   vv_mensagem       varchar2(4000);
   --
   cursor c_per_apur_cred_pis( en_perapurcredpis_id in per_apur_cred_pis.id%type ) is
      select *
        from per_apur_cred_pis pa
       where pa.id  = en_perapurcredpis_id;
   --
   cursor c_apur is
      select ac.id apurcredpis_id
        from apur_cred_pis ac
       where ac.perapurcredpis_id = en_perapurcredpis_id;
   --
BEGIN
   --
   vn_fase := 1;
   --
   pkb_excluir_log( en_referencia_id  => en_perapurcredpis_id
                  , ev_obj_referencia => 'PER_APUR_CRED_PIS' );
   --
   vn_fase := 2;
   --
   gv_mensagem_log   := 'Desprocesso por período das apurações de crédito do imposto PIS/PASEP';
   gv_obj_referencia := 'PER_APUR_CRED_PIS';
   gv_resumo_log     := 'Início do processo (identificador do período = '||en_perapurcredpis_id||').';
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => en_perapurcredpis_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
   gv_resumo_log := null;
   --
   vn_fase := 3;
   --
   if nvl(en_perapurcredpis_id,0) = 0 then
      --
      vn_fase := 4;
      gv_resumo_log := 'O identificador do período de apuração deve ser informado para que o desprocessamento seja efetuado.';
      pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                       , ev_mensagem        => gv_mensagem_log
                                       , ev_resumo          => gv_resumo_log
                                       , en_tipo_log        => erro_de_validacao
                                       , en_referencia_id   => null
                                       , ev_obj_referencia  => gv_obj_referencia );
      --
   else
      --
      vn_fase := 5;
      --
      gt_row_per_apur_cred_pis := null;
      open c_per_apur_cred_pis( en_perapurcredpis_id => en_perapurcredpis_id );
      fetch c_per_apur_cred_pis into gt_row_per_apur_cred_pis;
      close c_per_apur_cred_pis;
      --
      vn_fase := 6;
      --
      if pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => gt_row_per_apur_cred_pis.empresa_id
                                               , ed_data       => gt_row_per_apur_cred_pis.dt_ini ) or  -- true
         pk_csf_efd_pc.fkb_existe_perarq_gerado( en_empresa_id => gt_row_per_apur_cred_pis.empresa_id
                                               , ed_data       => gt_row_per_apur_cred_pis.dt_fin ) then -- true
         --
         vn_fase := 7;
         --
         gv_resumo_log := 'Existe(m) período(s) de abertura efd pis/cofins com situação: Validado ou Arquivo Gerado. Desprocesse a geração do arquivo.';
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_validacao
                                          , en_referencia_id   => gt_row_per_apur_cred_pis.id
                                          , ev_obj_referencia  => gv_obj_referencia );
         --
      else
         --
         vn_fase := 8;
         --
         for r_reg in c_apur
         loop
            --
            exit when c_apur%notfound or (c_apur%notfound) is null;
            --
            vn_fase := 9;
            -- Verificar se existem relacionamentos gerados pelo Bloco M200, entre os Blocos M100 e 1100
            if pk_csf_efd_pc.fkb_existe_relac_apur_pis( en_apurcredpis_id => r_reg.apurcredpis_id ) or -- true
               pk_csf_efd_pc.fkb_existe_rel_apur_contr_pis( en_apurcredpis_id => r_reg.apurcredpis_id ) then -- true
               --
               vn_fase := 10;
               --
               vv_mensagem := 'Existe(m) relacionamento(s) entre os Blocos M100 e 1100 gerados pelo Bloco M200. Desprocesse o Bloco M200 - Consolidação da Apuração.';
               exit;
               --
            end if;
            --
            vn_fase := 11;
            --
            pkb_desprocessar_apur_pis_m100( en_apurcredpis_id => r_reg.apurcredpis_id );
            --
         end loop;
         --
         vn_fase := 12;
         --
         -- Recupera o Periodo de consolidação caso não exista --
         pkb_ret_perconscontrpis ( en_empresa_id         => gt_row_per_apur_cred_pis.empresa_id
                                 , ed_dt_ini             => gt_row_per_apur_cred_pis.dt_ini
                                 , ed_dt_fin             => gt_row_per_apur_cred_pis.dt_fin
                                 , st_per_cons_contr_pis => gt_row_per_cons_contr_pis);
         --         
         if nvl(gt_row_per_cons_contr_pis.id,0) = 0 then
            --
            gv_resumo_log := 'Não foi encontrado um período de consolidação (PIS) com a situação PROCESSADA';
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                             , ev_mensagem        => gv_resumo_log
                                             , ev_resumo          => gv_resumo_log
                                             , en_tipo_log        => erro_de_validacao
                                             , en_referencia_id   => gn_referencia_id
                                             , ev_obj_referencia  => gv_obj_referencia );      
            --
         end if;
         --
         vn_fase := 13;
         --
         -- Exclui a memória de Calculo do M100 --
         pkb_exclui_mem_calc_pis ( en_perconscontrpis_id => gt_row_per_cons_contr_pis.id
                                 , ev_reg_controlador    => 'M100');
         --
         vn_fase := 14;
         --
         if vv_mensagem is not null then
            --
            vn_fase := 15;
            --
            gv_resumo_log := vv_mensagem;
            pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                             , ev_mensagem        => gv_mensagem_log
                                             , ev_resumo          => gv_resumo_log
                                             , en_tipo_log        => erro_de_validacao
                                             , en_referencia_id   => gt_row_per_apur_cred_pis.id
                                             , ev_obj_referencia  => gv_obj_referencia );
            --
         end if;
         --
      end if;
      --
   end if;
   --
   vn_fase := 16;
   --
   commit;
   --
   vn_fase := 17;
   --
   gv_resumo_log := 'Término do processo (identificador do período = '||en_perapurcredpis_id||').';
   pk_log_generico.pkb_log_generico ( sn_loggenerico_id => vn_loggenerico_id
                                    , ev_mensagem       => gv_mensagem_log
                                    , ev_resumo         => gv_resumo_log
                                    , en_tipo_log       => erro_inform_geral
                                    , en_referencia_id  => en_perapurcredpis_id
                                    , ev_obj_referencia => gv_obj_referencia );
   --
EXCEPTION
   when others then
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.pkb_despr_per_apur_pis_m100 (fase = '||vn_fase||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => 'Desprocessar por período a apuração do crédito do PIS'
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => en_perapurcredpis_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
END PKB_DESPR_PER_APUR_PIS_M100;
--------------------------------------------------------------------------
--| Procedimento para gravar os valores nas tabelas de banco - Bloco M200
--------------------------------------------------------------------------
procedure pkb_grava_dados_m200 is
   --
   vn_fase    number := 0;
   vn_ind     number := 0;
   vn_ind_bi  number := 0;
   vn_ind_tri number := 0;
   --
begin
   --
   vn_fase := 1;
   --| Gravar os dados de consolidação do pis - cons_contr_pis
   vn_ind := nvl(vt_tab_reg_m200.first,0);
   --
   vn_fase := 2;
   --
   loop
      --
      vn_fase := 3;
      --
      if nvl(vn_ind,0) = 0 then -- índice = conscontrpis_id
         exit;
      end if;
      --
      vn_fase := 4;
      --
      begin
         insert into cons_contr_pis( id
                                   , dm_situacao
                                   , vl_tot_cont_nc_per
                                   , vl_tot_cred_desc
                                   , vl_tot_cred_desc_ant
                                   , vl_tot_cont_nc_dev
                                   , vl_ret_nc
                                   , vl_out_ded_nc
                                   , vl_cont_nc_rec
                                   , vl_tot_cont_cum_per
                                   , vl_ret_cum
                                   , vl_out_ded_cum
                                   , vl_cont_cum_rec
                                   , vl_tot_cont_rec
                                   , perconscontrpis_id )
                             values( vt_tab_reg_m200(vn_ind).conscontrpis_id
                                   , vt_tab_reg_m200(vn_ind).dm_situacao
                                   , vt_tab_reg_m200(vn_ind).vl_tot_cont_nc_per
                                   , vt_tab_reg_m200(vn_ind).vl_tot_cred_desc
                                   , vt_tab_reg_m200(vn_ind).vl_tot_cred_desc_ant
                                   , vt_tab_reg_m200(vn_ind).vl_tot_cont_nc_dev
                                   , vt_tab_reg_m200(vn_ind).vl_ret_nc
                                   , vt_tab_reg_m200(vn_ind).vl_out_ded_nc
                                   , vt_tab_reg_m200(vn_ind).vl_cont_nc_rec
                                   , vt_tab_reg_m200(vn_ind).vl_tot_cont_cum_per
                                   , vt_tab_reg_m200(vn_ind).vl_ret_cum
                                   , vt_tab_reg_m200(vn_ind).vl_out_ded_cum
                                   , vt_tab_reg_m200(vn_ind).vl_cont_cum_rec
                                   , vt_tab_reg_m200(vn_ind).vl_tot_cont_rec
                                   , gt_row_per_cons_contr_pis.id );
      exception
         when dup_val_on_index then
            --
            vn_fase := 5;
            --
            begin
               update cons_contr_pis cc
                  set cc.dm_situacao          = vt_tab_reg_m200(vn_ind).dm_situacao
                    , cc.vl_tot_cont_nc_per   = vt_tab_reg_m200(vn_ind).vl_tot_cont_nc_per
                    , cc.vl_tot_cred_desc     = vt_tab_reg_m200(vn_ind).vl_tot_cred_desc
                    , cc.vl_tot_cred_desc_ant = vt_tab_reg_m200(vn_ind).vl_tot_cred_desc_ant
                    , cc.vl_tot_cont_nc_dev   = vt_tab_reg_m200(vn_ind).vl_tot_cont_nc_dev
                    , cc.vl_ret_nc            = vt_tab_reg_m200(vn_ind).vl_ret_nc
                    , cc.vl_out_ded_nc        = vt_tab_reg_m200(vn_ind).vl_out_ded_nc
                    , cc.vl_cont_nc_rec       = vt_tab_reg_m200(vn_ind).vl_cont_nc_rec
                    , cc.vl_tot_cont_cum_per  = vt_tab_reg_m200(vn_ind).vl_tot_cont_cum_per
                    , cc.vl_ret_cum           = vt_tab_reg_m200(vn_ind).vl_ret_cum
                    , cc.vl_out_ded_cum       = vt_tab_reg_m200(vn_ind).vl_out_ded_cum
                    , cc.vl_cont_cum_rec      = vt_tab_reg_m200(vn_ind).vl_cont_cum_rec
                    , cc.vl_tot_cont_rec      = vt_tab_reg_m200(vn_ind).vl_tot_cont_rec
                    , cc.perconscontrpis_id   = gt_row_per_cons_contr_pis.id
                where cc.id = vt_tab_reg_m200(vn_ind).conscontrpis_id;
            exception
               when others then
                  --
                  gv_resumo_log := 'Problemas ao alterar consolidação (id='||vt_tab_reg_m200(vn_ind).conscontrpis_id||' fase = '||vn_fase||'). Erro = '||sqlerrm;
                  --
                  declare
                     vn_loggenerico_id  log_generico.id%type;
                  begin
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                      , ev_mensagem        => gv_mensagem_log
                                                      , ev_resumo          => gv_resumo_log
                                                      , en_tipo_log        => erro_de_sistema
                                                      , en_referencia_id   => gn_referencia_id
                                                      , ev_obj_referencia  => gv_obj_referencia );
                  exception
                     when others then
                        null;
                  end;
                  --
                  raise_application_error (-20101, gv_resumo_log);
                  --
            end;
         when others then
            --
            gv_resumo_log := 'Problemas ao incluir consolidação (id='||vt_tab_reg_m200(vn_ind).conscontrpis_id||' fase = '||vn_fase||'). Erro = '||sqlerrm;
            --
            declare
               vn_loggenerico_id  log_generico.id%type;
            begin
               pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                , ev_mensagem        => gv_mensagem_log
                                                , ev_resumo          => gv_resumo_log
                                                , en_tipo_log        => erro_de_sistema
                                                , en_referencia_id   => gn_referencia_id
                                                , ev_obj_referencia  => gv_obj_referencia );
            exception
               when others then
                  null;
            end;
            --
            raise_application_error (-20101, gv_resumo_log);
            --
      end;
      --
      vn_fase := 6;
      --
      if vn_ind = vt_tab_reg_m200.last then
         exit;
      else
         vn_ind := vt_tab_reg_m200.next(vn_ind);
      end if;
      --
   end loop; -- final do processo de consolidação
   --
   vn_fase := 7;
   --| Gravar os dados de detalhe de consolidação do pis - det_cons_contr_pis
   vn_ind := nvl(vt_bi_tab_reg_m210.first,0);
   --
   vn_fase := 8;
   --
   loop
      --
      vn_fase := 9;
      --
      if nvl(vn_ind,0) = 0 then -- índice = conscontrpis_id
         exit;
      end if;
      --
      vn_ind_bi := nvl(vt_bi_tab_reg_m210(vn_ind).first,0); -- índice = detconscontrpis_id
      --
      vn_fase := 10;
      --
      loop
         --
         vn_fase := 11;
         --
         if nvl(vn_ind_bi,0) = 0 then
            exit;
         end if;
         --
         vn_fase := 12;
         --
         begin
            insert into det_cons_contr_pis( id
                                          , conscontrpis_id
                                          , contrsocapurpc_id
                                          , vl_rec_brt
                                          , vl_bc_cont
                                          , aliq_pis
                                          , quant_bc_pis
                                          , vl_aliq_pis_quant
                                          , vl_cont_apur
                                          , vl_ajust_acrec
                                          , vl_ajust_reduc
                                          , vl_cont_difer
                                          , vl_cont_difer_ant
                                          , vl_cont_per 
                                          , vl_ajus_acres_bc_pis
                                          , vl_ajus_reduc_bc_pis
                                          , vl_bc_cont_ajus)
                                    values( vt_bi_tab_reg_m210(vn_ind)(vn_ind_bi).detconscontrpis_id
                                          , vt_bi_tab_reg_m210(vn_ind)(vn_ind_bi).conscontrpis_id
                                          , vt_bi_tab_reg_m210(vn_ind)(vn_ind_bi).contrsocapurpc_id
                                          , vt_bi_tab_reg_m210(vn_ind)(vn_ind_bi).vl_rec_brt
                                          , vt_bi_tab_reg_m210(vn_ind)(vn_ind_bi).vl_bc_cont
                                          , vt_bi_tab_reg_m210(vn_ind)(vn_ind_bi).aliq_pis
                                          , vt_bi_tab_reg_m210(vn_ind)(vn_ind_bi).quant_bc_pis
                                          , vt_bi_tab_reg_m210(vn_ind)(vn_ind_bi).aliq_pis_quant
                                          , (((nvl(vt_bi_tab_reg_m210(vn_ind)(vn_ind_bi).vl_bc_cont_ajus ,0) * nvl(vt_bi_tab_reg_m210(vn_ind)(vn_ind_bi).aliq_pis,0)) / 100) +
                                             (nvl(vt_bi_tab_reg_m210(vn_ind)(vn_ind_bi).quant_bc_pis,0) * nvl(vt_bi_tab_reg_m210(vn_ind)(vn_ind_bi).aliq_pis_quant,0)))
                                          , vt_bi_tab_reg_m210(vn_ind)(vn_ind_bi).vl_ajust_acrec
                                          , vt_bi_tab_reg_m210(vn_ind)(vn_ind_bi).vl_ajust_reduc
                                          , vt_bi_tab_reg_m210(vn_ind)(vn_ind_bi).vl_cont_difer
                                          , vt_bi_tab_reg_m210(vn_ind)(vn_ind_bi).vl_cont_difer_ant
                                          , vt_bi_tab_reg_m210(vn_ind)(vn_ind_bi).vl_cont_per 
                                          , vt_bi_tab_reg_m210(vn_ind)(vn_ind_bi).vl_ajus_acres_bc_pis
                                          , vt_bi_tab_reg_m210(vn_ind)(vn_ind_bi).vl_ajus_reduc_bc_pis
                                          , vt_bi_tab_reg_m210(vn_ind)(vn_ind_bi).vl_bc_cont_ajus);
         exception
            when others then
               --
               gv_resumo_log := 'Problemas ao incluir detalhes de consolidação (id='||vt_bi_tab_reg_m210(vn_ind)(vn_ind_bi).detconscontrpis_id||
                                ' fase = '||vn_fase||'). Erro = '||sqlerrm;
               --
               declare
                  vn_loggenerico_id  log_generico.id%type;
               begin
                  pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                   , ev_mensagem        => gv_mensagem_log
                                                   , ev_resumo          => gv_resumo_log
                                                   , en_tipo_log        => erro_de_sistema
                                                   , en_referencia_id   => gn_referencia_id
                                                   , ev_obj_referencia  => gv_obj_referencia );
               exception
                  when others then
                     null;
               end;
               --
               raise_application_error (-20101, gv_resumo_log);
               --
         end;
         --
         vn_fase := 13;
         --
         if vn_ind_bi = vt_bi_tab_reg_m210(vn_ind).last then
            exit;
         else
            vn_ind_bi := vt_bi_tab_reg_m210(vn_ind).next(vn_ind_bi);
         end if;
         --
      end loop; -- final do processo dos detalhes da consolidação
      --
      vn_fase := 14;
      --
      if vn_ind = vt_bi_tab_reg_m210.last then
         exit;
      else
         vn_ind := vt_bi_tab_reg_m210.next(vn_ind);
      end if;
      --
   end loop; -- final do processo de consolidação
   --
   vn_fase := 15;
   --| Gravar os dados de ajuste dos detalhes da consolidação do pis - ajust_contr_pis_apur
   vn_ind := nvl(vt_tri_tab_reg_m220.first,0);
   --
   vn_fase := 16;
   --
   loop
      --
      vn_fase := 17;
      --
      if nvl(vn_ind,0) = 0 then -- índice = conscontrpis_id
         exit;
      end if;
      --
      vn_ind_bi := nvl(vt_tri_tab_reg_m220(vn_ind).first,0); -- índice = detconscontrpis_id
      --
      vn_fase := 18;
      --
      loop
         --
         vn_fase := 19;
         --
         if nvl(vn_ind_bi,0) = 0 then
            exit;
         end if;
         --
         vn_fase := 20;
         --
         vn_ind_tri := nvl(vt_tri_tab_reg_m220(vn_ind)(vn_ind_bi).first,0);
         --
         vn_fase := 21;
         --
         loop
            --
            vn_fase := 22;
            --
            if nvl(vn_ind_tri,0) = 0 then -- índice = dm_ind_aj
               exit;
            end if;
            --
            vn_fase := 23;
            --
            begin
               insert into ajust_contr_pis_apur( id
                                               , detconscontrpis_id
                                               , dm_ind_aj
                                               , vl_aj
                                               , ajustcontrpc_id
                                               , num_doc
                                               , descr_aj
                                               , dt_ref )
                                         values( vt_tri_tab_reg_m220(vn_ind)(vn_ind_bi)(vn_ind_tri).ajustcontrpisapur_id
                                               , vn_ind_bi -- detconscontrpis_id
                                               , (vn_ind_tri - 1) -- ajuste de redução ou acréscimo - para compor como índice não podemos utilizar como zero
                                               , vt_tri_tab_reg_m220(vn_ind)(vn_ind_bi)(vn_ind_tri).vl_ajuste
                                               , pk_csf_efd_pc.fkg_id_cd_ajustcontrpc('06') -- Estorno
                                               , null
                                               , 'Referente ao estorno do(s) CFOP(s) '||vt_tri_tab_reg_m220(vn_ind)(vn_ind_bi)(vn_ind_tri).cd_cfop
                                               , gt_row_per_cons_contr_pis.dt_fin );
            exception
               when others then
                  --
                  gv_resumo_log := 'Problemas ao incluir ajuste/detalhes de consolidação (id='||vt_bi_tab_reg_m210(vn_ind)(vn_ind_bi).detconscontrpis_id||
                                   ' fase = '||vn_fase||'). Erro = '||sqlerrm;
                  --
                  declare
                     vn_loggenerico_id  log_generico.id%type;
                  begin
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                      , ev_mensagem        => gv_mensagem_log
                                                      , ev_resumo          => gv_resumo_log
                                                      , en_tipo_log        => erro_de_sistema
                                                      , en_referencia_id   => gn_referencia_id
                                                      , ev_obj_referencia  => gv_obj_referencia );
                  exception
                     when others then
                        null;
                  end;
                  --
                  raise_application_error (-20101, gv_resumo_log);
                  --
            end;
            --
            vn_fase := 24;
            --
            begin
               insert into det_ajust_contr_pis_apur( id
                                                   , ajustcontrpisapur_id
                                                   , det_valor_aj
                                                   , codst_id
                                                   , det_bc_cred
                                                   , det_aliq
                                                   , dt_oper_aj
                                                   , descr_aj
                                                   , planoconta_id
                                                   , info_compl )
                                             values( detajustcontrpisapur_seq.nextval
                                                   , vt_tri_tab_reg_m220(vn_ind)(vn_ind_bi)(vn_ind_tri).ajustcontrpisapur_id
                                                   , vt_tri_tab_reg_m220(vn_ind)(vn_ind_bi)(vn_ind_tri).vl_ajuste -- det_valor_aj
                                                   , null -- codst_id
                                                   , null -- det_bc_cred
                                                   , null -- det_aliq
                                                   , gt_row_per_cons_contr_pis.dt_fin -- dt_oper_aj
                                                   , 'Referente ao estorno do(s) CFOP(s) '||vt_tri_tab_reg_m220(vn_ind)(vn_ind_bi)(vn_ind_tri).cd_cfop -- descr_aj
                                                   , null -- planoconta_id
                                                   , null ); -- info_compl
            exception
               when others then
                  --
                  gv_resumo_log := 'Problemas ao incluir detalhe do ajuste/detalhes de consolidação (detconscontrpis_id = '||
                                   vt_bi_tab_reg_m210(vn_ind)(vn_ind_bi).detconscontrpis_id||', ajustcontrpisapur_id = '||
                                   vt_tri_tab_reg_m220(vn_ind)(vn_ind_bi)(vn_ind_tri).ajustcontrpisapur_id||' fase = '||vn_fase||'). Erro = '||sqlerrm;
                  --
                  declare
                     vn_loggenerico_id  log_generico.id%type;
                  begin
                     pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                                      , ev_mensagem        => gv_mensagem_log
                                                      , ev_resumo          => gv_resumo_log
                                                      , en_tipo_log        => erro_de_sistema
                                                      , en_referencia_id   => gn_referencia_id
                                                      , ev_obj_referencia  => gv_obj_referencia );
                  exception
                     when others then
                        null;
                  end;
                  --
                  raise_application_error (-20101, gv_resumo_log);
                  --
            end;
            --
            vn_fase := 25;
            --
            if vn_ind_tri = vt_tri_tab_reg_m220(vn_ind)(vn_ind_bi).last then
               exit;
            else
               vn_ind_tri := vt_tri_tab_reg_m220(vn_ind)(vn_ind_bi).next(vn_ind_tri);
            end if;
            --
         end loop; -- final do processo dos ajustes dos detalhes da consolidação
         --
         vn_fase := 26;
         --
         if vn_ind_bi = vt_tri_tab_reg_m220(vn_ind).last then
            exit;
         else
            vn_ind_bi := vt_tri_tab_reg_m220(vn_ind).next(vn_ind_bi);
         end if;
         --
      end loop; -- final do processo dos detalhes da consolidação
      --
      vn_fase := 27;
      --
      if vn_ind = vt_tri_tab_reg_m220.last then
         exit;
      else
         vn_ind := vt_tri_tab_reg_m220.next(vn_ind);
      end if;
      --
   end loop; -- final do processo de consolidação
   --
   vn_fase := 28;
   --
   commit;
   --
EXCEPTION
   when others then
      --
      gv_resumo_log := 'Problemas em pk_apur_pis.pkb_grava_dados_m200 (fase = '||vn_fase||'). Erro = '||sqlerrm;
      --
      declare
         vn_loggenerico_id  log_generico.id%type;
      begin
         pk_log_generico.pkb_log_generico ( sn_loggenerico_id  => vn_loggenerico_id
                                          , ev_mensagem        => gv_mensagem_log
                                          , ev_resumo          => gv_resumo_log
                                          , en_tipo_log        => erro_de_sistema
                                          , en_referencia_id   => gn_referencia_id
                                          , ev_obj_referencia  => gv_obj_referencia );
      exception
         when others then
            null;
      end;
      --
      raise_application_error (-20101, gv_resumo_log);
      --
END PKB_GRAVA_DADOS_M200;
