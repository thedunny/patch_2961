                       pkb_monta_vetor_m100( en_ch_cc_tpcr_or_tp_aliq => vn_tipocredpc_id||r_reg.dm_ind_cred_ori||r_reg.tipo||
                                                                         replace(replace(nvl(r_reg.aliq_pis,0),',',''),'.','')||
