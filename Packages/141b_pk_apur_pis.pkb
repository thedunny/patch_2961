   else
      --
      vn_fase := 5;
      -- Recupera os dados da folha de salários utilizado como referência
      pkb_dados_fol_pis_m350( en_infpisfolha_id => en_infpisfolha_id );
      gv_mensagem_log := 'Desprocessar a '||gv_mensagem_log;
